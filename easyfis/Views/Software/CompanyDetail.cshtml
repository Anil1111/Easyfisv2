@{
    var CompanyId = Request.QueryString["CompanyId"];
    var branchId = Request.QueryString["BranchId"];
}
<!DOCTYPE html>
<html>
<head>
    <!-- Meta -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <meta name="description" content="Some slide and push menu demos using CSS3 transitions.">

    <!-- Title of the Page -->
    <title>Company Detail</title>

    <!-- CSS StyleSheets-->
    @Styles.Render("~/Content/Software-css")
</head>
<body>
    <div id="o-wrapper" class="o-wrapper">
        <!-- Header -->
        @Html.Partial("_SoftwareHeader")

        <!-- Section -->
        <section>
            <div class="sub-nav">
                <div class="container">
                    <h4><i class="fa fa-building"></i> Company Detail</h4>
                </div>
                <div class="div_Current_Page">
                    <div class="container">
                        <span id="currentPage"></span>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-md-12" align="right">
                        <button class="btn btn-primary btn-sm" id="cmdSaveCompany" onclick="cmdCompanySaveButton_OnClick()"><i class="fa fa-save"></i> Save</button>
                        <button class="btn btn-primary btn-sm" id="cmdSaveUpdateCompany" onclick="cmdCompanySaveUpdateButton_OnClick()"><i class="fa fa-save"></i> Save (Lock)</button>
                        <button class="btn btn-primary btn-sm" id="cmdEditCompany" onclick="cmdCompanyEditButton_OnClick()"><i class="fa fa-edit"></i> Edit (Unlock)</button>
                        <button onclick="window.location.href ='@Url.Action("CompanyPDF", "Software", new { CompanyId = CompanyId, BranchId = branchId })'" cla class="btn btn-primary btn-sm" id="cmdPrintCompany"><i class="fa fa-print"></i> Print</button>
                        <button onclick="window.location.href = '@Url.Action("Company", "Software", new { BranchId = branchId })'" class="btn btn-danger btn-sm" id="cmdCloseButtonCompany"><i class="fa fa-times"></i> Close</button>
                    </div>
                </div>
                @*<hr />*@
                <div class="well">
                    <form class="form-horizontal" role="form">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label col-sm-4">Company</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control input-sm requiredCompany" id="company_Company" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-sm-4">Address</label>
                                    <div class="col-sm-8">
                                        <textarea class="form-control textarea-custom requiredCompany" id="company_Address"></textarea>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label col-sm-4">Contact No</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control input-sm requiredCompany" id="company_ContactNumber" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-sm-4">TAX No</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control input-sm requiredCompany" id="company_TaxNumber" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-sm-4">Lock:</label>
                                    <div class="col-sm-8">
                                        <input type="checkbox" class="checkbox_custom" id="isLocked_Status" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-4"></div>
                                    <div class="col-sm-8">
                                        <input type="hidden" id="Company_IsLocked">
                                        <input type="hidden" id="Company_IsLocked_defaultTrue">
                                        <input type="hidden" id="Company_IsLocked_defaultFalse">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6"></div>
                        </div>
                    </form>
                </div>

                <br />
                <!-- Branch List -->
                <div id="branchList">
                    <div class="row">
                        <div class="col-lg-12">
                            <h4>Branch List</h4>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="input-group input-group-sm">
                                <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                <input type="text" class="form-control input-sm" id="InputFilter" placeholder="Search Branch">
                            </div>
                        </div>
                        <div class="col-lg-8">
                            <button type="submit" class="btn btn-primary pull-right btn-sm" id="cmdBranchAddBranchButton" onclick="cmdbranchAddBranchButton_OnClick()"><i class="fa fa-plus"></i> Add</button>
                        </div>
                    </div>
                    <br />
                    <div class="row" align="left">
                        <div class="col-lg-12">
                            <div id="BranchGrid" class="grid" align="left"></div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="btn-group col-md-7" id="naviagtionPageGrid">
                            <button type="button" class="btn btn-default border-custom" id="btnMoveToFirstPageGrid">
                                <span class="glyphicon glyphicon-fast-backward"></span>
                            </button>
                            <button type="button" class="btn btn-default border-custom" id="btnMoveToPreviousPageGrid">
                                <span class="glyphicon glyphicon-step-backward"></span>
                            </button>
                            <button type="button" class="btn btn-default border-custom" disabled style="width: 100px" id="btnCurrentPageGrid"></button>
                            <button type="button" class="btn btn-default border-custom" id="btnMoveToNextPageGrid">
                                <span class="glyphicon glyphicon-step-forward"></span>
                            </button>
                            <button type="button" class="btn btn-default border-custom" id="btnMoveToLastPageGrid">
                                <span class="glyphicon glyphicon-fast-forward"></span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <!-- Branch Edit Detail -->
        <div class="modal fade" id="branchEdit" role="dialog">
            <div class="modal-dialog">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="companyNameModalLabel">Branch - Detail</h4>
                    </div>
                    <div class="modal-body">
                        <form id="branchDetailForm">
                            <input type="hidden" class="form-control" id="branch_Id" />
                            <input type="hidden" class="form-control" id="branch_companyId" />
                            <dl class="dl-horizontal">
                                <dt>Company</dt>
                                <dd><input type="text" class="form-control input-sm" id="branch_company" readonly /></dd>
                                <dt>Branch</dt>
                                <dd><input type="text" class="form-control input-sm requiredBranch" id="branch_branch" /></dd>
                                <dt>Branch code</dt>
                                <dd>
                                    <input type="text" class="form-control input-sm" id="branch_Code_Display" />
                                    <input type="hidden" id="branch_Code" />
                                </dd>
                                <dt>Address</dt>
                                <dd><textarea class="form-control textarea-custom requiredBranch" id="branch_Address"></textarea></dd>
                                <dt>Contact Number:</dt>
                                <dd><input type="text" class="form-control input-sm requiredBranch" id="branch_ContactNumber" /></dd>
                                <dt>Tax Number</dt>
                                <dd><input type="text" class="form-control input-sm" id="branch_TaxNumber" /></dd>
                            </dl>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary btn-sm" id="cmdSaveBranchButton" onclick="cmdBranchSaveButton_OnClick()"><i class="fa fa-save"></i> Save</button>
                        <button id="cmdBranchDetailCloseButton" class="btn btn-danger btn-sm" data-dismiss="modal"><i class="fa fa-close"></i> Close</button>
                    </div>
                </div>
            </div>
        </div>

        <!-- Branch Delete Modal Confirmation -->
        <div class="modal fade" id="deleteBranchConfirm" role="dialog">
            <div class="modal-dialog modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Delete Branch <i class="fa fa-trash"></i></h4>
                    </div>
                    <div class="modal-body">
                        Are you sure you want to delete this?
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-danger btn-sm" id="cmdConfirmDeleteBranch" onclick="cmdBranchConfirmDeleteButton_OnClick()"><i class="fa fa-trash"></i> Delete</button>
                        <button id="cmdConfirmDeleteCloseButton" class="btn btn-primary btn-sm" data-dismiss="modal"><i class="fa fa-close"></i> Cancel</button>
                    </div>
                </div>
            </div>
        </div>

        <!-- Loading Modal -->
        <div class="modal fade" id="loadingModal" tabindex="-1" role="dialog" aria-labelledby="Loading..." aria-hidden="true" data-backdrop="static" data-keyboard="false">
            <div class=" modal-dialog modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-body">
                        <h4>Loading ...</h4>
                    </div>
                    <div class="modal-footer">
                        <img class="img-thumbnail img-responsive center-block img-custom" src='~/images/loading/progress_bar.gif'>
                    </div>
                </div>
            </div>
        </div>

        <!-- Footer -->
        @Html.Partial("_SoftwareFooter")
    </div>

    <!-- SideBar Menu -->
    @Html.Partial("_SoftwareSideBarMenu")

    <div id="c-mask" class="c-mask"></div><!-- /c-mask -->
    <!-- Scripts JavaScripts -->
    @Scripts.Render("~/Scripts/Software-js")
    <script type="text/javascript">
        // ================
        // Global Variables
        // ================
        var branches;
        var branchGrid;

        var companies;

        var btnFirstPageGrid;
        var btnPreviousPageGrid;
        var btnNextPageGrid;
        var btnLastPageGrid;
        var btnCurrentPageGrid;

        // ================
        // GET Company List
        // ================
        function getCompanyLastInsertedId() {
            var companies = new wijmo.collections.ObservableArray;
            $.ajax({
                url: '/api/companyLastId',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (Results) {
                    if (Results.length > 0) {
                        companies.push({
                            Id: Results.Id,
                            Company: Results.Company,
                            Address: Results.Address,
                            ContactNumber: Results.ContactNumber,
                            TaxNumber: Results.TaxNumber,
                            IsLocked: Results.IsLocked,
                            CreatedById: Results.CreatedById,
                            CreatedBy: Results.CreatedBy,
                            CreatedDateTime: Results.CreatedDateTime,
                            UpdatedById: Results.UpdatedById,
                            UpdatedBy: Results.UpdatedBy,
                            UpdatedDateTime: Results.UpdatedDateTime
                        });
                    }
                    window.location = '/Software/CompanyDetail?CompanyId=' + Results.Id + '&BranchId=' + @branchId;
                    window.setTimeout(function () { location.reload() }, 1000);
                }
            }).fail(function (xhr, textStatus, err) {
                alert(err);
            });
            return companies;
        }

        // =================
        // GET Company by Id
        // =================
        function getCompanyById() {
            $('#loadingModal').modal('show');
            var companies = new wijmo.collections.ObservableArray;
            var companyId = @CompanyId;
            $.ajax({
                url: '/api/company/' + companyId,
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (Results) {
                    $('#loadingModal').modal('hide');
                    if (Results.length > 0) {
                        companies.push({
                            Id: Results.Id,
                            Company: Results.Company,
                            Address: Results.Address,
                            ContactNumber: Results.ContactNumber,
                            TaxNumber: Results.TaxNumber,
                            IsLocked: Results.IsLocked,
                            CreatedById: Results.CreatedById,
                            CreatedBy: Results.CreatedBy,
                            CreatedDateTime: Results.CreatedDateTime,
                            UpdatedById: Results.UpdatedById,
                            UpdatedBy: Results.UpdatedBy,
                            UpdatedDateTime: Results.UpdatedDateTime
                        });
                    }
                    //document.getElementById('companyDetailLabel').innerHTML = Results.Company;
                    //document.getElementById('companyNameLabel').innerHTML = Results.Company + " Branch List";
                    document.getElementById('companyNameModalLabel').innerHTML = Results.Company + " Branch Detail";
                    document.getElementById('branch_company').value = Results.Company;
                    document.getElementById('company_Company').value = Results.Company;
                    document.getElementById('company_Address').value = Results.Address;
                    document.getElementById('company_ContactNumber').value = Results.ContactNumber;
                    document.getElementById('company_TaxNumber').value = Results.TaxNumber;

                    document.getElementById('Company_IsLocked').value = Results.IsLocked;
                    document.getElementById('createdBy').innerHTML = Results.CreatedBy;
                    document.getElementById('createdDate').innerHTML = Results.CreatedDateTime;
                    document.getElementById('updatedBy').innerHTML = Results.UpdatedBy;
                    document.getElementById('updatedDate').innerHTML = Results.UpdatedDateTime;

                    if(document.getElementById('Company_IsLocked').value == 'true') {
                        //$("#cmdSaveCompany").prop("disabled", false);
                        $("#cmdEditCompany").prop("disabled", false);
                        $("#cmdPrintCompany").prop("disabled", false);
                        $("#cmdSaveUpdateCompany").prop("disabled", true);
                        $("#cmdBranchAddBranchButton").prop("disabled", true);
                        document.getElementById('company_Company').disabled = true;
                        document.getElementById('company_Address').disabled = true;
                        document.getElementById('company_ContactNumber').disabled = true;
                        document.getElementById('company_TaxNumber').disabled = true;

                        document.getElementById('isLocked_Status').checked = true;

                    } else if(document.getElementById('Company_IsLocked').value == 'false')  {
                        //$("#cmdSaveCompany").prop("disabled", true);
                        $("#cmdEditCompany").prop("disabled", true);
                        $("#cmdPrintCompany").prop("disabled", true);
                        $("#cmdSaveUpdateCompany").prop("disabled", false);

                        document.getElementById('isLocked_Status').checked = false;

                        document.getElementById('company_Company').disabled = false;
                        document.getElementById('company_Address').disabled = false;
                        document.getElementById('company_ContactNumber').disabled = false;
                        document.getElementById('company_TaxNumber').disabled = false;
                    }
                }
            }).fail(function (xhr, textStatus, err) {
                alert(err);
            });
            return companies;
        }

        // ===================
        // Company Save Button
        // ===================
        function cmdCompanySaveButton_OnClick() {
            var companyObject = new Object();
            var companyId = @CompanyId;
            companyObject.Company = document.getElementById('company_Company').value;
            companyObject.Address = document.getElementById('company_Address').value;
            companyObject.ContactNumber = document.getElementById('company_ContactNumber').value;
            companyObject.TaxNumber = document.getElementById('company_TaxNumber').value;
            var data = JSON.stringify(companyObject);

            var isValid = true;
            $('.requiredCompany').each(function () {
                if ($.trim($(this).val()) == '') {
                    isValid = false;
                    $(this).css({
                        "border": "1px solid red"
                    });
                }
                else {
                    $(this).css({
                        "border": "",
                    });
                }
            });
            if (isValid == false) {
                toastr.error("Invalid Entry");
                e.preventDefault();
            }
            else {
                $("#cmdSaveCompany").prop("disabled", true);
                $("#cmdSaveUpdateCompany").prop("disabled", true);
                $("#cmdEditCompany").prop("disabled", true);
                $("#cmdPrintCompany").prop("disabled", true);
                $("#cmdCloseButtonCompany").prop("disabled", true);
                $("#cmdSaveBranchButton").prop("disabled", true);
                $("#cmdBranchDetailCloseButton").prop("disabled", true);
                if (companyId == 0) {
                    $.ajax({
                        type: "POST",
                        url: '/api/addCompany',
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        data: data,
                        statusCode: {
                            200: function () {
                                $("#cmdSaveCompany").prop("disabled", true);
                                $("#cmdSaveUpdateCompany").prop("disabled", true);
                                $("#cmdEditCompany").prop("disabled", true);
                                $("#cmdPrintCompany").prop("disabled", true);
                                $("#cmdCloseButtonCompany").prop("disabled", true);
                                $("#cmdSaveBranchButton").prop("disabled", true);
                                $("#cmdBranchDetailCloseButton").prop("disabled", true);
                                toastr.success("Successfully Saved");
                                setTimeout(getCompanyLastInsertedId, 1000)
                            },
                            404: function () {
                                toastr.error("Can't Save");
                                $("#cmdSaveCompany").prop("disabled", false);
                                $("#cmdSaveUpdateCompany").prop("disabled", false);
                                $("#cmdEditCompany").prop("disabled", false);
                                $("#cmdPrintCompany").prop("disabled", false);
                                $("#cmdCloseButtonCompany").prop("disabled", false);
                                $("#cmdSaveBranchButton").prop("disabled", false);
                                $("#cmdBranchDetailCloseButton").prop("disabled", false);
                            },
                            400: function () {
                                toastr.error("Bad Request");
                                $("#cmdSaveCompany").prop("disabled", false);
                                $("#cmdSaveUpdateCompany").prop("disabled", false);
                                $("#cmdEditCompany").prop("disabled", false);
                                $("#cmdPrintCompany").prop("disabled", false);
                                $("#cmdCloseButtonCompany").prop("disabled", false);
                                $("#cmdSaveBranchButton").prop("disabled", false);
                                $("#cmdBranchDetailCloseButton").prop("disabled", false);
                            }
                        }
                    });
                } else {
                    var companyObject = new Object();
                    var companyId = @CompanyId
                    companyObject.Company = document.getElementById('company_Company').value;
                    companyObject.Address = document.getElementById('company_Address').value;
                    companyObject.ContactNumber = document.getElementById('company_ContactNumber').value;
                    companyObject.TaxNumber = document.getElementById('company_TaxNumber').value;
                    if(companyId > 0) {
                        companyObject.IsLocked = document.getElementById('Company_IsLocked_defaultFalse').value;
                    } else {
                        companyObject.IsLocked = document.getElementById('Company_IsLocked_defaultTrue').value;
                    }

                    var dataUpdate = JSON.stringify(companyObject);
                    $.ajax({
                        type: "PUT",
                        url: '/api/updateCompany/' + companyId,
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        data: dataUpdate,
                        statusCode: {
                            200: function () {
                                $("#cmdSaveCompany").prop("disabled", true);
                                $("#cmdSaveUpdateCompany").prop("disabled", true);
                                $("#cmdEditCompany").prop("disabled", true);
                                $("#cmdPrintCompany").prop("disabled", true);
                                $("#cmdCloseButtonCompany").prop("disabled", true);
                                $("#cmdSaveBranchButton").prop("disabled", true);
                                $("#cmdBranchDetailCloseButton").prop("disabled", true);
                                toastr.success("Successfully Updated");
                                window.setTimeout(function () {
                                    location.reload()
                                }, 1000);
                            },
                            404: function () {
                                toastr.error("Can't Update");
                                $("#cmdSaveCompany").prop("disabled", false);
                                $("#cmdSaveUpdateCompany").prop("disabled", false);
                                $("#cmdEditCompany").prop("disabled", false);
                                $("#cmdPrintCompany").prop("disabled", false);
                                $("#cmdCloseButtonCompany").prop("disabled", false);
                                $("#cmdSaveBranchButton").prop("disabled", false);
                                $("#cmdBranchDetailCloseButton").prop("disabled", false);
                            },
                            400: function () {
                                toastr.error("Bad Request");
                                $("#cmdSaveCompany").prop("disabled", false);
                                $("#cmdSaveUpdateCompany").prop("disabled", false);
                                $("#cmdEditCompany").prop("disabled", false);
                                $("#cmdPrintCompany").prop("disabled", false);
                                $("#cmdCloseButtonCompany").prop("disabled", false);
                                $("#cmdSaveBranchButton").prop("disabled", false);
                                $("#cmdBranchDetailCloseButton").prop("disabled", false);
                            }
                        }
                    });
                }
            }
        }

