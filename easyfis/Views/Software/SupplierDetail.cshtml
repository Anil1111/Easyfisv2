                        "align": "right"
                    },
                    {
                        "header": "Cost Value",
                        "binding": "ConsignmentCostValueDisplay",
                        "width": "2*",
                        "align": "right"
                    },
                ],
                autoGenerateColumns: false,
                itemsSource: consignmentItemCollectionView,
                isReadOnly: true,
                autoSizeMode: wijmo.grid.AutoSizeMode.Both,
                allowDragging: wijmo.grid.AllowDragging.None,
                selectionMode: wijmo.grid.SelectionMode.Row
            });

            consignmentItemFlexGrid.trackChanges = true;

            updateNavigateButtonsItemsConsignment();

            btnMoveToFirstPageGridConsignment.addEventListener('click', function () {
                consignmentItemCollectionView.moveToFirstPage();
                updateNavigateButtonsItemsConsignment();
            });

            btnMoveToPreviousPageGridConsignment.addEventListener('click', function () {
                consignmentItemCollectionView.moveToPreviousPage();
                updateNavigateButtonsItemsConsignment();
            });

            btnMoveToNextPageGridConsignment.addEventListener('click', function () {
                consignmentItemCollectionView.moveToNextPage();
                updateNavigateButtonsItemsConsignment();
            });

            btnMoveToLastPageGridConsignment.addEventListener('click', function () {
                consignmentItemCollectionView.moveToLastPage();
                updateNavigateButtonsItemsConsignment();
            });
        }

        // =========================
        // Update Navigation Buttons
        // =========================
        function updateNavigateButtonsItemsConsignment() {
            if (consignmentItemCollectionView.pageSize <= 0) {
                document.getElementById('naviagtionPageGridConsignment').style.display = 'none';
                return;
            }
            document.getElementById('naviagtionPageGridConsignment').style.display = 'block';
            if (consignmentItemCollectionView.pageIndex === 0) {
                btnMoveToFirstPageGridConsignment.setAttribute('disabled', 'disabled');
                btnMoveToPreviousPageGridConsignment.setAttribute('disabled', 'disabled');
                btnMoveToNextPageGridConsignment.removeAttribute('disabled');
                btnMoveToLastPageGridConsignment.removeAttribute('disabled');
            } else if (consignmentItemCollectionView.pageIndex === (consignmentItemCollectionView.pageCount - 1)) {
                btnMoveToFirstPageGridConsignment.removeAttribute('disabled');
                btnMoveToPreviousPageGridConsignment.removeAttribute('disabled');
                btnMoveToLastPageGridConsignment.setAttribute('disabled', 'disabled');
                btnMoveToNextPageGridConsignment.setAttribute('disabled', 'disabled');
            } else {
                btnMoveToFirstPageGridConsignment.removeAttribute('disabled');
                btnMoveToPreviousPageGridConsignment.removeAttribute('disabled');
                btnMoveToNextPageGridConsignment.removeAttribute('disabled');
                btnMoveToLastPageGridConsignment.removeAttribute('disabled');
            }
            btnCurrentPageGridConsignment.innerHTML = (consignmentItemCollectionView.pageIndex + 1) + ' / ' + consignmentItemCollectionView.pageCount;
        }

        // ==================================
        // Refresh Consignment Item Flex Grid
        // ==================================
        function refreshConsignmentItemFlexGrid() {
            consignmentItemCollectionView = new wijmo.collections.CollectionView(getConsignmentItemData());
            consignmentItemCollectionView.canFilter = true;
            consignmentItemCollectionView.pageSize = cboConsignmentShowNumberOfRows.selectedValue;

            var filterText = '';
            $('#ConsignmentInputFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                consignmentItemCollectionView.refresh();
            });

            consignmentItemCollectionView.filter = function (item) {
                return !filterText || (item.ManualArticleCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Article.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ManualArticleOldCode.toLowerCase().indexOf(filterText) > -1);
            }

            consignmentItemCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateNavigateButtonsItemsConsignment();
            });

            consignmentItemFlexGrid.itemsSource = consignmentItemCollectionView;
            consignmentItemFlexGrid.trackChanges = true;
        }

        // ================================================
        // Create Consignment Combo Display Number of Rows
        // ===============================================
        function createConsignmentCboDisplayNumberOfRows() {
            var rowsObservableArray = new wijmo.collections.ObservableArray();

            for (var i = 0; i <= 4; i++) {
                var rows = 0;
                var rowsString = "";

                if (i == 0) {
                    rows = 15;
                    rowsString = "Show 15 Rows";
                } else if (i == 1) {
                    rows = 50;
                    rowsString = "Show 50 Rows";
                } else if (i == 2) {
                    rows = 100;
                    rowsString = "Show 100 Rows";
                } else if (i == 3) {
                    rows = 150;
                    rowsString = "Show 150 Rows";
                } else {
                    rows = 200;
                    rowsString = "Show 200 Rows";
                }

                rowsObservableArray.push({
                    rowNumber: rows,
                    rowString: rowsString
                });
            }
            cboConsignmentShowNumberOfRows.dispose();
            cboConsignmentShowNumberOfRows = new wijmo.input.ComboBox('#cboConsignmentShowNumberOfRows', {
                isEditable: false,
                itemsSource: rowsObservableArray,
                displayMemberPath: "rowString",
                selectedValuePath: "rowNumber",
                selectedValue: 50,
                onSelectedIndexChanged: function () {
                    NProgress.start();

                    setTimeout(function () {
                        var numberOfRows = cboConsignmentShowNumberOfRows.selectedValue;
                        consignmentItemCollectionView.pageSize = parseInt(numberOfRows);
                        consignmentItemCollectionView.refresh();
                        consignmentItemFlexGrid.refresh();

                        NProgress.done();
                    }, 300);
                }
            });
        }

        // ==================================
        // Create Consignment Item Combo Box
        // =================================
        function createCboItem(ItemIdSelectedValue) {
            var consignmentItemObservableArray = new wijmo.collections.ObservableArray();

            $.ajax({
                url: '/api/consignment/item/list/item',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            consignmentItemObservableArray.push({
                                Id: results[i]["Id"],
                                ManualArticleCode: results[i]["ManualArticleCode"],
                                Article: results[i]["Article"]
                            });
                        }
                    }

                    var disabled = true;
                    $("#btnConsignmentItemSave").prop("disabled", true);

                    if (results.length > 0) {
                        if (ItemIdSelectedValue != null) {
                            var disabled = true;
                        } else {
                            disabled = false;
                            $("#btnConsignmentItemSave").prop("disabled", false);
                        }
                    }

                    cboConsignmentItemCode.dispose();
                    cboConsignmentItemCode = new wijmo.input.ComboBox('#cboConsignmentItemCode', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: consignmentItemObservableArray,
                        displayMemberPath: "ManualArticleCode",
                        selectedValuePath: "Id",
                        selectedValue: ItemIdSelectedValue,
                        onSelectedIndexChanged: function () {
                            cboConsignmentItemDescription.selectedValue = this.selectedValue;
                        }
                    });

                    cboConsignmentItemDescription.dispose();
                    cboConsignmentItemDescription = new wijmo.input.ComboBox('#cboConsignmentItemDescription', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: consignmentItemObservableArray,
                        displayMemberPath: "Article",
                        selectedValuePath: "Id",
                        selectedValue: ItemIdSelectedValue,
                        onSelectedIndexChanged: function () {
                            cboConsignmentItemCode.selectedValue = this.selectedValue;
                        }
                    });

                    setTimeout(function () {
                        $("#consignmentLoading").hide();
                        $("#consignmentContent").show();
                        $('#btnConsignmentItemSave').prop('disabled', false);
                    }, 500);
                }
            });
        }

        // ===============
        // Add Consignment
        // ===============
        function cmdAddConsignment_OnClick() {
            $('#consignmentModal').modal({
                show: true,
                backdrop: 'static'
            });

            $('.requiredConsignmentItem').each(function () {
                $(this).css({
                    "border": ""
                });
            });

            $("#consignmentLoading").show();
            document.getElementById("consignmentLoading").innerHTML = 'Loading...';
            $("#consignmentContent").hide();

            document.getElementById("consignmentModalLabel").innerHTML = 'Add Consignment Item';

            document.getElementById('btnConsignmentItemSave').innerHTML = "<i class='fa fa-save'></i> Save";
            $('#btnConsignmentItemSave').prop('disabled', true);
            $('#btnConsignmentItemCloseModal').prop('disabled', false);

            createCboItem(null);
            document.getElementById('inpConsignmentItemCostPercentage').value = formatDecimalValues(0);
            document.getElementById('inpConsignmentItemCostValue').value = formatDecimalValues(0);
