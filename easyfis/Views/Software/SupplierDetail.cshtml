        // ==================
        // Delete Consignment
        // ==================
        function btnDeleteConsignmentItemOnclick() {
            $('#consignmentDeletModal').modal({
                show: true,
                backdrop: 'static'
            });

            document.getElementById('btnConsignmentItemConfirmDelete').innerHTML = "<i class='fa fa-trash'></i> Delete";
            $('#btnConsignmentItemConfirmDelete').prop('disabled', false);
            $('#btnConsignmentItemConfirmDeleteCloseModal').prop('disabled', false);
        }

        // ==========================
        // Consignment Confirm Delete
        // ==========================
        function btnConsignmentItemConfirmDeleteOnclick() {
            document.getElementById('btnConsignmentItemConfirmDelete').innerHTML = "<i class='fa fa-trash'></i> Deleting...";
            $('#btnConsignmentItemConfirmDelete').prop('disabled', true);
            $('#btnConsignmentItemConfirmDeleteCloseModal').prop('disabled', true);

            consignmentItemCollectionView.editItem(consignmentItemCollectionView.currentItem);
            var itemId = consignmentItemCollectionView.currentEditItem.Id;

            $.ajax({
                url: '/api/consignment/item/' + itemId,
                type: "DELETE",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                statusCode: {
                    200: function () {
                        toastr.success("Delete Successful");
                        $('#consignmentDeletModal').modal('hide');
                        refreshConsignmentItemFlexGrid();
                    },
                    404: function (message) {
                        toastr.error(message.responseText.replace(/^"(.*)"$/, '$1'), "Error 404");
                        $('#consignmentDeletModal').modal('hide');
                        document.getElementById('btnConsignmentItemConfirmDelete').innerHTML = "<i class='fa fa-trash'></i> Delete";
                        $('#btnConsignmentItemConfirmDelete').prop('disabled', false);
                        $('#btnConsignmentItemConfirmDeleteCloseModal').prop('disabled', false);
                    },
                    400: function (message) {
                        toastr.error(message.responseText.replace(/^"(.*)"$/, '$1'), "Error 400");
                        $('#consignmentDeletModal').modal('hide');
                        document.getElementById('btnConsignmentItemConfirmDelete').innerHTML = "<i class='fa fa-trash'></i> Delete";
                        $('#btnConsignmentItemConfirmDelete').prop('disabled', false);
                        $('#btnConsignmentItemConfirmDeleteCloseModal').prop('disabled', false);
                    },
                    500: function (message) {
                        toastr.error(message.responseText.replace(/^"(.*)"$/, '$1'), "Error 500");
                        $('#consignmentDeletModal').modal('hide');
                        document.getElementById('btnConsignmentItemConfirmDelete').innerHTML = "<i class='fa fa-trash'></i> Delete";
                        $('#btnConsignmentItemConfirmDelete').prop('disabled', false);
                        $('#btnConsignmentItemConfirmDeleteCloseModal').prop('disabled', false);
                    }
                }
            });
        }

        // ================
        // Save Consignment
        // ================
        function btnConsignmentItemSaveOnclick() {
            var objConsignment = new Object();
            objConsignment.Id = cboConsignmentItemDescription.selectedValue;
            objConsignment.ConsignmentCostPercentage = document.getElementById('inpConsignmentItemCostPercentage').value;
            objConsignment.ConsignmentCostValue = document.getElementById('inpConsignmentItemCostValue').value;
            var objConsignmentData = JSON.stringify(objConsignment);

            if (consignmentEntryValidation()) {
                document.getElementById('btnConsignmentItemSave').innerHTML = "<i class='fa fa-save'></i> Saving...";
                $('#btnConsignmentItemSave').prop('disabled', true);
                $('#btnConsignmentItemCloseModal').prop('disabled', true);

                $.ajax({
                    type: "PUT",
                    url: '/api/consignment/item/add/update/' + supplierId,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    data: objConsignmentData,
                    statusCode: {
                        200: function () {
                            toastr.success("Save Successful");
                            $('#consignmentModal').modal('hide');
                            refreshConsignmentItemFlexGrid();
                        },
                        404: function (message) {
                            toastr.error(message.responseText.replace(/^"(.*)"$/, '$1'), "Error 404");
                            $('#consignmentModal').modal('hide');
                            document.getElementById('btnConsignmentItemSave').innerHTML = "<i class='fa fa-save'></i> Save";
                            $('#btnConsignmentItemSave').prop('disabled', true);
                            $('#btnConsignmentItemCloseModal').prop('disabled', false);
                        },
                        400: function (message) {
                            toastr.error(message.responseText.replace(/^"(.*)"$/, '$1'), "Error 400");
                            $('#consignmentModal').modal('hide');
                            document.getElementById('btnConsignmentItemSave').innerHTML = "<i class='fa fa-save'></i> Save";
                            $('#btnConsignmentItemSave').prop('disabled', true);
                            $('#btnConsignmentItemCloseModal').prop('disabled', false);
                        },
                        500: function (message) {
                            toastr.error(message.responseText.replace(/^"(.*)"$/, '$1'), "Error 500");
                            $('#consignmentModal').modal('hide');
                            document.getElementById('btnConsignmentItemSave').innerHTML = "<i class='fa fa-save'></i> Save";
                            $('#btnConsignmentItemSave').prop('disabled', true);
                            $('#btnConsignmentItemCloseModal').prop('disabled', false);
                        }
                    }
                });
            } else {
                toastr.error("Invalid Entry");
            }
        }

        // ==========================
        // Supplier Tabs and Controls
        // ==========================
        $('#journalLedgerTab').click(function () {
            $('#JournalLedger').show();
