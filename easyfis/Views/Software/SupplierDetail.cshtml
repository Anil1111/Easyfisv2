        // ================
        // Edit Consignment
        // ================
        function btnEditConsignmentItemOnclick() {
            $('#consignmentModal').modal({
                show: true,
                backdrop: 'static'
            });

            $('.requiredConsignmentItem').each(function () {
                $(this).css({
                    "border": ""
                });
            });

            $("#consignmentLoading").show();
            document.getElementById("consignmentLoading").innerHTML = 'Loadng...';
            $("#consignmentContent").hide();

            document.getElementById("consignmentModalLabel").innerHTML = 'Edit Consignment Item';

