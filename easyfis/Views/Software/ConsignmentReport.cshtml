<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="shortcut icon" href="~/Images/icon/easyfislogomini.ico">
    <title>Consignment Report</title>

    @Styles.Render("~/Content/Software-css")
</head>
<body>
    <div id="o-wrapper" class="o-wrapper">
        <main class="o-content">
            @Html.Partial("_SoftwareHeader")

            <section class="content-header">
                <div class="container">
                    <h3>
                        Consignment Report
                    </h3>
                    <p>
                        <i class="fa fa-building-o fa-fw"></i> &nbsp; @ViewData["Company"] <br />
                        <i class="fa fa-home fa-fw"></i> &nbsp; <a href="#" onclick="getUserBranches()" style="color: white;">@ViewData["Branch"]</a>
                    </p>
                </div>
            </section>
            <section class="container">
                <div class="panel panel-default">

                    <!--
                        ===============
                        Search / Filter
                        ===============
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 49.5%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                        <input type="text" class="form-control" id="inpConsignmentReportSearchFilter" placeholder="Search...">
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><b><i class="fa fa-table"></i></b></span>
                                        <div class="comboBox-wide" id="cboConsignmentReportDisplayNumberOfRows"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24.5%" align="right">
                                    <button onclick="btnConsignmentReportDownloadCSVOnclick()" id="btnConsignmentReportDownloadCSV" class="btn btn-success"><i class="fa fa-file-excel-o" aria-hidden="true"></i> CSV</button>
                                    <button onclick="btnConsignmentReportPrintPDFOnclick()" id="btnConsignmentReportPrintPDF" class="btn btn-primary"><i class="fa fa-print"></i> Print</button>
                                    <a href="/Software/AccountsReceivable" class="btn btn-danger"><i class="fa fa-close"></i> Close</a>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        ======================================
                        Date Range, Company and Branch Filters
                        ======================================
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Start Date</b></span>
                                        <div class="comboBox-wide" id="cboConsignmentReportStartDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%" align="right">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>End Date</b></span>
                                        <div class="comboBox-wide" id="cboConsignmentReportEndDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Company</b></span>
                                        <div class="comboBox-wide" id="cboConsignmentReportCompanyFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Branch</b></span>
                                        <div class="comboBox-wide" id="cboConsignmentReportBranchFilter"></div>
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        ===============
                        Supplier Filter
                        ===============
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 49.5%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Supplier</b></span>
                                        <div class="comboBox-wide" id="cboConsignmentReportSupplierFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 50.5%"></td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        =========
                        Flex Grid
                        =========
                    -->
                    <div class="panel-body">
                        <div id="consignmentReportFlexGrid"></div>
                    </div>

                    <!--
                        ==================
                        Navigation Buttons
                        ==================
                    -->
                    <div class="panel-footer">
                        <div class="row">
                            <div class="btn-group col-md-7" id="consignmentReportNavigationPageGrid">
                                <button type="button" class="btn btn-default border-custom" id="btnConsignmentReportMoveToFirstPageGrid">
                                    <span class="glyphicon glyphicon-fast-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnConsignmentReportMoveToPreviousPageGrid">
                                    <span class="glyphicon glyphicon-step-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" disabled style="width: 100px" id="btnConsignmentReportCurrentPageGrid"></button>
                                <button type="button" class="btn btn-default border-custom" id="btnConsignmentReportMoveToNextPageGrid">
                                    <span class="glyphicon glyphicon-step-forward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnConsignmentReportMoveToLastPageGrid">
                                    <span class="glyphicon glyphicon-fast-forward"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <!--
                    ======
                    Totals
                    ======
                -->
                <div class="panel panel-default">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-6"></div>
                            <div class="col-md-6">
                                <dl class="dl-horizontal dl-custom">
                                    <dt>
                                        <b>Total Amount</b>
                                    </dt>
                                    <dd>
                                        <input type="text" class="form-control numberField" id="inpTotalAmount" placeholder="0.00" disabled />
                                    </dd>
                                </dl>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            @Html.Partial("_SoftwareFooter")
        </main>
    </div>
    <div id="c-mask" class="c-mask"></div>

    @Scripts.Render("~/Scripts/Software-js")
    <script type="text/javascript">
        // ============
        // Date Filters
        // ============
        var cboConsignmentReportStartDateFilter = new wijmo.input.InputDate('#cboConsignmentReportStartDateFilter');
        var cboConsignmentReportEndDateFilter = new wijmo.input.InputDate('#cboConsignmentReportEndDateFilter');
        var isCboConsignmentReportStartDateFilterSelected = false;
        var isCboConsignmentReporEndDateFilterSelected = false;

        // =========================
        // Company and Branch Filter
        // =========================
        var cboConsignmentReportCompanyFilter = new wijmo.input.ComboBox('#cboConsignmentReportCompanyFilter');
        var isCboConsignmentReportCompanyFilterSelected = false;
        var cboConsignmentReportBranchFilter = new wijmo.input.ComboBox('#cboConsignmentReportBranchFilter');

        // ===============
        // Supplier Filter
        // ===============
        var cboConsignmentReportSupplierFilter = new wijmo.input.ComboBox('#cboConsignmentReportSupplierFilter');

        // =============================
        // Collection View and Flex Grid
        // =============================
        var consignmentReportCollectionView;
        var consignmentReportFlexGrid;
        var cboConsignmentReportDisplayNumberOfRows = new wijmo.input.ComboBox('#cboConsignmentReportDisplayNumberOfRows');

        // ==================
        // Navigation Buttons
        // ==================
        var btnConsignmentReportMoveToFirstPageGrid = document.getElementById('btnConsignmentReportMoveToFirstPageGrid');
        var btnConsignmentReportMoveToPreviousPageGrid = document.getElementById('btnConsignmentReportMoveToPreviousPageGrid');
        var btnConsignmentReportMoveToNextPageGrid = document.getElementById('btnConsignmentReportMoveToNextPageGrid');
        var btnConsignmentReportMoveToLastPageGrid = document.getElementById('btnConsignmentReportMoveToLastPageGrid');
        var btnConsignmentReportCurrentPageGrid = document.getElementById('btnConsignmentReportCurrentPageGrid');
        var consignmentReportCurrentPageIndex = 0;

        // =======================
        // Get URL Parameter Value
        // =======================
        function getURLParameterValue(name) {
            name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");

            var regexS = "[\\?&]" + name + "=([^&#]*)";
            var regex = new RegExp(regexS);

            var results = regex.exec(window.location.href);
            if (results == null) {
                return "";
            } else {
                return results[1];
            }
        }

        // ================================
        // Create Combo Date Ranged Filters
        // ================================
        function createCboDateRangeFilters() {
            NProgress.start();

            cboConsignmentReportStartDateFilter.dispose();
            cboConsignmentReportStartDateFilter = new wijmo.input.InputDate('#cboConsignmentReportStartDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameterValue("StartDate")),
                onValueChanged: function () {
                    if (!isCboConsignmentReportStartDateFilterSelected) {
                        NProgress.start();
                        refreshConsignmentReportFlexGrid();
                    } else {
                        isCboConsignmentReportStartDateFilterSelected = false;
                    }
                }
            });

            cboConsignmentReportEndDateFilter.dispose();
            cboConsignmentReportEndDateFilter = new wijmo.input.InputDate('#cboConsignmentReportEndDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameterValue("EndDate")),
                onValueChanged: function () {
                    if (!isCboConsignmentReporEndDateFilterSelected) {
                        NProgress.start();
                        refreshConsignmentReportFlexGrid();
                    } else {
                        isCboConsignmentReporEndDateFilterSelected = false;
                    }
                }
            });

            createCboCompanyFilter();
        }

        // ===========================
        // Create Combo Company Filter
        // ===========================
        function createCboCompanyFilter() {
            var companyObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/consignmentReport/dropdown/list/company',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            companyObservableArray.push({
                                Id: results[i]["Id"],
                                Company: results[i]["Company"]
                            });
                        }
                    }

                    var disabled = true;
                    if (results.length > 0) {
                        disabled = false;
                    } else {
                        $("#btnConsignmentReportDownloadCSV").prop("disabled", true);
                        $("#btnConsignmentReportPrintPDF").prop("disabled", true);
                    }

                    cboConsignmentReportCompanyFilter.dispose();
                    cboConsignmentReportCompanyFilter = new wijmo.input.ComboBox('#cboConsignmentReportCompanyFilter', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: companyObservableArray,
                        displayMemberPath: "Company",
                        selectedValuePath: "Id",
                        selectedValue: getURLParameterValue("CompanyId"),
                        onSelectedIndexChanged: function () {
                            NProgress.start();
                            $("#btnConsignmentReportDownloadCSV").prop("disabled", false);
                            $("#btnConsignmentReportPrintPDF").prop("disabled", false);
                            isCboConsignmentReportCompanyFilterSelected = true;
                            createCboBranchFilter();
                        }
                    });

                    createCboBranchFilter();
                }
            });
        }

        // ==========================
        // Create Combo Branch Filter
        // ==========================
        function createCboBranchFilter() {
            var branchObservableArray = new wijmo.collections.ObservableArray();
            if (cboConsignmentReportCompanyFilter.selectedValue != null) {
                $.ajax({
                    url: '/api/consignmentReport/dropdown/list/branch/' + cboConsignmentReportCompanyFilter.selectedValue,
                    cache: false,
                    type: 'GET',
                    contentType: 'application/json; charset=utf-8',
                    success: function (results) {
                        if (results.length > 0) {
                            for (i = 0; i < results.length; i++) {
                                branchObservableArray.push({
                                    Id: results[i]["Id"],
                                    Branch: results[i]["Branch"]
                                });
                            }
                        }

                        var disabled = true;
                        if (results.length > 0) {
                            disabled = false;
                        } else {
                            $("#btnConsignmentReportDownloadCSV").prop("disabled", true);
                            $("#btnConsignmentReportPrintPDF").prop("disabled", true);
                        }

                        cboConsignmentReportBranchFilter.dispose();
                        cboConsignmentReportBranchFilter = new wijmo.input.ComboBox('#cboConsignmentReportBranchFilter', {
                            disabled: disabled,
                            isEditable: false,
                            itemsSource: branchObservableArray,
                            displayMemberPath: "Branch",
                            selectedValuePath: "Id",
                            selectedValue: getURLParameterValue("BranchId"),
                            onSelectedIndexChanged: function () {
                                NProgress.start();
                                refreshConsignmentReportFlexGrid();
                            }
                        });

                        if (!isCboConsignmentReportCompanyFilterSelected) {
                            createCboSupplierFilter();
                        } else {
                            refreshConsignmentReportFlexGrid();
                        }
                    }
                });
            }
        }

        // ============================
        // Create Combo Supplier Filter
        // ============================
        function createCboSupplierFilter() {
            var supplierObservableArray = new wijmo.collections.ObservableArray();

            $.ajax({
                url: '/api/listArticleByArticleTypeId/3',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            supplierObservableArray.push({
                                Id: results[i]["Id"],
                                Supplier: results[i]["Article"]
                            });
                        }
                    }

                    var disabled = true;
                    if (results.length > 0) {
                        disabled = false;
                    } else {

                    }

                    cboConsignmentReportSupplierFilter.dispose();
                    cboConsignmentReportSupplierFilter = new wijmo.input.ComboBox('#cboConsignmentReportSupplierFilter', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: supplierObservableArray,
                        displayMemberPath: "Supplier",
                        selectedValuePath: "Id",
                        selectedValue: getURLParameterValue("SupplierId"),
                        onSelectedIndexChanged: function () {
                            NProgress.start();
                            refreshConsignmentReportFlexGrid();
                        }
                    });

                    createCboDisplayNumberOfRows();
                }
            });
        }

        // ===================================
        // Create Combo Display Number of Rows
        // ===================================
        function createCboDisplayNumberOfRows() {
            var rowsObservableArray = new wijmo.collections.ObservableArray();

            for (var i = 0; i <= 4; i++) {
                var rows = 0;
                var rowsString = "";

                if (i == 0) {
                    rows = 15;
                    rowsString = "Show 15 Rows";
                } else if (i == 1) {
                    rows = 50;
                    rowsString = "Show 50 Rows";
                } else if (i == 2) {
                    rows = 100;
                    rowsString = "Show 100 Rows";
                } else if (i == 3) {
                    rows = 150;
                    rowsString = "Show 150 Rows";
                } else {
                    rows = 200;
                    rowsString = "Show 200 Rows";
                }

                rowsObservableArray.push({
                    rowNumber: rows,
                    rowString: rowsString
                });
            }

            cboConsignmentReportDisplayNumberOfRows.dispose();
            cboConsignmentReportDisplayNumberOfRows = new wijmo.input.ComboBox('#cboConsignmentReportDisplayNumberOfRows', {
                isEditable: false,
                itemsSource: rowsObservableArray,
                displayMemberPath: "rowString",
                selectedValuePath: "rowNumber",
                selectedValue: 50,
                onSelectedIndexChanged: function () {
                    NProgress.start();

                    setTimeout(function () {
                        var numberOfRows = cboConsignmentReportDisplayNumberOfRows.selectedValue;
                        consignmentReportCollectionView.pageSize = parseInt(numberOfRows);
                        consignmentReportCollectionView.refresh();
                        consignmentReportFlexGrid.refresh();

                        NProgress.done();
                    }, 300);
                }
            });

            if (!isCboConsignmentReportCompanyFilterSelected) {
                createConsignmentReportFlexGrid();
            } else {
                refreshConsignmentReportFlexGrid();
            }
        }

        // ===========================
        // Get Consignment Report Data
        // ===========================
        function getConsignmentReportData() {
            var consignmentReports = new wijmo.collections.ObservableArray;
            if (cboConsignmentReportCompanyFilter.selectedValue != null) {
                if (cboConsignmentReportBranchFilter.selectedValue != null) {
                    if (cboConsignmentReportSupplierFilter.selectedValue != null) {

                        var startDate = cboConsignmentReportStartDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                        var endDate = cboConsignmentReportEndDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                        var companyId = cboConsignmentReportCompanyFilter.selectedValue;
                        var branchId = cboConsignmentReportBranchFilter.selectedValue;
                        var supplierId = cboConsignmentReportSupplierFilter.selectedValue;

                        $.ajax({
                            url: '/api/consignmentReport/list/' + startDate + '/' + endDate + '/' + + companyId + '/' + branchId + '/' + supplierId,
                            cache: false,
                            type: 'GET',
                            contentType: 'application/json; charset=utf-8',
                            success: function (results) {
                                var inpTotalAmount = 0;
                                if (results.length > 0) {
                                    for (i = 0; i < results.length; i++) {
                                        consignmentReports.push({
                                            ItemManualArticleOldCode: results[i]["ItemManualArticleOldCode"],
                                            ItemCode: results[i]["ItemCode"],
                                            ItemDescription: results[i]["ItemDescription"],
                                            Unit: results[i]["Unit"],
                                            Quantity: results[i]["Quantity"],
                                            QuantityDisplay: parseFloat(results[i]["Quantity"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                            Cost: results[i]["Cost"],
                                            CostDisplay: parseFloat(results[i]["Cost"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                            Amount: results[i]["Amount"],
                                            AmountDisplay: parseFloat(results[i]["Amount"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        });

                                        inpTotalAmount += results[i]["Amount"];
                                    }
                                }

                                document.getElementById("inpTotalAmount").value = parseFloat(inpTotalAmount).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                                NProgress.done();
                            }
                        });
                    } else {
                        document.getElementById("inpTotalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                        NProgress.done();
                    }
                } else {
                    document.getElementById("inpTotalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    NProgress.done();
                }
            } else {
                document.getElementById("inpTotalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                NProgress.done();
            }

            return consignmentReports;
        }

        // ===================================
        // Create Consignment Report Flex Grid
        // ===================================
        function createConsignmentReportFlexGrid() {
            consignmentReportCollectionView = new wijmo.collections.CollectionView(getConsignmentReportData());
            consignmentReportCollectionView.canFilter = true;
            consignmentReportCollectionView.pageSize = cboConsignmentReportDisplayNumberOfRows.selectedValue;

            var filterText = '';
            $('#inpConsignmentReportSearchFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                consignmentReportCollectionView.refresh();
            });

            consignmentReportCollectionView.filter = function (item) {
                return !filterText || (item.ItemManualArticleOldCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Unit.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemDescription.toLowerCase().indexOf(filterText) > -1);
            }

            consignmentReportCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateConsignmentReportNavigationButtons();
            });

            consignmentReportFlexGrid = new wijmo.grid.FlexGrid('#consignmentReportFlexGrid');
            consignmentReportFlexGrid.initialize({
                columns: [
                    {
                        "header": "SKU",
                        "binding": "ItemManualArticleOldCode",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Barcode",
                        "binding": "ItemCode",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Description",
                        "binding": "ItemDescription",
                        "allowSorting": true,
                        "width": "3*"
                    },
                    {
                        "header": "Unit",
                        "binding": "Unit",
                        "allowSorting": true,
                        "width": "1*"
                    },
                    {
                        "header": "Quantity",
                        "binding": "QuantityDisplay",
                        "allowSorting": true,
                        "width": "1.5*",
                        "align": "right"
                    },
                    {
                        "header": "Cost",
                        "binding": "CostDisplay",
                        "allowSorting": true,
                        "width": "1.5*",
                        "align": "right"
                    },
                    {
                        "header": "Amount",
                        "binding": "AmountDisplay",
                        "allowSorting": true,
                        "width": "1.5*",
                        "align": "right"
                    }
                ],
                autoGenerateColumns: false,
                itemsSource: consignmentReportCollectionView,
                isReadOnly: true,
                autoSizeMode: wijmo.grid.AutoSizeMode.Both,
                allowDragging: wijmo.grid.AllowDragging.None,
                selectionMode: wijmo.grid.SelectionMode.Row
            });

            consignmentReportFlexGrid.trackChanges = true;

            updateConsignmentReportNavigationButtons();

            btnConsignmentReportMoveToFirstPageGrid.addEventListener('click', function () {
                consignmentReportCollectionView.moveToFirstPage();
                consignmentReportCurrentPageIndex = consignmentReportCollectionView.pageIndex + 1;
                updateConsignmentReportNavigationButtons();
            });

            btnConsignmentReportMoveToPreviousPageGrid.addEventListener('click', function () {
                consignmentReportCollectionView.moveToPreviousPage();
                consignmentReportCurrentPageIndex = consignmentReportCollectionView.pageIndex + 1;
                updateConsignmentReportNavigationButtons();
            });

            btnConsignmentReportMoveToNextPageGrid.addEventListener('click', function () {
                consignmentReportCollectionView.moveToNextPage();
                consignmentReportCurrentPageIndex = consignmentReportCollectionView.pageIndex + 1;
                updateConsignmentReportNavigationButtons();
            });

            btnConsignmentReportMoveToLastPageGrid.addEventListener('click', function () {
                consignmentReportCollectionView.moveToLastPage();
                consignmentReportCurrentPageIndex = consignmentReportCollectionView.pageIndex + 1;
                updateConsignmentReportNavigationButtons();
            });
        }

        // =====================================
        // Update Consignment Navigation Buttons
        // =====================================
        function updateConsignmentReportNavigationButtons() {
            if (consignmentReportCollectionView.pageSize <= 0) {
                document.getElementById('consignmentReportNavigationPageGrid').style.display = 'none';
                return;
            }
            document.getElementById('consignmentReportNavigationPageGrid').style.display = 'block';
            if (consignmentReportCollectionView.pageIndex === 0) {
                btnConsignmentReportMoveToFirstPageGrid.setAttribute('disabled', 'disabled');
                btnConsignmentReportMoveToPreviousPageGrid.setAttribute('disabled', 'disabled');
                btnConsignmentReportMoveToNextPageGrid.removeAttribute('disabled');
                btnConsignmentReportMoveToLastPageGrid.removeAttribute('disabled');
            } else if (consignmentReportCollectionView.pageIndex === (consignmentReportCollectionView.pageCount - 1)) {
                btnConsignmentReportMoveToFirstPageGrid.removeAttribute('disabled');
                btnConsignmentReportMoveToPreviousPageGrid.removeAttribute('disabled');
                btnConsignmentReportMoveToLastPageGrid.setAttribute('disabled', 'disabled');
                btnConsignmentReportMoveToNextPageGrid.setAttribute('disabled', 'disabled');
            } else {
                btnConsignmentReportMoveToFirstPageGrid.removeAttribute('disabled');
                btnConsignmentReportMoveToPreviousPageGrid.removeAttribute('disabled');
                btnConsignmentReportMoveToNextPageGrid.removeAttribute('disabled');
                btnConsignmentReportMoveToLastPageGrid.removeAttribute('disabled');
            }
            btnConsignmentReportCurrentPageGrid.innerHTML = (consignmentReportCollectionView.pageIndex + 1) + ' / ' + consignmentReportCollectionView.pageCount;
        }

        // =================
        // Refresh Flex Grid
        // =================
        function refreshConsignmentReportFlexGrid() {
            consignmentReportCollectionView = new wijmo.collections.CollectionView(getConsignmentReportData());
            consignmentReportCollectionView.canFilter = true;
            consignmentReportCollectionView.pageSize = cboConsignmentReportDisplayNumberOfRows.selectedValue;

            var filterText = '';
            $('#inpConsignmentReportSearchFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                consignmentReportCollectionView.refresh();
            });

            consignmentReportCollectionView.filter = function (item) {
                return !filterText || (item.ItemManualArticleOldCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Unit.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemDescription.toLowerCase().indexOf(filterText) > -1);
            }

            consignmentReportCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateConsignmentReportNavigationButtons();
            });

            consignmentReportFlexGrid.itemsSource = consignmentReportCollectionView;
            consignmentReportFlexGrid.trackChanges = true;
        }

        // ============
        // Download CSV
        // ============
        function btnConsignmentReportDownloadCSVOnclick() {
            var CSV = '';
            var consignmentReport = [];
            var fileName = 'ConsignmentReport.CSV';

            consignmentReportCollectionView.moveToFirstPage();

            for (var p = 1; p <= consignmentReportCollectionView.pageCount; p++) {
                for (i = 0; i < consignmentReportCollectionView.items.length; i++) {

                    consignmentReport.push({
                        ItemManualArticleOldCode: consignmentReportCollectionView.items[i].ItemManualArticleOldCode,
                        ItemCode: consignmentReportCollectionView.items[i].ItemCode,
                        ItemDescription: consignmentReportCollectionView.items[i].ItemDescription,
                        Unit: consignmentReportCollectionView.items[i].Unit,
                        Quantity: consignmentReportCollectionView.items[i].Quantity,
                        Cost: consignmentReportCollectionView.items[i].Cost,
                        Amount: consignmentReportCollectionView.items[i].Amount
                    });
                }

                consignmentReportCollectionView.moveToNextPage();
                if (p == consignmentReportCollectionView.pageCount) {
                    consignmentReportCollectionView.moveToPage(consignmentReportCurrentPageIndex - 1);
                }
            }

            var Headers = [];
            Headers.push({
                ItemManualArticleOldCode: "SKU",
                ItemCode: "Barcode",
                ItemDescription: "Description",
                Unit: "Unit",
                Quantity: "Quantity",
                Cost: "Cost",
                Amount: "Amount"
            });

            var header = '';
            for (var b in Headers[0]) {
                header += '"' + Headers[0][b] + '",';
            }

            CSV = header + '\r\n';
            for (var i = 0; i < consignmentReport.length; i++) {
                var row = '';
                for (var a in consignmentReport[i]) {
                    row += '"' + consignmentReport[i][a] + '",';
                }

                row.slice(0, row.length - 1);
                CSV += row + '\r\n';
            }

            if (CSV == '') {
                toastr.error("No data.");
            } else {
                var link = document.createElement("a");
                if (link.download !== undefined) {
                    var blob = new Blob([CSV], { type: 'text/csv;charset=utf-8;' });
                    var url = URL.createObjectURL(blob);
                    link.setAttribute("href", url);
                    link.setAttribute("download", fileName);
                    link.style = "visibility:hidden";
                }

                if (navigator.msSaveBlob) {
                    link.addEventListener("click", function (event) {
                        var blob = new Blob([CSV], {
                            "type": "text/csv;charset=utf-8;"
                        });
                        navigator.msSaveBlob(blob, fileName);
                    }, false);
                }

                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);

                toastr.success("Dowload successful.");
            }
        }

        // =========
        // Print PDF
        // =========
        function btnConsignmentReportPrintPDFOnclick() {
            if (cboConsignmentReportCompanyFilter.selectedValue != null) {
                if (cboConsignmentReportBranchFilter.selectedValue != null) {
                    if (cboConsignmentReportSupplierFilter.selectedValue != null) {
                        var startDate = cboConsignmentReportStartDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                        var endDate = cboConsignmentReportEndDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                        var companyId = cboConsignmentReportCompanyFilter.selectedValue;
                        var branchId = cboConsignmentReportBranchFilter.selectedValue;
                        var supplierId = cboConsignmentReportSupplierFilter.selectedValue;

                        window.location.href = '/RepConsignmentReport/ConsignmentReport?StartDate=' + startDate + '&EndDate=' + endDate + '&CompanyId=' + companyId + '&BranchId=' + branchId + '&SupplierId=' + supplierId;
                    } else {
                        toastr.error("Lacking Information");
                    }
                } else {
                    toastr.error("Lacking Information");
                }
            } else {
                toastr.error("Lacking Information");
            }
        }

        // ============
        // On Load Page
        // ============
        $(document).ready(function () {
            createCboDateRangeFilters();
        });
    </script>
    <script type="text/javascript">
        // =================
        // User Branch Combo
        // =================
        var cboUserBranches = new wijmo.input.ComboBox('#cboUserBranches');

        // =========================
        // Get Current User Branches
        // =========================
        function getUserBranches() {
            $("#loadingUserBranches").show();
            document.getElementById("loadingUserBranches").innerHTML = '<i class="fa fa-spinner fa-spin"></i>';
            $("#userBranchContent").hide();

            $('#userBranchesModal').modal({ show: true, backdrop: 'static' });

            document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
            $("#updateUserBranch").prop("disabled", false);
            $("#closeUserBranch").prop("disabled", false);

            var userBranchesObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/userBranch/list',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (userBranchesResults) {
                    if (userBranchesResults.length > 0) {
                        for (i = 0; i < userBranchesResults.length; i++) {
                            userBranchesObservableArray.push({
                                BranchId: userBranchesResults[i]["BranchId"],
                                Branch: userBranchesResults[i]["Branch"]
                            });
                        }
                    }

                    var currentBranch = "@ViewData["BranchId"]";
                    cboUserBranches.dispose();
                    cboUserBranches = new wijmo.input.ComboBox('#cboUserBranches', {
                        isEditable: false,
                        placeholder: "Select Branches",
                        itemsSource: userBranchesObservableArray,
                        displayMemberPath: "Branch",
                        selectedValuePath: "BranchId",
                        selectedValue: currentBranch
                    });

                    setTimeout(function () {
                        $("#loadingUserBranches").hide();
                        $("#userBranchContent").show();
                    }, 500)
                }
            });
        }

        // ==========================
        // Update Current User Branch
        // ==========================
        function updateUserBranchOnclick() {
            document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-spinner fa-spin'></i> Updating";
            $("#updateUserBranch").prop("disabled", true);
            $("#closeUserBranch").prop("disabled", true);

            var userBranchObject = new Object();
            userBranchObject.BranchId = cboUserBranches.selectedValue;
            var userBranchData = JSON.stringify(userBranchObject);

            $.ajax({
                type: "PUT",
                url: '/api/userBranch/update',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: userBranchData,
                statusCode: {
                    200: function () {
                        toastr.success("Update Successful");
                        window.setTimeout(function () {
                            location.reload()
                        }, 1000);
                    },
                    404: function () {
                        toastr.error("Not Found");
                        document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
                        $("#updateUserBranch").prop("disabled", false);
                        $("#closeUserBranch").prop("disabled", false);
                    },
                    500: function () {
                        toastr.error("Bad Request");
                        document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
                        $("#updateUserBranch").prop("disabled", false);
                        $("#closeUserBranch").prop("disabled", false);
                    }
                }
            });
        }
    </script>
