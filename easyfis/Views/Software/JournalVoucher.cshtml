@{
    var branchId = Request.QueryString["BranchId"];
}
<!DOCTYPE html>
<html>
<head>
    <!-- Meta -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <meta name="description" content="Some slide and push menu demos using CSS3 transitions.">

    <!-- Title of the Page -->
    <title>Journal Voucher</title>

    <!-- CSS StyleSheets-->
    @Styles.Render("~/Font-Awesome/css")
    @Styles.Render("~/Content/Software-css")
    @Styles.Render("~/WijmoCSS/Wijmo-css")
</head>
<body>
    <div id="wrapper" class="wrapper">
        <!-- Header -->
        @Html.Partial("_SoftwareHeader")

        <!-- Section -->
        <section>
            <br /><br /><br />
            <div class="container">
                <!-- Journal Voucher List -->
                <div id="journalVoucherList">
                    <div class="row">
                        <div class="col-lg-12">
                            <h4><i class="fa fa-book fa-2x"></i> Journal Voucher</h4>
                            <br />
                            <div class="div_Current_Page">
                                <span id="currentPage"></span>
                            </div>
                        </div>
                    </div>
                    <label>Branch </label>
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="comboBox-wide" id="branch_Id"></div>
                            <input type="hidden" id="branch_Id_data" />
                            <input type="hidden" id="currentBranchName" />
                        </div>
                        <div class="col-lg-8"></div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="input-group input-group-sm">
                                <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                <input type="text" class="form-control input-sm" id="InputFilterJournalVoucher" placeholder="Search">
                            </div>
                        </div>
                        <div class="col-lg-8" align="right">
                            <a href="@Url.Action("JournalVoucherDetail", "Software", new { JournalVoucherId = 0, BranchId = branchId })" class="btn btn-primary btn-sm"><i class="fa fa-plus"></i> Add</a>
                            <a href="@Url.Action("Index", "Software", new { BranchId = branchId })" class="btn btn-danger btn-sm" id="cmdCloseButtonCompany"><i class="fa fa-times"></i> Close</a>
                        </div>
                    </div>
                    <br />
                    <div class="row" align="left">
                        <div class="col-lg-12">
                            <div id="JournalVoucherGrid" class="grid" align="left"></div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="btn-group col-md-7" id="naviagtionPageGridJournalVoucher">
                            <button type="button" class="btn btn-default" id="btnMoveToFirstPageGridJournalVoucher">
                                <span class="glyphicon glyphicon-fast-backward"></span>
                            </button>
                            <button type="button" class="btn btn-default" id="btnMoveToPreviousPageGridJournalVoucher">
                                <span class="glyphicon glyphicon-step-backward"></span>
                            </button>
                            <button type="button" class="btn btn-default" disabled style="width: 100px" id="btnCurrentPageGridJournalVoucher"></button>
                            <button type="button" class="btn btn-default" id="btnMoveToNextPageGridJournalVoucher">
                                <span class="glyphicon glyphicon-step-forward"></span>
                            </button>
                            <button type="button" class="btn btn-default" id="btnMoveToLastPageGridJournalVoucher">
                                <span class="glyphicon glyphicon-fast-forward"></span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <!-- Journal Voucher Delete -->
        <div class="modal fade" id="deleteJournalVoucherConfirm" role="dialog">
            <div class="modal-dialog modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Delete Journal Voucher <i class="fa fa-trash"></i></h4>
                    </div>
                    <div class="modal-body">
                        Are you sure you want to delete this?
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-danger btn-sm" id="cmdJournalVoucherDeleteCompany" onclick="cmdJournalVoucherConfirmDeleteButton_OnClick()"><i class="fa fa-trash"></i> Delete</button>
                        <button id="cmdConfirmDeleteCloseButtonJournalVoucher" class="btn btn-primary btn-sm" data-dismiss="modal"><i class="fa fa-close"></i> Cancel</button>
                    </div>
                </div>
            </div>
        </div>

        <!-- Delete Lock -->
        <div class="modal fade" id="deleteRecordLock" role="dialog">
            <div class="modal-dialog modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Record Locked <i class="fa fa-lock"></i></h4>
                    </div>
                    <div class="modal-body">
                        Sorry, the record is lock! It can't be deleted!
                    </div>
                    <div class="modal-footer">
                        <button id="cmdConfirmDeleteCloseButton" class="btn btn-primary btn-sm" data-dismiss="modal"> Okay</button>
                    </div>
                </div>
            </div>
        </div>

        <!-- Loading Modal -->
        <div class="modal fade" id="loadingModal" tabindex="-1" role="dialog" aria-labelledby="Loading..." aria-hidden="true" data-backdrop="static" data-keyboard="false">
            <div class=" modal-dialog modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-body">
                        <h4>Loading ...</h4>
                    </div>
                    <div class="modal-footer">
                        <img class="img-thumbnail img-responsive center-block img-custom" src='~/images/loading/progress_bar.gif'>
                    </div>
                </div>
            </div>
        </div>

        <!-- Footer -->
        @Html.Partial("_SoftwareFooter")
    </div>

    <!-- SideBar Menu -->
    @Html.Partial("_SoftwareSideBarMenu")

    <div id="c-mask" class="c-mask"></div>
    <!-- /c-mask -->
    @Scripts.Render("~/Scripts/Software-js")
    @Scripts.Render("~/WijmoScripts/Wijmo-js")
    <script type="text/javascript">
        // ================
        // Global Variables
        // ================
        var journalVouchers;
        var journalVoucherGrid;

        var branches;
        var cboBranch;

        var btnFirstPageGridJournalVoucher;
        var btnPreviousPageGridJournalVoucher;
        var btnNextPageGridJournalVoucher;
        var btnLastPageGridJournalVoucher;
        var btnCurrentPageGridJournalVoucher;

        // ========================
        // Get Journal Voucher LIST
        // ========================
        function getJournalVoucher() {
            var journalVouchers = new wijmo.collections.ObservableArray;
            $('#loadingModal').modal('show');
            //var JV_Number = Results.JVNumber;
            //var JV_Number_Display = JV_Number.replace(/(\d{3})(\d+)/, '$1-$2');
            //document.getElementById('journalVoucher_No_Display').value = JV_Number_Display;

            $.ajax({
                url: '/api/listJournalVoucherByBranchId/' + @branchId,
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (Results) {
                    $('#loadingModal').modal('hide');
                    if (Results.length > 0) {
                        for (i = 0; i < Results.length; i++) {
                            if(Results[i]["IsLocked"] == true) {
                                journalVouchers.push({
                                    EditId: "<button class='btn btn-primary btn-xs btn-block' onclick='cmdJournalVoucherEditButton_OnClick()'><i class='fa fa-edit'></i> Edit</button>",
                                    DeleteId: "<button class='btn btn-danger btn-xs btn-block' onclick='cmdJournalVoucherDeleteLockButton_OnClick()'><i class='fa fa-trash'></i> Delete</button>",
                                    Id: Results[i]["Id"],
                                    BranchId: Results[i]["BranchId"],
                                    Branch: Results[i]["Branch"],
                                    JVNumber: Results[i]["JVNumber"],
                                    JVDate: Results[i]["JVDate"],
                                    Particulars: Results[i]["Particulars"],
                                    ManualJVNumber: Results[i]["ManualJVNumber"],
                                    PreparedById: Results[i]["PreparedById"],
                                    PreparedBy: Results[i]["PreparedBy"],
                                    CheckedById: Results[i]["CheckedById"],
                                    CheckedBy: Results[i]["CheckedBy"],
                                    ApprovedById: Results[i]["ApprovedById"],
                                    ApprovedBy: Results[i]["ApprovedBy"],
                                    IsLocked: Results[i]["IsLocked"],
                                    CreatedById: Results[i]["CreatedById"],
                                    CreatedBy: Results[i]["CreatedBy"],
                                    CreatedDateTime: Results[i]["CreatedDateTime"],
                                    UpdatedById: Results[i]["UpdatedById"],
                                    UpdatedBy: Results[i]["UpdatedBy"],
                                    UpdatedDateTime: Results[i]["UpdatedDateTime"]
                                });
                            } else {
                                journalVouchers.push({
                                    EditId: "<button class='btn btn-primary btn-xs btn-block' onclick='cmdJournalVoucherEditButton_OnClick()'><i class='fa fa-edit'></i> Edit</button>",
                                    DeleteId: "<button class='btn btn-danger btn-xs btn-block' onclick='cmdJournalVoucherDeleteButton_OnClick()'><i class='fa fa-trash'></i> Delete</button>",
                                    Id: Results[i]["Id"],
                                    BranchId: Results[i]["BranchId"],
                                    Branch: Results[i]["Branch"],
                                    JVNumber: Results[i]["JVNumber"],
                                    JVDate: Results[i]["JVDate"],
                                    Particulars: Results[i]["Particulars"],
                                    ManualJVNumber: Results[i]["ManualJVNumber"],
                                    PreparedById: Results[i]["PreparedById"],
                                    PreparedBy: Results[i]["PreparedBy"],
                                    CheckedById: Results[i]["CheckedById"],
                                    CheckedBy: Results[i]["CheckedBy"],
                                    ApprovedById: Results[i]["ApprovedById"],
                                    ApprovedBy: Results[i]["ApprovedBy"],
                                    IsLocked: Results[i]["IsLocked"],
                                    CreatedById: Results[i]["CreatedById"],
                                    CreatedBy: Results[i]["CreatedBy"],
                                    CreatedDateTime: Results[i]["CreatedDateTime"],
                                    UpdatedById: Results[i]["UpdatedById"],
                                    UpdatedBy: Results[i]["UpdatedBy"],
                                    UpdatedDateTime: Results[i]["UpdatedDateTime"]
                                });
                            }
                        }
                    }
                }
            }).fail(function (xhr, textStatus, err) {
                alert(err);
            });
            return journalVouchers;
        }

        // ==================================
        // Journal Voucher Delete Lock Button
        // ==================================
        function cmdJournalVoucherDeleteLockButton_OnClick() {
            $('#deleteRecordLock').modal({
                show: true,
                backdrop: 'static'
            });
        }

        // ===========================
        // Journal Voucher Edit Button
        // ===========================
        function cmdJournalVoucherEditButton_OnClick() {
            journalVouchers.editItem(journalVouchers.currentItem);
            var journalVoucher = journalVouchers.currentEditItem;
            var id = journalVoucher.Id;
            window.location = '/Software/JournalVoucherDetail?JournalVoucherId=' + id + '&BranchId=' + @branchId;
        }

        // =============================
        // Journal Voucher delete Button
        // =============================
        function cmdJournalVoucherDeleteButton_OnClick() {
            $('#deleteJournalVoucherConfirm').modal({
                show: true,
                backdrop: 'static'
            });

            $("#cmdJournalVoucherDeleteCompany").prop("disabled", false);
            $("#cmdConfirmDeleteCloseButtonJournalVoucher").prop("disabled", false);
        }
        // Journal Voucher Confirm Delete Button
        function cmdJournalVoucherConfirmDeleteButton_OnClick() {
            $("#cmdJournalVoucherDeleteCompany").prop("disabled", true);
            $("#cmdConfirmDeleteCloseButtonJournalVoucher").prop("disabled", true);

            journalVouchers.editItem(journalVouchers.currentItem);
            var id = journalVouchers.currentEditItem.Id;
            $.ajax({
                url: '/api/deleteJournalVoucher/' + id,
                type: "DELETE",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                statusCode: {
                    200: function () {
                        $("#cmdJournalVoucherDeleteCompany").prop("disabled", true);
                        $("#cmdConfirmDeleteCloseButtonJournalVoucher").prop("disabled", true);
                        toastr.success("Successfully Deleted");
                        
                        $('#deleteJournalVoucherConfirm').modal('hide');

                        journalVouchers = new wijmo.collections.CollectionView(getJournalVoucher());
                        journalVouchers.canFilter = true;
                        journalVouchers.pageSize = 15;

                        var filterTextJournalVoucher = '';
                        $('#InputFilterJournalVoucher').keyup(function () {
                            filterTextJournalVoucher = this.value.toLowerCase();
                            journalVouchers.refresh();
                        });
                        journalVouchers.filter = function (item) {
                            return !filterTextJournalVoucher || (item.JVNumber.toLowerCase().indexOf(filterTextJournalVoucher) > -1);
                        }
                        journalVouchers.collectionChanged.addHandler(function (sender, args) {
                            updateNavigateButtonsJournalVoucher();
                        });
                        journalVouchers.currentChanged.addHandler(function (sender, args) {
                            updateDetailsJournalVoucher();
                        });

                        // Flex Grid
                        journalVoucherGrid.itemsSource = journalVouchers;
                        journalVoucherGrid.trackChanges = true;
                    },
                    404: function () {
                        toastr.error("Request Not Found");
                        $('#deleteJournalVoucherConfirm').modal('hide');
                        $("#cmdJournalVoucherDeleteCompany").prop("disabled", false);
                        $("#cmdConfirmDeleteCloseButtonJournalVoucher").prop("disabled", false);
                    },
                    400: function () {
                        toastr.error("Can't be deleted");
                        $('#deleteJournalVoucherConfirm').modal('hide');
                        $("#cmdJournalVoucherDeleteCompany").prop("disabled", false);
                        $("#cmdConfirmDeleteCloseButtonJournalVoucher").prop("disabled", false);
                    }
                }
            });
        }

        // ========================
        // GET Branch for Combo Box
        // ========================
        function getBranchesForComboBox() {
            branches = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/listBranch',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            branches.push({
                                BranchId: results[i]["Id"],
                                BranchBranch: results[i]["Branch"]
                            });
                        }

                        $.ajax({
                            url: '/api/listBranchById/' + @branchId,
                            cache: false,
                            type: 'GET',
                            contentType: 'application/json; charset=utf-8',
                            data: {},
                            success: function (results) {
                                if (results.length > 0) {
                                    branches.push({
                                        BranchId: results.Id,
                                        BranchBranch: results.Branch
                                    });
                                }
                                //document.getElementById('currentBranchName').innerHTML = '<a href="@Url.Action("Index", "Software", new { BranchId = branchId })" class="branchNameStyle">' + results.Branch + "</a>: /";
                                document.getElementById('currentBranchName').value = results.Branch;
                            }
                        });

                        setTimeout(function() {
                            createCboBranch(branches);
                        }, 200);
                    }
                }
            }).fail(function (xhr, textStatus, err) {
                alert(err);
            }
            );
        }

        // ======================
        // Combo Box for Branches
        // ======================
        function createCboBranch(branches) {
            var branchCollections = new wijmo.collections.CollectionView(branches);
            var branchList = new Array();
            for (var i = 0; i < branchCollections.items.length; i++) {
                branchList.push(branchCollections.items[i].BranchBranch);
            }

            cboBranch = new wijmo.input.ComboBox('#branch_Id', {
                placeholder: "Select Branch",
                itemsSource: branchList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('currentBranchName').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#branch_Id_data").val(branchCollections.items[this.selectedIndex].BranchId);
                    $('#cmdGoGetBranch').prop('disabled', false);

                    var branchId = document.getElementById('branch_Id_data').value;
                    //window.location = '/Software/JournalVoucher?BranchId=' + branchId;

                    journalVouchers = new wijmo.collections.CollectionView(getJVByBranchIDClick(branchId));
                    journalVouchers.canFilter = true;
                    journalVouchers.pageSize = 15;

                    var filterTextJournalVoucher = '';
                    $('#InputFilterJournalVoucher').keyup(function () {
                        filterTextJournalVoucher = this.value.toLowerCase();
                        journalVouchers.refresh();
                    });
                    journalVouchers.filter = function (item) {
                        return !filterTextJournalVoucher || (item.JVNumber.toLowerCase().indexOf(filterTextJournalVoucher) > -1);
                    }
                    journalVouchers.collectionChanged.addHandler(function (sender, args) {
                        updateNavigateButtonsJournalVoucher();
                    });
                    journalVouchers.currentChanged.addHandler(function (sender, args) {
                        updateDetailsJournalVoucher();
                    });

                    // Flex Grid
                    journalVoucherGrid.itemsSource = journalVouchers;
                    journalVoucherGrid.trackChanges = true;

                    updateNavigateButtonsJournalVoucher();
                }
            });
        }

        // ====================
        // Get JV in Go Buttom
        // ===================
        function getJVByBranchIDClick(branchId) {
            var journalVouchers = new wijmo.collections.ObservableArray;
            $('#loadingModal').modal('show');

            $.ajax({
                url: '/api/listJournalVoucherByBranchId/' + branchId,
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (Results) {
                    $('#loadingModal').modal('hide');
                    if (Results.length > 0) {
                        for (i = 0; i < Results.length; i++) {
                            journalVouchers.push({
                                EditId: "<button class='btn btn-primary btn-xs btn-block' onclick='cmdJournalVoucherEditButton_OnClick()'><i class='fa fa-edit'></i> Edit</button>",
                                DeleteId: "<button class='btn btn-danger btn-xs btn-block' onclick='cmdJournalVoucherDeleteButton_OnClick()'><i class='fa fa-trash'></i> Delete</button>",
                                Id: Results[i]["Id"],
                                BranchId: Results[i]["BranchId"],
                                Branch: Results[i]["Branch"],
                                JVNumber: Results[i]["JVNumber"],
                                JVDate: Results[i]["JVDate"],
                                Particulars: Results[i]["Particulars"],
                                ManualJVNumber: Results[i]["ManualJVNumber"],
                                PreparedById: Results[i]["PreparedById"],
                                PreparedBy: Results[i]["PreparedBy"],
                                CheckedById: Results[i]["CheckedById"],
                                CheckedBy: Results[i]["CheckedBy"],
                                ApprovedById: Results[i]["ApprovedById"],
                                ApprovedBy: Results[i]["ApprovedBy"],
                                IsLocked: Results[i]["IsLocked"],
                                CreatedById: Results[i]["CreatedById"],
                                CreatedBy: Results[i]["CreatedBy"],
                                CreatedDateTime: Results[i]["CreatedDateTime"],
                                UpdatedById: Results[i]["UpdatedById"],
                                UpdatedBy: Results[i]["UpdatedBy"],
                                UpdatedDateTime: Results[i]["UpdatedDateTime"]
                            });
                        }
                    }
                }
            }).fail(function (xhr, textStatus, err) {
                alert(err);
            });
            return journalVouchers;
        }

        // ===================
        // FlexGrid Selection
        // ===================
        function updateDetailsJournalVoucher() {
            var item = journalVouchers.currentItem;
            document.getElementById('createdBy').innerHTML = item.CreatedBy;
            document.getElementById('createdDate').innerHTML = item.CreatedDateTime;
            document.getElementById('updatedBy').innerHTML = item.UpdatedBy;
            document.getElementById('updatedDate').innerHTML = item.UpdatedDateTime;
        }

        // ======================================
        // Navigation Buttons for Journal Voucher
        // ======================================
        function updateNavigateButtonsJournalVoucher() {
            if (journalVouchers.pageSize <= 0) {
                document.getElementById('naviagtionPageGridJournalVoucher').style.display = 'none';
                return;
            }
            document.getElementById('naviagtionPageGridJournalVoucher').style.display = 'block';
            if (journalVouchers.pageIndex === 0) {
                btnFirstPageGridJournalVoucher.setAttribute('disabled', 'disabled');
                btnPreviousPageGridJournalVoucher.setAttribute('disabled', 'disabled');
                btnNextPageGridJournalVoucher.removeAttribute('disabled');
                btnLastPageGridJournalVoucher.removeAttribute('disabled');
            } else if (journalVouchers.pageIndex === (journalVouchers.pageCount - 1)) {
                btnFirstPageGridJournalVoucher.removeAttribute('disabled');
                btnPreviousPageGridJournalVoucher.removeAttribute('disabled');
                btnLastPageGridJournalVoucher.setAttribute('disabled', 'disabled');
                btnNextPageGridJournalVoucher.setAttribute('disabled', 'disabled');
            } else {
                btnFirstPageGridJournalVoucher.removeAttribute('disabled');
                btnPreviousPageGridJournalVoucher.removeAttribute('disabled');
                btnNextPageGridJournalVoucher.removeAttribute('disabled');
                btnLastPageGridJournalVoucher.removeAttribute('disabled');
            }
            btnCurrentPageGridJournalVoucher.innerHTML = (journalVouchers.pageIndex + 1) + ' / ' + journalVouchers.pageCount;
        }


        // ============
        // On Load Page
        // ============
        $(document).ready(function () {
            document.getElementById('currentPage').innerHTML = '<a href="@Url.Action("Index", "Software", new { BranchId = branchId })" class="currentPageLinkCustom">Main Menu</a> ' + ' > Journal Voucher';

            $('#cmdGoGetBranch').prop('disabled', true);

            getBranchesForComboBox();

            journalVouchers = new wijmo.collections.CollectionView(getJournalVoucher());
            journalVouchers.canFilter = true;
            journalVouchers.pageSize = 15;

            var filterTextJournalVoucher = '';
            $('#InputFilterJournalVoucher').keyup(function () {
                filterTextJournalVoucher = this.value.toLowerCase();
                journalVouchers.refresh();
            });
            journalVouchers.filter = function (item) {
                return !filterTextJournalVoucher || (item.JVNumber.toLowerCase().indexOf(filterTextJournalVoucher) > -1);
            }
            journalVouchers.collectionChanged.addHandler(function (sender, args) {
                updateNavigateButtonsJournalVoucher();
            });
            journalVouchers.currentChanged.addHandler(function (sender, args) {
                updateDetailsJournalVoucher();
            });

            // Flex Grid
            journalVoucherGrid = new wijmo.grid.FlexGrid('#JournalVoucherGrid');
            journalVoucherGrid.initialize({
                columns: [
                            {
                                "header": "Edit",
                                "binding": "EditId",
                                "width": 60,
                                "align": "center",
                                "allowResizing": false,
                                "allowSorting": false,
                                "isContentHtml": true
                            },
                            {
                                "header": "Delete",
                                "binding": "DeleteId",
                                "width": 70,
                                "align": "center",
                                "allowResizing": false,
                                "allowSorting": false,
                                "isContentHtml": true
                            },
                            {
                                "header": "JV Number",
                                "binding": "JVNumber",
                                "allowSorting": true,
                                "width": "2*"
                            },
                            {
                                "header": "JV Date",
                                "binding": "JVDate",
                                "allowSorting": true,
                                "width": "2*"
                            },
                            {
                                "header": "Particulars",
                                "binding": "Particulars",
                                "allowSorting": true,
                                "width": "4*"
                            },
                            {
                                "header": "Lock",
                                "binding": "IsLocked",
                                "allowSorting": true,
                                "width": "0.5*"
                            }
                ],
                autoGenerateColumns: false,
                itemsSource: journalVouchers,
                isReadOnly: true,
                //frozenColumns: 2,
                autoSizeMode: wijmo.grid.AutoSizeMode.Both,
                allowDragging: wijmo.grid.AllowDragging.None,
                selectionMode: wijmo.grid.SelectionMode.Row
            });

            journalVoucherGrid.trackChanges = true;

            //Navigation button
            btnFirstPageGridJournalVoucher = document.getElementById('btnMoveToFirstPageGridJournalVoucher');
            btnPreviousPageGridJournalVoucher = document.getElementById('btnMoveToPreviousPageGridJournalVoucher');
            btnNextPageGridJournalVoucher = document.getElementById('btnMoveToNextPageGridJournalVoucher');
            btnLastPageGridJournalVoucher = document.getElementById('btnMoveToLastPageGridJournalVoucher');
            btnCurrentPageGridJournalVoucher = document.getElementById('btnCurrentPageGridJournalVoucher');

            updateNavigateButtonsJournalVoucher();

            btnFirstPageGridJournalVoucher.addEventListener('click', function () {
                journalVouchers.moveToFirstPage();
                updateNavigateButtonsJournalVoucher();
            });
            btnPreviousPageGridJournalVoucher.addEventListener('click', function () {
                journalVouchers.moveToPreviousPage();
                updateNavigateButtonsJournalVoucher();
            });
            btnNextPageGridJournalVoucher.addEventListener('click', function () {
                journalVouchers.moveToNextPage();
                updateNavigateButtonsJournalVoucher();
            });
            btnLastPageGridJournalVoucher.addEventListener('click', function () {
                journalVouchers.moveToLastPage();
                updateNavigateButtonsJournalVoucher();
            });
        });
    </script>
</body>
</html>