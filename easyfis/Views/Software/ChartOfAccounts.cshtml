                            <input type="hidden" id="accountCategory_Id" />
                            <dl class="dl-horizontal">
                                <dt>Category Code</dt>
                                <dd><input type="text" class="form-control input-sm requiredAccountCategory" id="accountCategory_Code" /></dd>
