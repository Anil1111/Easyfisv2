<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="shortcut icon" href="~/Images/icon/easyfislogomini.ico">
    <title>BIR CAS - Disbursement Book</title>

    @Styles.Render("~/Content/Software-css")
</head>
<body>
    <div id="o-wrapper" class="o-wrapper">
        <main class="o-content">
            @Html.Partial("_SoftwareHeader")

            <section class="content-header">
                <div class="container">
                    <h3>
                        BIR CAS - Check/Cash Voucher Journal
                    </h3>
                    <p>
                        <i class="fa fa-building-o fa-fw"></i> &nbsp; @ViewData["Company"] <br />
                        <i class="fa fa-home fa-fw"></i> &nbsp; <a href="#" onclick="getUserBranches()" style="color: white;">@ViewData["Branch"]</a>
                    </p>
                </div>
            </section>
            <section class="container">
                <div class="panel panel-default">

                    <!--
                        ===============
                        Search / Filter
                        ===============
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 49.5%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                        <input type="text" class="form-control" id="inpBIRCASDisbursementBookSeacrhFilter" placeholder="Search...">
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><b><i class="fa fa-table"></i></b></span>
                                        <div class="comboBox-wide" id="cboBIRCASDisbursementBookDisplayNumberOfRows"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24.5%" align="right">
                                    <button class="btn btn-success" onclick="btnBIRCASDisbursementBookDownloadCSVOnclick()" id="btnBIRCASDisbursementBookDownloadCSV"><i class="fa fa-file-excel-o" aria-hidden="true"></i> CSV</button>
                                    <button class="btn btn-primary" onclick="btnBIRCASDisbursementBookPrintPDFOnclick()" id="btnBIRCASDisbursementBookPrintPDF"><i class="fa fa-file-pdf-o" aria-hidden="true"></i> PDF</button>
                                    <a href="/Software/BIRCASReports" class="btn btn-danger"><i class="fa fa-close"></i> Close</a>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        ======================================
                        Date Range, Company and Branch Filters
                        ======================================
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 24%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><b>Start Date</b></span>
                                        <div class="comboBox-wide" id="cboBIRCASDisbursementBookStartDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%" align="right">
                                    <div class="input-group">
                                        <span class="input-group-addon"><b>End Date</b></span>
                                        <div class="comboBox-wide" id="cboBIRCASDisbursementBookEndDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><b>Company</b></span>
                                        <div class="comboBox-wide" id="cboBIRCASDisbursementBookCompanyFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><b>Branch</b></span>
                                        <div class="comboBox-wide" id="cboBIRCASDisbursementBookBranchFilter"></div>
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        =========
                        Flex Grid
                        =========
                    -->
                    <div class="panel-body">
                        <div id="BIRCASDisbursementBookFlexGrid" class="grid"></div>
                    </div>

                    <!--
                        ==================
                        Navigation Buttons
                        ==================
                    -->
                    <div class="panel-footer">
                        <div class="row">
                            <div class="btn-group col-md-7" id="BIRCASDisbursementBookNavigationPageGrid">
                                <button type="button" class="btn btn-default border-custom" id="btnBIRCASDisbursementBookMoveToFirstPageGrid">
                                    <span class="glyphicon glyphicon-fast-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnBIRCASDisbursementBookMoveToPreviousPageGrid">
                                    <span class="glyphicon glyphicon-step-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" disabled style="width: 100px" id="btnBIRCASDisbursementBookCurrentPageGrid"></button>
                                <button type="button" class="btn btn-default border-custom" id="btnBIRCASDisbursementBookMoveToNextPageGrid">
                                    <span class="glyphicon glyphicon-step-forward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnBIRCASDisbursementBookMoveToLastPageGrid">
                                    <span class="glyphicon glyphicon-fast-forward"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <!--
                    ======
                    Totals
                    ======
                -->
                <div class="panel panel-default">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-6"></div>
                            <div class="col-md-6">
                                <dl class="dl-horizontal dl-custom">
                                    <dt>
                                        <b>Total Debit</b>
                                    </dt>
                                    <dd>
                                        <input type="text" class="form-control numberField" id="inpTotalDebitAmount" placeholder="0.00" disabled />
                                    </dd>
                                    <dt>
                                        <b>Total Credit</b>
                                    </dt>
                                    <dd>
                                        <input type="text" class="form-control numberField" id="inpTotalCreditAmount" placeholder="0.00" disabled />
                                    </dd>
                                    <dt>
                                        <b>Balance</b>
                                    </dt>
                                    <dd>
                                        <input type="text" class="form-control numberField" id="inpTotalBalanceAmount" placeholder="0.00" disabled />
                                    </dd>
                                </dl>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            @Html.Partial("_SoftwareFooter")
        </main>
    </div>
    <div id="c-mask" class="c-mask"></div>

    @Scripts.Render("~/Scripts/Software-js")
    <script type="text/javascript">
        // ==================
        // Data Range Filters
        // ==================
        var cboBIRCASDisbursementBookStartDateFilter = new wijmo.input.InputDate('#cboBIRCASDisbursementBookStartDateFilter');
        var cboBIRCASDisbursementBookEndDateFilter = new wijmo.input.InputDate('#cboBIRCASDisbursementBookEndDateFilter');
        var isCboBIRCASDisbursementBookStartDateFilterSelected = false;
        var isCboBIRCASDisbursementBookEndDateFilterSelected = false;

        // ==========================
        // Company and Branch Filters
        // ==========================
        var cboBIRCASDisbursementBookCompanyFilter = new wijmo.input.ComboBox('#cboBIRCASDisbursementBookCompanyFilter');
        var isCboBIRCASDisbursementBookCompanyFilterSelected = false;
        var cboBIRCASDisbursementBookBranchFilter = new wijmo.input.ComboBox('#cboBIRCASDisbursementBookBranchFilter');

        // =============================
        // Collection View and Flex Grid
        // =============================
        var BIRCASDisbursementBookCollectionView;
        var BIRCASDisbursementBookFlexGrid;
        var cboBIRCASDisbursementBookDisplayNumberOfRows = new wijmo.input.ComboBox('#cboBIRCASDisbursementBookDisplayNumberOfRows');

        // ==================
        // Navigation Buttons
        // ==================
        var btnBIRCASDisbursementBookMoveToFirstPageGrid = document.getElementById('btnBIRCASDisbursementBookMoveToFirstPageGrid');
        var btnBIRCASDisbursementBookMoveToPreviousPageGrid = document.getElementById('btnBIRCASDisbursementBookMoveToPreviousPageGrid');
        var btnBIRCASDisbursementBookMoveToNextPageGrid = document.getElementById('btnBIRCASDisbursementBookMoveToNextPageGrid');
        var btnBIRCASDisbursementBookMoveToLastPageGrid = document.getElementById('btnBIRCASDisbursementBookMoveToLastPageGrid');
        var btnBIRCASDisbursementBookCurrentPageGrid = document.getElementById('btnBIRCASDisbursementBookCurrentPageGrid');
        var BIRCASDisbursementBookCurrentPageIndex = 0;

        // =======================
        // Get URL Parameter Value
        // =======================
        function getURLParameterValue(name) {
            name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");

            var regexS = "[\\?&]" + name + "=([^&#]*)";
            var regex = new RegExp(regexS);

            var results = regex.exec(window.location.href);
            if (results == null) {
                return "";
            } else {
                return results[1];
            }
        }

        // ================================
        // Create Combo Date Ranged Filters
        // ================================
        function createCboDateRangedFilter() {
            NProgress.start();
            cboBIRCASDisbursementBookStartDateFilter.dispose();
            cboBIRCASDisbursementBookStartDateFilter = new wijmo.input.InputDate('#cboBIRCASDisbursementBookStartDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameterValue("StartDate")),
                onValueChanged: function () {
                    if (!isCboBIRCASDisbursementBookStartDateFilterSelected) {
                        NProgress.start();
                        refreshBIRCASDisbursementBookFlexGrid();
                    } else {
                        isCboBIRCASDisbursementBookStartDateFilterSelected = false;
                    }
                }
            });

            cboBIRCASDisbursementBookEndDateFilter.dispose();
            cboBIRCASDisbursementBookEndDateFilter = new wijmo.input.InputDate('#cboBIRCASDisbursementBookEndDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameterValue("EndDate")),
                onValueChanged: function () {
                    if (!isCboBIRCASDisbursementBookEndDateFilterSelected) {
                        NProgress.start();
                        refreshBIRCASDisbursementBookFlexGrid();
                    } else {
                        isCboBIRCASDisbursementBookEndDateFilterSelected = false;
                    }
                }
            });

            createCboCompanyFilter();
        }

        // ===========================
        // Create Combo Company Filter
        // ===========================
        function createCboCompanyFilter() {
            var companyObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/BIRCASDisbursementBook/dropdown/list/company',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            companyObservableArray.push({
                                Id: results[i]["Id"],
                                Company: results[i]["Company"]
                            });
                        }
                    }

                    var disabled = true;
                    if (results.length > 0) {
                        disabled = false;
                    } else {
                        $("#btnBIRCASDisbursementBookDownloadCSV").prop("disabled", true);
                        $("#btnBIRCASDisbursementBookPrintPDF").prop("disabled", true);
                    }

                    cboBIRCASDisbursementBookCompanyFilter.dispose();
                    cboBIRCASDisbursementBookCompanyFilter = new wijmo.input.ComboBox('#cboBIRCASDisbursementBookCompanyFilter', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: companyObservableArray,
                        displayMemberPath: "Company",
                        selectedValuePath: "Id",
                        selectedValue: getURLParameterValue("CompanyId"),
                        onSelectedIndexChanged: function () {
                            NProgress.start();
                            $("#btnBIRCASDisbursementBookDownloadCSV").prop("disabled", false);
                            $("#btnBIRCASDisbursementBookPrintPDF").prop("disabled", false);
                            isCboBIRCASDisbursementBookCompanyFilterSelected = true;
                            createCboBranchFilter();
                        }
                    });

                    createCboBranchFilter();
                }
            });
        }

        // ==========================
        // Create Combo Branch Filter
        // ==========================
        function createCboBranchFilter() {
            var branchObservableArray = new wijmo.collections.ObservableArray();

            if (cboBIRCASDisbursementBookCompanyFilter.selectedValue != null) {
                $.ajax({
                    url: '/api/BIRCASDisbursementBook/dropdown/list/branch/' + cboBIRCASDisbursementBookCompanyFilter.selectedValue,
                    cache: false,
                    type: 'GET',
                    contentType: 'application/json; charset=utf-8',
                    success: function (results) {
                        if (results.length > 0) {
                            branchObservableArray.push({
                                Id: 0,
                                Branch: "All Branches"
                            });

                            for (i = 0; i < results.length; i++) {
                                branchObservableArray.push({
                                    Id: results[i]["Id"],
                                    Branch: results[i]["Branch"]
                                });
                            }
                        }

                        var disabled = true;
                        if (results.length > 0) {
                            disabled = false;
                        } else {
                            $("#btnBIRCASDisbursementBookDownloadCSV").prop("disabled", true);
                            $("#btnBIRCASDisbursementBookPrintPDF").prop("disabled", true);
                        }

                        cboBIRCASDisbursementBookBranchFilter.dispose();
                        cboBIRCASDisbursementBookBranchFilter = new wijmo.input.ComboBox('#cboBIRCASDisbursementBookBranchFilter', {
                            disabled: disabled,
                            isEditable: false,
                            itemsSource: branchObservableArray,
                            displayMemberPath: "Branch",
                            selectedValuePath: "Id",
                            selectedValue: getURLParameterValue("BranchId"),
                            onSelectedIndexChanged: function () {
                                NProgress.start();
                                refreshBIRCASDisbursementBookFlexGrid();
                            }
                        });

                        createCboDisplayNumberOfRows();
                    }
                });
            }
        }

        // ===================================
        // Create Combo Display Number of Rows
        // ===================================
        function createCboDisplayNumberOfRows() {
            var rowsObservableArray = new wijmo.collections.ObservableArray();

            for (var i = 0; i <= 4; i++) {
                var rows = 0;
                var rowsString = "";

                if (i == 0) {
                    rows = 15;
                    rowsString = "Show 15 Rows";
                } else if (i == 1) {
                    rows = 50;
                    rowsString = "Show 50 Rows";
                } else if (i == 2) {
                    rows = 100;
                    rowsString = "Show 100 Rows";
                } else if (i == 3) {
                    rows = 150;
                    rowsString = "Show 150 Rows";
                } else {
                    rows = 200;
                    rowsString = "Show 200 Rows";
                }

                rowsObservableArray.push({
                    rowNumber: rows,
                    rowString: rowsString
                });
            }

            cboBIRCASDisbursementBookDisplayNumberOfRows.dispose();
            cboBIRCASDisbursementBookDisplayNumberOfRows = new wijmo.input.ComboBox('#cboBIRCASDisbursementBookDisplayNumberOfRows', {
                isEditable: false,
                itemsSource: rowsObservableArray,
                displayMemberPath: "rowString",
                selectedValuePath: "rowNumber",
                selectedValue: 50,
                onSelectedIndexChanged: function () {
                    NProgress.start();

                    setTimeout(function () {
                        var numberOfRows = cboBIRCASDisbursementBookDisplayNumberOfRows.selectedValue;
                        BIRCASDisbursementBookCollectionView.pageSize = parseInt(numberOfRows);
                        BIRCASDisbursementBookCollectionView.refresh();
                        BIRCASDisbursementBookFlexGrid.refresh();

                        NProgress.done();
                    }, 300);
                }
            });

            if (!isCboBIRCASDisbursementBookCompanyFilterSelected) {
                createBIRCASDisbursementBookFlexGrid();
            } else {
                refreshBIRCASDisbursementBookFlexGrid();
            }
        }

        // ========
        // Get Data
        // ========
        function getBIRCASDisbursementBookData() {
            var BIRCASDisbursementBookObservableArray = new wijmo.collections.ObservableArray;
            if (cboBIRCASDisbursementBookCompanyFilter.selectedValue != null) {
                if (cboBIRCASDisbursementBookBranchFilter.selectedValue != null) {
                    var startDate = cboBIRCASDisbursementBookStartDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var endDate = cboBIRCASDisbursementBookEndDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var companyId = cboBIRCASDisbursementBookCompanyFilter.selectedValue;
                    var branchId = cboBIRCASDisbursementBookBranchFilter.selectedValue;

                    $.ajax({
                        url: '/api/BIRCASDisbursementBook/list/' + startDate + '/' + endDate + '/' + + companyId + '/' + branchId,
                        cache: false,
                        type: 'GET',
                        contentType: 'application/json; charset=utf-8',
                        success: function (results) {

                            var totalDebit = 0;
                            var totalCredit = 0;
                            var totalBalance = 0;

                            if (results.length > 0) {
                                for (i = 0; i < results.length; i++) {
                                    var dataDateResult = new Date(results[i]["Date"]);
                                    var dataDate = ('0' + (dataDateResult.getMonth() + 1)).slice(-2) + '-' + ('0' + dataDateResult.getDate()).slice(-2) + '-' + dataDateResult.getFullYear();

                                    BIRCASDisbursementBookObservableArray.push({
                                        Date: dataDate,
                                        ReferenceNumber: results[i]["ReferenceNumber"],
                                        Supplier: results[i]["Supplier"],
                                        TIN: results[i]["TIN"],
                                        Address: results[i]["Address"],
                                        AccountCode: results[i]["AccountCode"],
                                        Account: results[i]["Account"],
                                        DebitAmount: results[i]["DebitAmount"],
                                        DebitAmountDisplay: parseFloat(results[i]["DebitAmount"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        CreditAmount: results[i]["CreditAmount"],
                                        CreditAmountDisplay: parseFloat(results[i]["CreditAmount"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        Balance: results[i]["DebitAmount"] - results[i]["CreditAmount"],
                                        BalanceDisplay: parseFloat(results[i]["DebitAmount"] - results[i]["CreditAmount"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                    });

                                    totalDebit += results[i]["DebitAmount"];
                                    totalCredit += results[i]["CreditAmount"];
                                    totalBalance += results[i]["DebitAmount"] - results[i]["CreditAmount"];
                                }
                            }

                            document.getElementById("inpTotalDebitAmount").value = parseFloat(totalDebit).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                            document.getElementById("inpTotalCreditAmount").value = parseFloat(totalCredit).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                            document.getElementById("inpTotalBalanceAmount").value = parseFloat(totalBalance).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                            NProgress.done();
                        }
                    });
                } else {
                    document.getElementById("inpTotalDebitAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalCreditAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalBalanceAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    NProgress.done();
                }
            } else {
                document.getElementById("inpTotalDebitAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                document.getElementById("inpTotalCreditAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                document.getElementById("inpTotalBalanceAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                NProgress.done();
            }

            return BIRCASDisbursementBookObservableArray;
        }

        // ================
        // Create Flex Grid
        // ================
        function createBIRCASDisbursementBookFlexGrid() {
            BIRCASDisbursementBookCollectionView = new wijmo.collections.CollectionView(getBIRCASDisbursementBookData());
            BIRCASDisbursementBookCollectionView.canFilter = true;
            BIRCASDisbursementBookCollectionView.pageSize = cboBIRCASDisbursementBookDisplayNumberOfRows.selectedValue;

            var filterText = '';
            $('#inpBIRCASDisbursementBookSeacrhFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                BIRCASDisbursementBookCollectionView.refresh();

                if (BIRCASDisbursementBookCollectionView.items.length > 0) {
                    var totalDebit = 0;
                    var totalCredit = 0;
                    var totalBalance = 0;

                    for (var p = 1; p <= BIRCASDisbursementBookCollectionView.pageCount; p++) {
                        for (i = 0; i < BIRCASDisbursementBookCollectionView.items.length; i++) {
                            totalDebit += BIRCASDisbursementBookCollectionView.items[i].DebitAmount;
                            totalCredit += BIRCASDisbursementBookCollectionView.items[i].CreditAmount;
                            totalBalance += BIRCASDisbursementBookCollectionView.items[i].Balance;
                        }

                        BIRCASDisbursementBookCollectionView.moveToNextPage();
                        if (p == BIRCASDisbursementBookCollectionView.pageCount) {
                            BIRCASDisbursementBookCollectionView.moveToPage(BIRCASDisbursementBookCurrentPageIndex - 1);
                        }
                    }

                    document.getElementById("inpTotalDebitAmount").value = parseFloat(totalDebit).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalCreditAmount").value = parseFloat(totalCredit).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalBalanceAmount").value = parseFloat(totalBalance).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                } else {
                    document.getElementById("inpTotalDebitAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalCreditAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalBalanceAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                }
            });

            BIRCASDisbursementBookCollectionView.filter = function (item) {
                return !filterText || (item.ReferenceNumber.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Supplier.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.TIN.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Address.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.AccountCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Account.toLowerCase().indexOf(filterText) > -1);
            }

            BIRCASDisbursementBookCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateBIRCASDisbursementBookNavigationButton();
            });

            BIRCASDisbursementBookFlexGrid = new wijmo.grid.FlexGrid('#BIRCASDisbursementBookFlexGrid');
            BIRCASDisbursementBookFlexGrid.initialize({
                columns: [
                    {
                        "header": "Date",
                        "binding": "Date",
                        "allowSorting": true,
                        "width": "1.5*"
                    },
                    {
                        "header": "Tx No.",
                        "binding": "ReferenceNumber",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Supplier",
                        "binding": "Supplier",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "TIN",
                        "binding": "TIN",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Address",
                        "binding": "Address",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Account Code",
                        "binding": "AccountCode",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Account",
                        "binding": "Account",
                        "allowSorting": true,
                        "width": "3*"
                    },
                    {
                        "header": "Debit Amount",
                        "binding": "DebitAmountDisplay",
                        "allowSorting": true,
                        "width": "2*",
                        "align": "right"
                    },
                    {
                        "header": "Credit Amount",
                        "binding": "CreditAmountDisplay",
                        "allowSorting": true,
                        "width": "2*",
                        "align": "right"
                    }
                ],
                autoGenerateColumns: false,
                itemsSource: BIRCASDisbursementBookCollectionView,
                isReadOnly: true,
                autoSizeMode: wijmo.grid.AutoSizeMode.Both,
                allowDragging: wijmo.grid.AllowDragging.None,
                selectionMode: wijmo.grid.SelectionMode.Row
            });

            BIRCASDisbursementBookFlexGrid.trackChanges = true;

            updateBIRCASDisbursementBookNavigationButton();

            btnBIRCASDisbursementBookMoveToFirstPageGrid.addEventListener('click', function () {
                BIRCASDisbursementBookCollectionView.moveToFirstPage();
                BIRCASDisbursementBookCurrentPageIndex = BIRCASDisbursementBookCollectionView.pageIndex + 1;
                updateBIRCASDisbursementBookNavigationButton();
            });

            btnBIRCASDisbursementBookMoveToPreviousPageGrid.addEventListener('click', function () {
                BIRCASDisbursementBookCollectionView.moveToPreviousPage();
                BIRCASDisbursementBookCurrentPageIndex = BIRCASDisbursementBookCollectionView.pageIndex + 1;
                updateBIRCASDisbursementBookNavigationButton();
            });

            btnBIRCASDisbursementBookMoveToNextPageGrid.addEventListener('click', function () {
                BIRCASDisbursementBookCollectionView.moveToNextPage();
                BIRCASDisbursementBookCurrentPageIndex = BIRCASDisbursementBookCollectionView.pageIndex + 1;
                updateBIRCASDisbursementBookNavigationButton();
            });

            btnBIRCASDisbursementBookMoveToLastPageGrid.addEventListener('click', function () {
                BIRCASDisbursementBookCollectionView.moveToLastPage();
                BIRCASDisbursementBookCurrentPageIndex = BIRCASDisbursementBookCollectionView.pageIndex + 1;
                updateBIRCASDisbursementBookNavigationButton();
            });
        }

        // =========================
        // Update Navigation Buttons
        // =========================
        function updateBIRCASDisbursementBookNavigationButton() {
            if (BIRCASDisbursementBookCollectionView.pageSize <= 0) {
                document.getElementById('BIRCASDisbursementBookNavigationPageGrid').style.display = 'none';
                return;
            }
            document.getElementById('BIRCASDisbursementBookNavigationPageGrid').style.display = 'block';
            if (BIRCASDisbursementBookCollectionView.pageIndex === 0) {
                btnBIRCASDisbursementBookMoveToFirstPageGrid.setAttribute('disabled', 'disabled');
                btnBIRCASDisbursementBookMoveToPreviousPageGrid.setAttribute('disabled', 'disabled');
                btnBIRCASDisbursementBookMoveToNextPageGrid.removeAttribute('disabled');
                btnBIRCASDisbursementBookMoveToLastPageGrid.removeAttribute('disabled');
            } else if (BIRCASDisbursementBookCollectionView.pageIndex === (BIRCASDisbursementBookCollectionView.pageCount - 1)) {
                btnBIRCASDisbursementBookMoveToFirstPageGrid.removeAttribute('disabled');
                btnBIRCASDisbursementBookMoveToPreviousPageGrid.removeAttribute('disabled');
                btnBIRCASDisbursementBookMoveToNextPageGrid.setAttribute('disabled', 'disabled');
                btnBIRCASDisbursementBookMoveToLastPageGrid.setAttribute('disabled', 'disabled');
            } else {
                btnBIRCASDisbursementBookMoveToFirstPageGrid.removeAttribute('disabled');
                btnBIRCASDisbursementBookMoveToPreviousPageGrid.removeAttribute('disabled');
                btnBIRCASDisbursementBookMoveToNextPageGrid.removeAttribute('disabled');
                btnBIRCASDisbursementBookMoveToLastPageGrid.removeAttribute('disabled');
            }
            btnBIRCASDisbursementBookCurrentPageGrid.innerHTML = (BIRCASDisbursementBookCollectionView.pageIndex + 1) + ' / ' + BIRCASDisbursementBookCollectionView.pageCount;
        }

        // =================
        // Refresh Flex Grid
        // =================
        function refreshBIRCASDisbursementBookFlexGrid() {
            BIRCASDisbursementBookCollectionView = new wijmo.collections.CollectionView(getBIRCASDisbursementBookData());
            BIRCASDisbursementBookCollectionView.canFilter = true;
            BIRCASDisbursementBookCollectionView.pageSize = cboBIRCASDisbursementBookDisplayNumberOfRows.selectedValue;

            var filterText = '';
            $('#inpBIRCASDisbursementBookSeacrhFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                BIRCASDisbursementBookCollectionView.refresh();

                if (BIRCASDisbursementBookCollectionView.items.length > 0) {
                    var totalDebit = 0;
                    var totalCredit = 0;
                    var totalBalance = 0;

                    for (var p = 1; p <= BIRCASDisbursementBookCollectionView.pageCount; p++) {
                        for (i = 0; i < BIRCASDisbursementBookCollectionView.items.length; i++) {
                            totalDebit += BIRCASDisbursementBookCollectionView.items[i].DebitAmount;
                            totalCredit += BIRCASDisbursementBookCollectionView.items[i].CreditAmount;
                            totalBalance += BIRCASDisbursementBookCollectionView.items[i].Balance;
                        }

                        BIRCASDisbursementBookCollectionView.moveToNextPage();
                        if (p == BIRCASDisbursementBookCollectionView.pageCount) {
                            BIRCASDisbursementBookCollectionView.moveToPage(BIRCASDisbursementBookCurrentPageIndex - 1);
                        }
                    }

                    document.getElementById("inpTotalDebitAmount").value = parseFloat(totalDebit).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalCreditAmount").value = parseFloat(totalCredit).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalBalanceAmount").value = parseFloat(totalBalance).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                } else {
                    document.getElementById("inpTotalDebitAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalCreditAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalBalanceAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                }
            });

            BIRCASDisbursementBookCollectionView.filter = function (item) {
                return !filterText || (item.ReferenceNumber.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Supplier.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.TIN.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Address.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.AccountCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Account.toLowerCase().indexOf(filterText) > -1);
            }

            BIRCASDisbursementBookCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateBIRCASDisbursementBookNavigationButton();
            });

            BIRCASDisbursementBookFlexGrid.itemsSource = BIRCASDisbursementBookCollectionView;
            BIRCASDisbursementBookFlexGrid.trackChanges = true;
        }

        // ============
        // Download CSV
        // ============
        function btnBIRCASDisbursementBookDownloadCSVOnclick() {
            var CSV = '';
            var BIRCASDisbursementBook = [];
            var fileName = 'BIRCASDisbursementBook.CSV';

            BIRCASDisbursementBookCollectionView.moveToFirstPage();

            for (var p = 1; p <= BIRCASDisbursementBookCollectionView.pageCount; p++) {
                for (i = 0; i < BIRCASDisbursementBookCollectionView.items.length; i++) {
                    BIRCASDisbursementBook.push({
                        Date: BIRCASDisbursementBookCollectionView.items[i].Date,
                        ReferenceNumber: BIRCASDisbursementBookCollectionView.items[i].ReferenceNumber,
                        Supplier: BIRCASDisbursementBookCollectionView.items[i].Supplier,
                        TIN: BIRCASDisbursementBookCollectionView.items[i].TIN,
                        Address: BIRCASDisbursementBookCollectionView.items[i].Address,
                        AccountCode: BIRCASDisbursementBookCollectionView.items[i].AccountCode,
                        Account: BIRCASDisbursementBookCollectionView.items[i].Account,
                        DebitAmount: BIRCASDisbursementBookCollectionView.items[i].DebitAmount,
                        CreditAmount: BIRCASDisbursementBookCollectionView.items[i].CreditAmount,
                    });
                }

                BIRCASDisbursementBookCollectionView.moveToNextPage();
                if (p == BIRCASDisbursementBookCollectionView.pageCount) {
                    BIRCASDisbursementBookCollectionView.moveToPage(BIRCASDisbursementBookCurrentPageIndex - 1);
                }
            }

            var Headers = [];
            Headers.push({
                Date: "Date",
                ReferenceNumber: "Tx No.",
                Supplier: "Supplier",
                TIN: "TIN",
                Address: "Address",
                AccountCode: "Account Code",
                Account: "Account",
                DebitAmount: "Debit Amount",
                CreditAmount: "Credit Amount"
            });

            var header = '';
            for (var b in Headers[0]) {
                header += '"' + Headers[0][b] + '",';
            }

            CSV = header + '\r\n';
            for (var i = 0; i < BIRCASDisbursementBook.length; i++) {
                var row = '';
                for (var a in BIRCASDisbursementBook[i]) {
                    row += '"' + BIRCASDisbursementBook[i][a] + '",';
                }

                row.slice(0, row.length - 1);
                CSV += row + '\r\n';
            }

            if (CSV == '') {
                toastr.error("No data.");
            } else {
                var link = document.createElement("a");
                if (link.download !== undefined) {
                    var blob = new Blob([CSV], { type: 'text/csv;charset=utf-8;' });
                    var url = URL.createObjectURL(blob);
                    link.setAttribute("href", url);
                    link.setAttribute("download", fileName);
                    link.style = "visibility:hidden";
                }

                if (navigator.msSaveBlob) {
                    link.addEventListener("click", function (event) {
                        var blob = new Blob([CSV], {
                            "type": "text/csv;charset=utf-8;"
                        });
                        navigator.msSaveBlob(blob, fileName);
                    }, false);
                }

                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);

                toastr.success("Dowload successful.");
            }
        }

        // =====================
        // Preview and Print PDF
        // =====================
        function btnBIRCASDisbursementBookPrintPDFOnclick() {
            if (cboBIRCASDisbursementBookCompanyFilter.selectedValue != null) {
                if (cboBIRCASDisbursementBookBranchFilter.selectedValue != null) {
