<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="shortcut icon" href="~/Images/icon/easyfislogomini.ico">
    <title>Disbursement Detail Report</title>

    @Styles.Render("~/Content/Software-css")
</head>
<body>
    <div id="o-wrapper" class="o-wrapper">
        <main class="o-content">
            @Html.Partial("_SoftwareHeader")

            <section class="content-header">
                <div class="container">
                    <h3>
                        Disbursement Detail Report
                    </h3>
                    <p>
                        <i class="fa fa-building-o fa-fw"></i> &nbsp; @ViewData["Company"] <br />
                        <i class="fa fa-home fa-fw"></i> &nbsp; <a href="#" onclick="getUserBranches()" style="color: white;">@ViewData["Branch"]</a>
                    </p>
                </div>
            </section>
            <section class="container">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 49.5%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                        <input type="text" class="form-control" id="disbursementDetailInputFilter" placeholder="Search...">
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><b><i class="fa fa-table"></i></b></span>
                                        <div class="comboBox-wide" id="cboDisbursementDetailReportShowNumberOfRows"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24.5%" align="right">
                                    <button onclick="btnDisbursementDetailReportCSVOnclick()" id="btnDisbursementDetailReportCSV" class="btn btn-success"><i class="fa fa-file-excel-o" aria-hidden="true"></i> CSV</button>
                                    <button onclick="btnDisbursementDetailReportPDFOnclick()" id="btnDisbursementDetailReportPDF" class="btn btn-primary"><i class="fa fa-print"></i> Print</button>
                                    <a href="/Software/AccountsPayable" class="btn btn-danger"><i class="fa fa-close"></i> Close</a>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Start Date</b></span>
                                        <div class="comboBox-wide" id="cboDisbursementDetailReportStartDate"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%" align="right">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>End Date</b></span>
                                        <div class="comboBox-wide" id="cboDisbursementDetailReportEndDate"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Company</b></span>
                                        <div class="comboBox-wide" id="cboDisbursementDetailReportCompany"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Branch</b></span>
                                        <div class="comboBox-wide" id="cboDisbursementDetailReportBranch"></div>
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div class="panel-body">
                        <div id="disbursementDetailReportGrid" style="font-size: 12px;"></div>
                    </div>
                    <div class="panel-footer">
                        <div class="row">
                            <div class="btn-group col-md-7" id="disbursementDetailReportNavigationPageGrid">
                                <button type="button" class="btn btn-default border-custom" id="btnDisbursementDetailReportMoveToFirstPageGrid">
                                    <span class="glyphicon glyphicon-fast-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnDisbursementDetailReportMoveToPreviousPageGrid">
                                    <span class="glyphicon glyphicon-step-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" disabled style="width: 100px" id="btnDisbursementDetailReportCurrentPageGrid"></button>
                                <button type="button" class="btn btn-default border-custom" id="btnDisbursementDetailReportMoveToNextPageGrid">
                                    <span class="glyphicon glyphicon-step-forward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnDisbursementDetailReportMoveToLastPageGrid">
                                    <span class="glyphicon glyphicon-fast-forward"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-6"></div>
                            <div class="col-md-6">
                                <dl class="dl-horizontal dl-custom">
                                    <dt>
                                        <b>Total Amount</b>
                                    </dt>
                                    <dd>
                                        <input type="text" class="form-control numberField" id="totalAmount" placeholder="0.00" disabled />
                                    </dd>
                                </dl>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            @Html.Partial("_SoftwareFooter")
        </main>
    </div>
    <div id="c-mask" class="c-mask"></div>

    @Scripts.Render("~/Scripts/Software-js")
    <script type="text/javascript">
        // ================
        // Global Variables
        // ================
        var cboDisbursementDetailReportStartDate = new wijmo.input.InputDate('#cboDisbursementDetailReportStartDate');
        var cboDisbursementDetailReportEndDate = new wijmo.input.InputDate('#cboDisbursementDetailReportEndDate');
        var isDisbursementDetailReportStartDateSelected = false;
        var isDisbursementDetailReportEndDateSelected = false;

        var cboDisbursementDetailReportCompany = new wijmo.input.ComboBox('#cboDisbursementDetailReportCompany');
        var cboDisbursementDetailReportBranch = new wijmo.input.ComboBox('#cboDisbursementDetailReportBranch');
        var isAPDisbursementDetailReportCompanySelected = false;

        var disbursementDetailReportCollections;
        var disbursementDetailReportGrid;

        var cboDisbursementDetailReportShowNumberOfRows = new wijmo.input.ComboBox('#cboDisbursementDetailReportShowNumberOfRows');

        var btnDisbursementDetailReportMoveToFirstPageGrid = document.getElementById('btnDisbursementDetailReportMoveToFirstPageGrid');
        var btnDisbursementDetailReportMoveToPreviousPageGrid = document.getElementById('btnDisbursementDetailReportMoveToPreviousPageGrid');
        var btnDisbursementDetailReportMoveToNextPageGrid = document.getElementById('btnDisbursementDetailReportMoveToNextPageGrid');
        var btnDisbursementDetailReportMoveToLastPageGrid = document.getElementById('btnDisbursementDetailReportMoveToLastPageGrid');
        var btnDisbursementDetailReportCurrentPageGrid = document.getElementById('btnDisbursementDetailReportCurrentPageGrid');
        var currentPageIndex = 0;

        // ===================
        // Get Parameter Value
        // ===================
        function getURLParameter(name) {
            name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");

            var regexS = "[\\?&]" + name + "=([^&#]*)";
            var regex = new RegExp(regexS);

            var results = regex.exec(window.location.href);
            if (results == null) {
                return "";
            } else {
                return results[1];
            }
        }

        // ============
        // Date Filters
        // ============
        function disbursementDetailReportDateFilters() {
            NProgress.start();

            cboDisbursementDetailReportStartDate.dispose();
            cboDisbursementDetailReportStartDate = new wijmo.input.InputDate('#cboDisbursementDetailReportStartDate', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameter("StartDate")),
                onValueChanged: function () {
                    if (!isDisbursementDetailReportStartDateSelected) {
                        NProgress.start();
                        refreshDisbursementDetailReportFlexGrid();
                    } else {
                        isDisbursementDetailReportStartDateSelected = false;
                    }
                }
            });

            cboDisbursementDetailReportEndDate.dispose();
            cboDisbursementDetailReportEndDate = new wijmo.input.InputDate('#cboDisbursementDetailReportEndDate', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameter("EndDate")),
                onValueChanged: function () {
                    if (!isDisbursementDetailReportEndDateSelected) {
                        NProgress.start();
                        refreshDisbursementDetailReportFlexGrid();
                    } else {
                        isDisbursementDetailReportEndDateSelected = false;
                    }
                }
            });

            createCboCompany();
        }

        // ==============
        // Company Filter
        // ==============
        function createCboCompany() {
            var companyObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/disbursementDetailReport/dropdown/list/company',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            companyObservableArray.push({
                                Id: results[i]["Id"],
                                Company: results[i]["Company"]
                            });
                        }
                    }

                    var disabled = true;
                    if (results.length > 0) {
                        disabled = false;
                    } else {
                        $("#btnDisbursementDetailReportCSV").prop("disabled", true);
                        $("#btnDisbursementDetailReportPDF").prop("disabled", true);
                    }

                    cboDisbursementDetailReportCompany.dispose();
                    cboDisbursementDetailReportCompany = new wijmo.input.ComboBox('#cboDisbursementDetailReportCompany', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: companyObservableArray,
                        displayMemberPath: "Company",
                        selectedValuePath: "Id",
                        selectedValue: getURLParameter("CompanyId"),
                        onSelectedIndexChanged: function () {
                            NProgress.start();
                            $("#btnDisbursementDetailReportCSV").prop("disabled", false);
                            $("#btnDisbursementDetailReportPDF").prop("disabled", false);
                            isAPDisbursementDetailReportCompanySelected = true;
                            createCboBranch();
                        }
                    });

                    createCboBranch();
                }
            });
        }

        // =============
        // Branch Filter
        // =============
        function createCboBranch() {
            var branchObservableArray = new wijmo.collections.ObservableArray();
            if (cboDisbursementDetailReportCompany.selectedValue != null) {
                $.ajax({
                    url: '/api/disbursementDetailReport/dropdown/list/branch/' + cboDisbursementDetailReportCompany.selectedValue,
                    cache: false,
                    type: 'GET',
                    contentType: 'application/json; charset=utf-8',
                    success: function (results) {
                        if (results.length > 0) {
                            for (i = 0; i < results.length; i++) {
                                branchObservableArray.push({
                                    Id: results[i]["Id"],
                                    Branch: results[i]["Branch"]
                                });
                            }
                        }

                        var disabled = true;
                        if (results.length > 0) {
                            disabled = false;
                        } else {
                            $("#btnDisbursementDetailReportCSV").prop("disabled", true);
                            $("#btnDisbursementDetailReportPDF").prop("disabled", true);
                        }

                        cboDisbursementDetailReportBranch.dispose();
                        cboDisbursementDetailReportBranch = new wijmo.input.ComboBox('#cboDisbursementDetailReportBranch', {
                            disabled: disabled,
                            isEditable: false,
                            itemsSource: branchObservableArray,
                            displayMemberPath: "Branch",
                            selectedValuePath: "Id",
                            selectedValue: getURLParameter("BranchId"),
                            onSelectedIndexChanged: function () {
                                NProgress.start();
                                refreshDisbursementDetailReportFlexGrid();
                            }
                        });

                        createCboShowRows();
                    }
                });
            }
        }

        // ===============
        // Show Rows Table
        // ===============
        function createCboShowRows() {
            var rowsObservableArray = new wijmo.collections.ObservableArray();

            for (var i = 0; i <= 4; i++) {
                var rows = 0;
                var rowsString = "";

                if (i == 0) {
                    rows = 15;
                    rowsString = "Show 15 Rows";
                } else if (i == 1) {
                    rows = 50;
                    rowsString = "Show 50 Rows";
                } else if (i == 2) {
                    rows = 100;
                    rowsString = "Show 100 Rows";
                } else if (i == 3) {
                    rows = 150;
                    rowsString = "Show 150 Rows";
                } else {
                    rows = 200;
                    rowsString = "Show 200 Rows";
                }

                rowsObservableArray.push({
                    rowNumber: rows,
                    rowString: rowsString
                });
            }

            cboDisbursementDetailReportShowNumberOfRows.dispose();
            cboDisbursementDetailReportShowNumberOfRows = new wijmo.input.ComboBox('#cboDisbursementDetailReportShowNumberOfRows', {
                isEditable: false,
                itemsSource: rowsObservableArray,
                displayMemberPath: "rowString",
                selectedValuePath: "rowNumber",
                selectedValue: 50,
                onSelectedIndexChanged: function () {
                    NProgress.start();

                    setTimeout(function () {
                        var numberOfRows = cboDisbursementDetailReportShowNumberOfRows.selectedValue;
                        disbursementDetailReportCollections.pageSize = parseInt(numberOfRows);
                        disbursementDetailReportCollections.refresh();
                        disbursementDetailReportGrid.refresh();

                        NProgress.done();
                    }, 300);
                }
            });

            if (!isAPDisbursementDetailReportCompanySelected) {
                createDisbursementDetailReportFlexGrid();
            } else {
                refreshDisbursementDetailReportFlexGrid();
            }
        }

        // ==================================
        // Get Disbursement Order Detail Data
        // ==================================
        function getDisbursementDetailReport() {
            var disbursementDetailReports = new wijmo.collections.ObservableArray;
            if (cboDisbursementDetailReportCompany.selectedValue != null) {
                if (cboDisbursementDetailReportBranch.selectedValue != null) {
                    var startDate = cboDisbursementDetailReportStartDate.value.toLocaleDateString().replace(/\//g, '-');
                    var endDate = cboDisbursementDetailReportEndDate.value.toLocaleDateString().replace(/\//g, '-');
                    var companyId = cboDisbursementDetailReportCompany.selectedValue;
                    var branchId = cboDisbursementDetailReportBranch.selectedValue;

                    $.ajax({
                        url: '/api/disbursementDetailReport/list/' + startDate + '/' + endDate + '/' + + companyId + '/' + branchId,
                        cache: false,
                        type: 'GET',
                        contentType: 'application/json; charset=utf-8',
                        success: function (results) {
                            var totalAmount = 0;
                            if (results.length > 0) {
                                for (i = 0; i < results.length; i++) {
                                    var docLink = "/Software/DisbursementDetail?id=" + results[i]["CVId"];

                                    var CVDateResult = new Date(results[i]["CVDate"])
                                    var CVDate = ('0' + (CVDateResult.getMonth() + 1)).slice(-2) + '-' + ('0' + CVDateResult.getDate()).slice(-2) + '-' + CVDateResult.getFullYear();
                                    
                                    disbursementDetailReports.push({
                                        BtnDetail: "<a href='" + docLink + "' class='btn btn-primary btn-xs btn-block' style='font-size: 9.5px;'><i class='fa fa-edit'></i> Detail",
                                        CVId: results[i]["CVId"],
                                        CVBranch: results[i]["CVBranch"],
                                        CVNumber: results[i]["CVNumber"],
                                        CVDate: CVDate,
                                        Payee: results[i]["Payee"],
                                        Branch: results[i]["Branch"],
                                        Account: results[i]["Account"],
                                        Article: results[i]["Article"],
                                        RRNumber: results[i]["RRNumber"],
                                        Amount: results[i]["Amount"],
                                        AmountDisplay: parseFloat(results[i]["Amount"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")
                                    });

                                    totalAmount += results[i]["Amount"];
                                }
                            }

                            document.getElementById("totalAmount").value = parseFloat(totalAmount).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                            NProgress.done();
                        }
                    });
                } else {
                    document.getElementById("totalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    NProgress.done();
                }
            } else {
                document.getElementById("totalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                NProgress.done();
            }

            return disbursementDetailReports;
        }

        // =========================
        // Update Navigation Buttons
        // =========================
        function updateNavigateButtonsItems() {
            if (disbursementDetailReportCollections.pageSize <= 0) {
                document.getElementById('disbursementDetailReportNavigationPageGrid').style.display = 'none';
                return;
            }
            document.getElementById('disbursementDetailReportNavigationPageGrid').style.display = 'block';
            if (disbursementDetailReportCollections.pageIndex === 0) {
                btnDisbursementDetailReportMoveToFirstPageGrid.setAttribute('disabled', 'disabled');
                btnDisbursementDetailReportMoveToPreviousPageGrid.setAttribute('disabled', 'disabled');
                btnDisbursementDetailReportMoveToNextPageGrid.removeAttribute('disabled');
                btnDisbursementDetailReportMoveToLastPageGrid.removeAttribute('disabled');
            } else if (disbursementDetailReportCollections.pageIndex === (disbursementDetailReportCollections.pageCount - 1)) {
                btnDisbursementDetailReportMoveToFirstPageGrid.removeAttribute('disabled');
                btnDisbursementDetailReportMoveToPreviousPageGrid.removeAttribute('disabled');
                btnDisbursementDetailReportMoveToLastPageGrid.setAttribute('disabled', 'disabled');
                btnDisbursementDetailReportMoveToNextPageGrid.setAttribute('disabled', 'disabled');
            } else {
                btnDisbursementDetailReportMoveToFirstPageGrid.removeAttribute('disabled');
                btnDisbursementDetailReportMoveToPreviousPageGrid.removeAttribute('disabled');
                btnDisbursementDetailReportMoveToNextPageGrid.removeAttribute('disabled');
                btnDisbursementDetailReportMoveToLastPageGrid.removeAttribute('disabled');
            }
            btnDisbursementDetailReportCurrentPageGrid.innerHTML = (disbursementDetailReportCollections.pageIndex + 1) + ' / ' + disbursementDetailReportCollections.pageCount;
        }

        // =================
        // Refresh Flex Grid
        // =================
        function refreshDisbursementDetailReportFlexGrid() {
            disbursementDetailReportCollections = new wijmo.collections.CollectionView(getDisbursementDetailReport());
            disbursementDetailReportCollections.canFilter = true;
            disbursementDetailReportCollections.pageSize = cboDisbursementDetailReportShowNumberOfRows.selectedValue;

            var filterText = '';
            $('#disbursementDetailInputFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                disbursementDetailReportCollections.refresh();
            });

            disbursementDetailReportCollections.filter = function (item) {
                return !filterText || (item.Branch.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.CVNumber.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Payee.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Article.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Account.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.RRNumber.toLowerCase().indexOf(filterText) > -1);
            }

            disbursementDetailReportCollections.collectionChanged.addHandler(function (sender, args) {
                updateNavigateButtonsItems();
            });

            disbursementDetailReportGrid.itemsSource = disbursementDetailReportCollections;
            disbursementDetailReportGrid.trackChanges = true;
        }

        // =========
        // Flex Grid
        // =========
        function createDisbursementDetailReportFlexGrid() {
            disbursementDetailReportCollections = new wijmo.collections.CollectionView(getDisbursementDetailReport());
            disbursementDetailReportCollections.canFilter = true;
            disbursementDetailReportCollections.pageSize = cboDisbursementDetailReportShowNumberOfRows.selectedValue;

            var filterText = '';
            $('#disbursementDetailInputFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                disbursementDetailReportCollections.refresh();
            });

            disbursementDetailReportCollections.filter = function (item) {
                return !filterText || (item.Branch.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.CVNumber.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Payee.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Article.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Account.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.RRNumber.toLowerCase().indexOf(filterText) > -1);
            }

            disbursementDetailReportCollections.collectionChanged.addHandler(function (sender, args) {
                updateNavigateButtonsItems();
            });

            disbursementDetailReportGrid = new wijmo.grid.FlexGrid('#disbursementDetailReportGrid');
            disbursementDetailReportGrid.initialize({
                columns: [
                    {
                        "header": "Detail",
                        "binding": "BtnDetail",
                        "width": 70,
                        "isContentHtml": true
                    },
                    {
                        "header": "CV No.",
                        "binding": "CVNumber",
                        "isContentHtml": true,
                        "allowSorting": true,
                        "width": "1.2*"
                    },
                    {
                        "header": "CV Date",
                        "binding": "CVDate",
                        "allowSorting": true,
                        "width": "1.2*"
                    },
                    {
                        "header": "Payee",
                        "binding": "Payee",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Branch",
                        "binding": "Branch",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Account",
                        "binding": "Account",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Article",
                        "binding": "Article",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "RR No.",
                        "binding": "RRNumber",
                        "allowSorting": true,
                        "width": "1.5*"
                    },
                    {
                        "header": "Amount",
                        "binding": "AmountDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1.5*"
                    }
                ],
                autoGenerateColumns: false,
                itemsSource: disbursementDetailReportCollections,
                isReadOnly: true,
                autoSizeMode: wijmo.grid.AutoSizeMode.Both,
                allowDragging: wijmo.grid.AllowDragging.None,
                selectionMode: wijmo.grid.SelectionMode.Row
            });

            disbursementDetailReportGrid.trackChanges = true;

            updateNavigateButtonsItems();

            btnDisbursementDetailReportMoveToFirstPageGrid.addEventListener('click', function () {
                disbursementDetailReportCollections.moveToFirstPage();
                currentPageIndex = disbursementDetailReportCollections.pageIndex + 1;
                updateNavigateButtonsItems();
            });

            btnDisbursementDetailReportMoveToPreviousPageGrid.addEventListener('click', function () {
                disbursementDetailReportCollections.moveToPreviousPage();
                currentPageIndex = disbursementDetailReportCollections.pageIndex + 1;
                updateNavigateButtonsItems();
            });

            btnDisbursementDetailReportMoveToNextPageGrid.addEventListener('click', function () {
                disbursementDetailReportCollections.moveToNextPage();
                currentPageIndex = disbursementDetailReportCollections.pageIndex + 1;
                updateNavigateButtonsItems();
            });

            btnDisbursementDetailReportMoveToLastPageGrid.addEventListener('click', function () {
                disbursementDetailReportCollections.moveToLastPage();
                currentPageIndex = disbursementDetailReportCollections.pageIndex + 1;
                updateNavigateButtonsItems();
            });
        }

        // ============
        // Download CSV
        // ============
        function btnDisbursementDetailReportCSVOnclick() {
            var CSV = '';
            var disbursementDetailReport = [];
            var fileName = 'DisbursementDetailReport.CSV';

            disbursementDetailReportCollections.moveToFirstPage();

            for (var p = 1; p <= disbursementDetailReportCollections.pageCount; p++) {
                for (i = 0; i < disbursementDetailReportCollections.items.length; i++) {

                    disbursementDetailReport.push({
                        CVBranch: disbursementDetailReportCollections.items[i].CVBranch,
                        CVNumber: "CV-" + disbursementDetailReportCollections.items[i].CVNumber,
                        CVDate: disbursementDetailReportCollections.items[i].CVDate,
                        Payee: disbursementDetailReportCollections.items[i].Payee,
                        Branch: disbursementDetailReportCollections.items[i].Branch,
                        Account: disbursementDetailReportCollections.items[i].Account,
                        Article: disbursementDetailReportCollections.items[i].Article,
                        RRNumber: "RR-" + disbursementDetailReportCollections.items[i].RRNumber,
                        Amount: disbursementDetailReportCollections.items[i].Amount,
                    });
                }

                disbursementDetailReportCollections.moveToNextPage();
                if (p == disbursementDetailReportCollections.pageCount) {
                    disbursementDetailReportCollections.moveToPage(currentPageIndex - 1);
                }
            }

            var Headers = [];
            Headers.push({
                CVBranch: "CV Branch",
                CVNumber: "CV Number",
                CVDate: "CV Date",
                Payee: "Payee",
                Branch: "Branch",
                Account: "Account",
                Article: "Article",
                RRNumber: "RR Number",
                Amount: "Amount"
            });

            var header = '';
            for (var b in Headers[0]) {
                header += '"' + Headers[0][b] + '",';
            }

            CSV = header + '\r\n';
            for (var i = 0; i < disbursementDetailReport.length; i++) {
                var row = '';
                for (var a in disbursementDetailReport[i]) {
                    row += '"' + disbursementDetailReport[i][a] + '",';
                }

                row.slice(0, row.length - 1);
                CSV += row + '\r\n';
            }

            if (CSV == '') {
                toastr.error("No data.");
            } else {
                var link = document.createElement("a");
                if (link.download !== undefined) {
                    var blob = new Blob([CSV], { type: 'text/csv;charset=utf-8;' });
                    var url = URL.createObjectURL(blob);
                    link.setAttribute("href", url);
                    link.setAttribute("download", fileName);
                    link.style = "visibility:hidden";
                }

                if (navigator.msSaveBlob) {
                    link.addEventListener("click", function (event) {
                        var blob = new Blob([CSV], {
                            "type": "text/csv;charset=utf-8;"
                        });
                        navigator.msSaveBlob(blob, fileName);
                    }, false);
                }

                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);

                toastr.success("Dowload successful.");
            }
        }

        // =====================
        // Preview and Print PDF
        // =====================
        function btnDisbursementDetailReportPDFOnclick() {
            if (cboDisbursementDetailReportCompany.selectedValue != null) {
                if (cboDisbursementDetailReportBranch.selectedValue != null) {
                    var startDate = cboDisbursementDetailReportStartDate.value.toLocaleDateString().replace(/\//g, '-');
                    var endDate = cboDisbursementDetailReportEndDate.value.toLocaleDateString().replace(/\//g, '-');
                    var companyId = cboDisbursementDetailReportCompany.selectedValue;
                    var branchId = cboDisbursementDetailReportBranch.selectedValue;

                    window.location.href = '/RepDisbursementDetailReport/DisbursementDetailReport?StartDate=' + startDate + '&EndDate=' + endDate + '&CompanyId=' + companyId + '&BranchId=' + branchId;
                } else {
                    toastr.error("Lacking Information");
                }
            } else {
                toastr.error("Lacking Information");
            }
        }

        // ============
        // On Load Page
        // ============
        $(document).ready(function () {
            disbursementDetailReportDateFilters();
        });
    </script>
    <script type="text/javascript">
        var cboUserBranches = new wijmo.input.ComboBox('#cboUserBranches');
        function getUserBranches() {
            $("#loadingUserBranches").show();
            document.getElementById("loadingUserBranches").innerHTML = '<i class="fa fa-spinner fa-spin"></i>';
            $("#userBranchContent").hide();

            $('#userBranchesModal').modal({ show: true, backdrop: 'static' });

            document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
            $("#updateUserBranch").prop("disabled", false);
            $("#closeUserBranch").prop("disabled", false);

            var userBranchesObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/userBranch/list',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (userBranchesResults) {
                    if (userBranchesResults.length > 0) {
                        for (i = 0; i < userBranchesResults.length; i++) {
                            userBranchesObservableArray.push({
                                BranchId: userBranchesResults[i]["BranchId"],
                                Branch: userBranchesResults[i]["Branch"]
                            });
                        }
                    }

                    var currentBranch = "@ViewData["BranchId"]";
                    cboUserBranches.dispose();
                    cboUserBranches = new wijmo.input.ComboBox('#cboUserBranches', {
                        isEditable: false,
                        placeholder: "Select Branches",
                        itemsSource: userBranchesObservableArray,
                        displayMemberPath: "Branch",
                        selectedValuePath: "BranchId",
                        selectedValue: currentBranch
                    });

                    setTimeout(function () {
                        $("#loadingUserBranches").hide();
                        $("#userBranchContent").show();
                    }, 500)
                }
            });
        }

        function updateUserBranchOnclick() {
            document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-spinner fa-spin'></i> Updating";
            $("#updateUserBranch").prop("disabled", true);
            $("#closeUserBranch").prop("disabled", true);

            var userBranchObject = new Object();
            userBranchObject.BranchId = cboUserBranches.selectedValue;
            var userBranchData = JSON.stringify(userBranchObject);

            $.ajax({
                type: "PUT",
                url: '/api/userBranch/update',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: userBranchData,
                statusCode: {
                    200: function () {
                        toastr.success("Update Successful");
                        window.setTimeout(function () {
                            location.reload()
                        }, 1000);
                    },
                    404: function () {
                        toastr.error("Not Found");
                        document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
                        $("#updateUserBranch").prop("disabled", false);
                        $("#closeUserBranch").prop("disabled", false);
                    },
                    500: function () {
                        toastr.error("Bad Request");
                        document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
                        $("#updateUserBranch").prop("disabled", false);
                        $("#closeUserBranch").prop("disabled", false);
                    }
                }
            });
        }
    </script>
