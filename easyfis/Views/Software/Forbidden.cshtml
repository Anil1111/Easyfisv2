<!DOCTYPE html>
<html>
<head>
    <!-- Meta -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <!-- Title of the Page -->
    <link rel="shortcut icon" href="~/Images/icon/easyfislogomini.ico">
    <title>Easyfis - Dashboard</title>

    <!-- CSS StyleSheets-->
    @Styles.Render("~/Content/Software-css")
</head>
<body class="hold-transition skin-blue fixed  sidebar-mini">
    @* add class 'sidebar-collapse' when you want to collapse the sidebar menu *@
    <div class="wrapper">
        @Html.Partial("_SoftwareHeader")
        @Html.Partial("_SoftwareSideBarMenu")

        <div class="content-wrapper">
            <section class="content">
                <div class="box box-default" style="background-color: #1e1e1e">
                    <div class="box-body">
                        <br /><br />
                        <center>
                            <i class="fa fa-exclamation-triangle" style="font-size: 200px; color: #fffa00"></i>
                            <h1 style="color: #fff">Error 403 - Forbidden</h1>
                            <br />
                            <p style="color: #fff">Sorry, you have no rights to access this page.</p>
                            <br /><br />
                            <button class="btn btn-primary btn-lg" onclick="btnForbiddenOkayOnClick()"> Okay </button>
                        </center>
                        <br /><br />
                    </div>
                </div>
            </section>
        </div>

        @Html.Partial("_SoftwareFooter")
    </div>
    @Scripts.Render("~/Scripts/Software-js")
    <script type="text/javascript">
        function btnForbiddenOkayOnClick() {
            window.history.back();
        }
    </script>
</body>
</html>