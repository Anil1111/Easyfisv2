        // computation of all sale item discount, vat, netprice, and final amount
        function computation() {
            var DiscountIsInclusive = document.getElementById('saleItem_DiscontInclusive').value;
            var quantity = document.getElementById('saleItem_Quantity').value.replace(/\,/g, '');
            var priceExVATPrice = document.getElementById('saleItem_PriceExVAT').value.replace(/\,/g, '');
            var discountRate = document.getElementById('saleItem_DiscountRate').value.replace(/\,/g, '');
            var VATPercentage = document.getElementById('saleItem_VATRate').value.replace(/\,/g, '');

            //var discountAmount = document.getElementById('saleItem_DiscountAmount').value.replace(/\,/g, '');

            var discountAmount = 0;
            var VATAmount = 0;
            var netPrice = 0;

            // compute net price
            //if (document.getElementById('saleItem_DiscontInclusive').value == 'false') {
            //var price = priceExVATPrice / (1 + (VATPercentage / 100));
            discountAmount = priceExVATPrice * (discountRate / 100);
            netPrice = priceExVATPrice - discountAmount;
            //} else {
            //discountAmount = priceExVATPrice * (discountRate / 100);

            //netPrice = priceExVATPrice - discountAmount;
            //netPrice = priceExVATPrice - (discountAmount / quantity);
            //}

            var amount = netPrice * quantity;
            //var amount = (priceExVATPrice * quantity) - discountAmount;

            // compute sales amount
            if (document.getElementById('saleItem_TAXInclusive').value == 'true') {
                VATAmount = amount / (1 + (VATPercentage / 100)) * (VATPercentage / 100);
            } else {
                VATAmount = amount * (VATPercentage / 100);
            }

            //document.getElementById('saleItem_NetPrice').value = netPrice.toLocaleString();
            //document.getElementById('saleItem_Amount').value = amount.toLocaleString();
            //document.getElementById('saleItem_VATAmount').value = VATAmount.toLocaleString();

            document.getElementById('saleItem_NetPrice').value = netPrice.toLocaleString();
            document.getElementById('saleItem_Amount').value = amount.toLocaleString();
            document.getElementById('saleItem_VATAmount').value = VATAmount.toLocaleString();
        }

