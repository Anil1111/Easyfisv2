                        }

                        var disabled = true;
                        if (results.length > 0) {
                            disabled = false;
                        } else {
                            $("#cmdSaveStockInItemButton").prop("disabled", true);
                        }

                        cboINItemUnit.dispose();
                        cboINItemUnit = new wijmo.input.ComboBox('#cboINItemUnit', {
                            disabled: disabled,
                            isEditable: false,
                            itemsSource: stockInUnits,
                            displayMemberPath: "Unit",
