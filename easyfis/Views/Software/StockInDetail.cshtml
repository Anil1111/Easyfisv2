                    $.ajax({
                        type: "PUT",
                        url: '/api/updateStockInItem/' + stockInItemId,
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        data: stockInItemData,
                        statusCode: {
                            200: function () {
                                $("#cmdSaveStockInItemButton").prop("disabled", true);
                                $("#cmdStockInItemDetailCloseButton").prop("disabled", true);
