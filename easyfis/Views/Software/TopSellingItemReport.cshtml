<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="shortcut icon" href="~/Images/icon/easyfislogomini.ico">
    <title>Top Selling Item Report</title>

    @Styles.Render("~/Content/Software-css")
</head>
<body>
    <div id="o-wrapper" class="o-wrapper">
        <main class="o-content">
            @Html.Partial("_SoftwareHeader")

            <section class="content-header">
                <div class="container">
                    <h3>
                        Top Selling Item Report
                    </h3>
                    <p>
                        <i class="fa fa-building-o fa-fw"></i> &nbsp; @ViewData["Company"] <br />
                        <i class="fa fa-home fa-fw"></i> &nbsp; <a href="#" onclick="getUserBranches()" style="color: white;">@ViewData["Branch"]</a>
                    </p>
                </div>
            </section>
            <section class="container">
                <div class="panel panel-default">

                    <!--
                        ===============
                        Search / Filter
                        ===============
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 49.5%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                        <input type="text" class="form-control" id="inpTopSellingItemsReportFilter" placeholder="Search...">
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><b><i class="fa fa-table"></i></b></span>
                                        <div class="comboBox-wide" id="cboTopSellingItemsReportShowNumberOfRows"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24.5%" align="right">
                                    <button onclick="btnTopSellingItemsReportDownloadCSVOnclick()" id="btnTopSellingItemsReportDownloadCSV" class="btn btn-success"><i class="fa fa-file-excel-o" aria-hidden="true"></i> CSV</button>
                                    <button onclick="btnTopSellingItemsReportPDFOnclick()" id="btnTopSellingItemsReportPDF" class="btn btn-primary"><i class="fa fa-print"></i> Print</button>
                                    <a href="/Software/AccountsReceivable" class="btn btn-danger"><i class="fa fa-close"></i> Close</a>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        ======================================
                        Date Range, Company and Branch Filters
                        ======================================
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Start Date</b></span>
                                        <div class="comboBox-wide" id="cboTopSellingItemsReportStartDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%" align="right">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>End Date</b></span>
                                        <div class="comboBox-wide" id="cboTopSellingItemsReportEndDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Company</b></span>
                                        <div class="comboBox-wide" id="cboTopSellingItemsReportCompanyFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Branch</b></span>
                                        <div class="comboBox-wide" id="cboTopSellingItemsReportBranchFilter"></div>
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        =========
                        Flex Grid
                        =========
                    -->
                    <div class="panel-body">
                        <div id="topSellingItemsReportFlexGrid"></div>
                    </div>

                    <!--
                        ==================
                        Navigation Buttons
                        ==================
                    -->
                    <div class="panel-footer">
                        <div class="row">
                            <div class="btn-group col-md-7" id="topSellingItemsReportNavigationPageGrid">
                                <button type="button" class="btn btn-default border-custom" id="btnTopSellingItemsReportMoveToFirstPageGrid">
                                    <span class="glyphicon glyphicon-fast-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnTopSellingItemsReportMoveToPreviousPageGrid">
                                    <span class="glyphicon glyphicon-step-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" disabled style="width: 100px" id="btnTopSellingItemsReportCurrentPageGrid"></button>
                                <button type="button" class="btn btn-default border-custom" id="btnTopSellingItemsReportMoveToNextPageGrid">
                                    <span class="glyphicon glyphicon-step-forward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnTopSellingItemsReportMoveToLastPageGrid">
                                    <span class="glyphicon glyphicon-fast-forward"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <!--
                    ======
                    Totals
                    ======
                -->
                <div class="panel panel-default">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-6"></div>
                            <div class="col-md-6">
                                <dl class="dl-horizontal dl-custom">
                                    <dt>
                                        <b>Total Amount</b>
                                    </dt>
                                    <dd>
                                        <input type="text" class="form-control numberField" id="totalAmount" placeholder="0.00" disabled />
                                    </dd>
                                </dl>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            @Html.Partial("_SoftwareFooter")
        </main>
    </div>
    <div id="c-mask" class="c-mask"></div>

    @Scripts.Render("~/Scripts/Software-js")
    <script type="text/javascript">
        // ============
        // Date Filters
        // ============
        var cboTopSellingItemsReportStartDateFilter = new wijmo.input.InputDate('#cboTopSellingItemsReportStartDateFilter');
        var cboTopSellingItemsReportEndDateFilter = new wijmo.input.InputDate('#cboTopSellingItemsReportEndDateFilter');
        var isTopSellingItemsReportStartDateSelected = false;
        var isTopSellingItemsReportEndDateSelected = false;

        // =========================
        // Company and Branch Filter
        // =========================
        var cboTopSellingItemsReportCompanyFilter = new wijmo.input.ComboBox('#cboTopSellingItemsReportCompanyFilter');
        var cboTopSellingItemsReportBranchFilter = new wijmo.input.ComboBox('#cboTopSellingItemsReportBranchFilter');
        var isCboTopSellingItemsReportCompanySelected = false;

        // =============================
        // Collection View and Flex Grid
        // =============================
        var topSellingItemsReportCollectionView;
        var topSellingItemsReportFlexGrid;
        var cboTopSellingItemsReportShowNumberOfRows = new wijmo.input.ComboBox('#cboTopSellingItemsReportShowNumberOfRows');

        // ==================
        // Navigation Buttons
        // ==================
        var btnTopSellingItemsReportMoveToFirstPageGrid = document.getElementById('btnTopSellingItemsReportMoveToFirstPageGrid');
        var btnTopSellingItemsReportMoveToPreviousPageGrid = document.getElementById('btnTopSellingItemsReportMoveToPreviousPageGrid');
        var btnTopSellingItemsReportMoveToNextPageGrid = document.getElementById('btnTopSellingItemsReportMoveToNextPageGrid');
        var btnTopSellingItemsReportMoveToLastPageGrid = document.getElementById('btnTopSellingItemsReportMoveToLastPageGrid');
        var btnTopSellingItemsReportCurrentPageGrid = document.getElementById('btnTopSellingItemsReportCurrentPageGrid');
        var currentPageIndex = 0;

        // =======================
        // Get URL Parameter Value
        // =======================
        function getURLParameterValue(name) {
            name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");

            var regexS = "[\\?&]" + name + "=([^&#]*)";
            var regex = new RegExp(regexS);

            var results = regex.exec(window.location.href);
            if (results == null) {
                return "";
            } else {
                return results[1];
            }
        }

        // ================================
        // Create Combo Date Ranged Filters
        // ================================
        function createCboDateRangeFilters() {
            NProgress.start();

            cboTopSellingItemsReportStartDateFilter.dispose();
            cboTopSellingItemsReportStartDateFilter = new wijmo.input.InputDate('#cboTopSellingItemsReportStartDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameterValue("StartDate")),
                onValueChanged: function () {
                    if (!isTopSellingItemsReportStartDateSelected) {
                        NProgress.start();
                        refreshTopSellingItemsReportFlexGrid();
                    } else {
                        isTopSellingItemsReportStartDateSelected = false;
                    }
                }
            });

            cboTopSellingItemsReportEndDateFilter.dispose();
            cboTopSellingItemsReportEndDateFilter = new wijmo.input.InputDate('#cboTopSellingItemsReportEndDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameterValue("EndDate")),
                onValueChanged: function () {
                    if (!isTopSellingItemsReportEndDateSelected) {
                        NProgress.start();
                        refreshTopSellingItemsReportFlexGrid();
                    } else {
                        isTopSellingItemsReportEndDateSelected = false;
                    }
                }
            });

            createCboCompanyFilter();
        }

        // ===========================
        // Create Combo Company Filter
        // ===========================
        function createCboCompanyFilter() {
            var companyObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/topSellingItemsReport/dropdown/list/company',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            companyObservableArray.push({
                                Id: results[i]["Id"],
                                Company: results[i]["Company"]
                            });
                        }
                    }

                    var disabled = true;
                    if (results.length > 0) {
                        disabled = false;
                    } else {
                        $("#btnTopSellingItemsReportDownloadCSV").prop("disabled", true);
                        $("#btnTopSellingItemsReportPDF").prop("disabled", true);
                    }

                    cboTopSellingItemsReportCompanyFilter.dispose();
                    cboTopSellingItemsReportCompanyFilter = new wijmo.input.ComboBox('#cboTopSellingItemsReportCompanyFilter', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: companyObservableArray,
                        displayMemberPath: "Company",
                        selectedValuePath: "Id",
                        selectedValue: getURLParameterValue("CompanyId"),
                        onSelectedIndexChanged: function () {
                            NProgress.start();
                            $("#btnTopSellingItemsReportDownloadCSV").prop("disabled", false);
                            $("#btnTopSellingItemsReportPDF").prop("disabled", false);
                            isCboTopSellingItemsReportCompanySelected = true;
                            createCboBranchFilter();
                        }
                    });

                    createCboBranchFilter();
                }
            });
        }

        // ==========================
        // Create Combo Branch Filter
        // ==========================
        function createCboBranchFilter() {
            var branchObservableArray = new wijmo.collections.ObservableArray();
            if (cboTopSellingItemsReportCompanyFilter.selectedValue != null) {
                $.ajax({
                    url: '/api/topSellingItemsReport/dropdown/list/branch/' + cboTopSellingItemsReportCompanyFilter.selectedValue,
                    cache: false,
                    type: 'GET',
                    contentType: 'application/json; charset=utf-8',
                    success: function (branchResults) {
                        if (branchResults.length > 0) {
                            for (i = 0; i < branchResults.length; i++) {
                                branchObservableArray.push({
                                    Id: branchResults[i]["Id"],
                                    Branch: branchResults[i]["Branch"]
                                });
                            }
                        }

                        var disabled = true;
                        if (branchResults.length > 0) {
                            disabled = false;
                        } else {
                            $("#btnTopSellingItemsReportDownloadCSV").prop("disabled", true);
                            $("#btnTopSellingItemsReportPDF").prop("disabled", true);
                        }

                        cboTopSellingItemsReportBranchFilter.dispose();
                        cboTopSellingItemsReportBranchFilter = new wijmo.input.ComboBox('#cboTopSellingItemsReportBranchFilter', {
                            disabled: disabled,
                            isEditable: false,
                            itemsSource: branchObservableArray,
                            displayMemberPath: "Branch",
                            selectedValuePath: "Id",
                            selectedValue: getURLParameterValue("BranchId"),
                            onSelectedIndexChanged: function () {
                                NProgress.start();
                                refreshTopSellingItemsReportFlexGrid();
                            }
                        });

                        createCboShowRows();
                    }
                });
            }
        }

        // ===================================
        // Create Combo Display Number of Rows
        // ===================================
        function createCboShowRows() {
            var rowsObservableArray = new wijmo.collections.ObservableArray();

            for (var i = 0; i <= 4; i++) {
                var rows = 0;
                var rowsString = "";

                if (i == 0) {
                    rows = 15;
                    rowsString = "Show 15 Rows";
                } else if (i == 1) {
                    rows = 50;
                    rowsString = "Show 50 Rows";
                } else if (i == 2) {
                    rows = 100;
                    rowsString = "Show 100 Rows";
                } else if (i == 3) {
                    rows = 150;
                    rowsString = "Show 150 Rows";
                } else {
                    rows = 200;
                    rowsString = "Show 200 Rows";
                }

                rowsObservableArray.push({
                    rowNumber: rows,
                    rowString: rowsString
                });
            }

            cboTopSellingItemsReportShowNumberOfRows.dispose();
            cboTopSellingItemsReportShowNumberOfRows = new wijmo.input.ComboBox('#cboTopSellingItemsReportShowNumberOfRows', {
                isEditable: false,
                itemsSource: rowsObservableArray,
                displayMemberPath: "rowString",
                selectedValuePath: "rowNumber",
                selectedValue: 50,
                onSelectedIndexChanged: function () {
                    NProgress.start();

                    setTimeout(function () {
                        var numberOfRows = cboTopSellingItemsReportShowNumberOfRows.selectedValue;
                        topSellingItemsReportCollectionView.pageSize = parseInt(numberOfRows);
                        topSellingItemsReportCollectionView.refresh();
                        topSellingItemsReportFlexGrid.refresh();

                        NProgress.done();
                    }, 300);
                }
            });

            if (!isCboTopSellingItemsReportCompanySelected) {
                createTopSellingItemsReportFlexGrid();
            } else {
                refreshTopSellingItemsReportFlexGrid();
            }
        }

        // ===========================
        // Get Top Selling Report Data
        // ===========================
        function getTopSellingItemsReportData() {
            var topSellingItemsReports = new wijmo.collections.ObservableArray;
            if (cboTopSellingItemsReportCompanyFilter.selectedValue != null) {
                if (cboTopSellingItemsReportBranchFilter.selectedValue != null) {

                    var startDate = cboTopSellingItemsReportStartDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var endDate = cboTopSellingItemsReportEndDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var companyId = cboTopSellingItemsReportCompanyFilter.selectedValue;
                    var branchId = cboTopSellingItemsReportBranchFilter.selectedValue;

                    $.ajax({
                        url: '/api/topSellingItemsReport/list/' + startDate + '/' + endDate + '/' + + companyId + '/' + branchId,
                        cache: false,
                        type: 'GET',
                        contentType: 'application/json; charset=utf-8',
                        success: function (salesDetailReportResult) {
                            var totalAmount = 0;
                            if (salesDetailReportResult.length > 0) {
                                for (i = 0; i < salesDetailReportResult.length; i++) {
                                    var docLink = "/Software/ItemDetail?id=" + salesDetailReportResult[i]["ItemId"];

                                    topSellingItemsReports.push({
                                        Detail: "<a href='" + docLink + "' class='btn btn-primary btn-xs btn-block'><i class='fa fa-edit'></i> Item Detail",
                                        Branch: salesDetailReportResult[i]["Branch"],
                                        Item: salesDetailReportResult[i]["Item"],
                                        Unit: salesDetailReportResult[i]["Unit"],
                                        Quantity: salesDetailReportResult[i]["Quantity"],
                                        QuantityDisplay: parseFloat(salesDetailReportResult[i]["Quantity"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        Price: salesDetailReportResult[i]["Price"],
                                        PriceDisplay: parseFloat(salesDetailReportResult[i]["Price"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        Amount: salesDetailReportResult[i]["Amount"],
                                        AmountDisplay: parseFloat(salesDetailReportResult[i]["Amount"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                    });

                                    totalAmount += salesDetailReportResult[i]["Amount"];
                                }
                            }

                            document.getElementById("totalAmount").value = parseFloat(totalAmount).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                            NProgress.done();
                        }
                    });

                } else {
                    document.getElementById("totalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    NProgress.done();
                }
            } else {
                document.getElementById("totalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                NProgress.done();
            }

            return topSellingItemsReports;
        }

        // ===================================
        // Create Top Selling Report Flex Grid
        // ===================================
        function createTopSellingItemsReportFlexGrid() {
            topSellingItemsReportCollectionView = new wijmo.collections.CollectionView(getTopSellingItemsReportData());
            topSellingItemsReportCollectionView.canFilter = true;
            topSellingItemsReportCollectionView.pageSize = cboTopSellingItemsReportShowNumberOfRows.selectedValue;

            var filterText = '';
            $('#inpTopSellingItemsReportFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                topSellingItemsReportCollectionView.refresh();
            });

            topSellingItemsReportCollectionView.filter = function (item) {
                return !filterText || (item.Item.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.BaseUnit.toLowerCase().indexOf(filterText) > -1);
            }

            topSellingItemsReportCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateTopSellingItemsReportNavigationButtons();
            });

            topSellingItemsReportFlexGrid = new wijmo.grid.FlexGrid('#topSellingItemsReportFlexGrid');
            topSellingItemsReportFlexGrid.initialize({
                columns: [
                    {
                        "header": "Detail",
                        "binding": "Detail",
                        "isContentHtml": true,
                        "allowSorting": true,
                        "width": "0.7*"
                    },
                    {
                        "header": "Item",
                        "binding": "Item",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Price",
                        "binding": "PriceDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1*"
                    },
                    {
                        "header": "Unit",
                        "binding": "Unit",
                        "allowSorting": true,
                        "width": "1*"
                    },
                    {
                        "header": "Quantity",
                        "binding": "QuantityDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1*"
                    },
                    {
                        "header": "Amount",
                        "binding": "AmountDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1*"
                    }
                ],
                autoGenerateColumns: false,
                itemsSource: topSellingItemsReportCollectionView,
                isReadOnly: true,
                autoSizeMode: wijmo.grid.AutoSizeMode.Both,
                allowDragging: wijmo.grid.AllowDragging.None,
                selectionMode: wijmo.grid.SelectionMode.Row
            });

            topSellingItemsReportFlexGrid.trackChanges = true;

            updateTopSellingItemsReportNavigationButtons();

            btnTopSellingItemsReportMoveToFirstPageGrid.addEventListener('click', function () {
                topSellingItemsReportCollectionView.moveToFirstPage();
                currentPageIndex = topSellingItemsReportCollectionView.pageIndex + 1;
                updateTopSellingItemsReportNavigationButtons();
            });

            btnTopSellingItemsReportMoveToPreviousPageGrid.addEventListener('click', function () {
                topSellingItemsReportCollectionView.moveToPreviousPage();
                currentPageIndex = topSellingItemsReportCollectionView.pageIndex + 1;
                updateTopSellingItemsReportNavigationButtons();
            });

            btnTopSellingItemsReportMoveToNextPageGrid.addEventListener('click', function () {
                topSellingItemsReportCollectionView.moveToNextPage();
                currentPageIndex = topSellingItemsReportCollectionView.pageIndex + 1;
                updateTopSellingItemsReportNavigationButtons();
            });

            btnTopSellingItemsReportMoveToLastPageGrid.addEventListener('click', function () {
                topSellingItemsReportCollectionView.moveToLastPage();
                currentPageIndex = topSellingItemsReportCollectionView.pageIndex + 1;
                updateTopSellingItemsReportNavigationButtons();
            });
        }

        // ============================================
        // Update Top Selling Report Navigation Buttons
        // ============================================
        function updateTopSellingItemsReportNavigationButtons() {
            if (topSellingItemsReportCollectionView.pageSize <= 0) {
                document.getElementById('topSellingItemsReportNavigationPageGrid').style.display = 'none';
                return;
            }
            document.getElementById('topSellingItemsReportNavigationPageGrid').style.display = 'block';
            if (topSellingItemsReportCollectionView.pageIndex === 0) {
                btnTopSellingItemsReportMoveToFirstPageGrid.setAttribute('disabled', 'disabled');
                btnTopSellingItemsReportMoveToPreviousPageGrid.setAttribute('disabled', 'disabled');
                btnTopSellingItemsReportMoveToNextPageGrid.removeAttribute('disabled');
                btnTopSellingItemsReportMoveToLastPageGrid.removeAttribute('disabled');
            } else if (topSellingItemsReportCollectionView.pageIndex === (topSellingItemsReportCollectionView.pageCount - 1)) {
                btnTopSellingItemsReportMoveToFirstPageGrid.removeAttribute('disabled');
                btnTopSellingItemsReportMoveToPreviousPageGrid.removeAttribute('disabled');
                btnTopSellingItemsReportMoveToLastPageGrid.setAttribute('disabled', 'disabled');
                btnTopSellingItemsReportMoveToNextPageGrid.setAttribute('disabled', 'disabled');
            } else {
                btnTopSellingItemsReportMoveToFirstPageGrid.removeAttribute('disabled');
                btnTopSellingItemsReportMoveToPreviousPageGrid.removeAttribute('disabled');
                btnTopSellingItemsReportMoveToNextPageGrid.removeAttribute('disabled');
                btnTopSellingItemsReportMoveToLastPageGrid.removeAttribute('disabled');
            }
            btnTopSellingItemsReportCurrentPageGrid.innerHTML = (topSellingItemsReportCollectionView.pageIndex + 1) + ' / ' + topSellingItemsReportCollectionView.pageCount;
        }

        // =================
        // Refresh Flex Grid
        // =================
        function refreshTopSellingItemsReportFlexGrid() {
            topSellingItemsReportCollectionView = new wijmo.collections.CollectionView(getTopSellingItemsReportData());
            topSellingItemsReportCollectionView.canFilter = true;
            topSellingItemsReportCollectionView.pageSize = cboTopSellingItemsReportShowNumberOfRows.selectedValue;

            var filterText = '';
            $('#inpTopSellingItemsReportFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                topSellingItemsReportCollectionView.refresh();
            });

            topSellingItemsReportCollectionView.filter = function (item) {
                return !filterText || (item.Item.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.BaseUnit.toLowerCase().indexOf(filterText) > -1);
            }

            topSellingItemsReportCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateTopSellingItemsReportNavigationButtons();
            });

            topSellingItemsReportFlexGrid.itemsSource = topSellingItemsReportCollectionView;
            topSellingItemsReportFlexGrid.trackChanges = true;
        }

        // ============
        // Download CSV
        // ============
        function btnTopSellingItemsReportDownloadCSVOnclick() {
            var CSV = '';
            var topSellingItemsReport = [];
            var fileName = 'TopSellingItemsReport.CSV';

            topSellingItemsReportCollectionView.moveToFirstPage();

            for (var p = 1; p <= topSellingItemsReportCollectionView.pageCount; p++) {
                for (i = 0; i < topSellingItemsReportCollectionView.items.length; i++) {
                    salesDetailReport.push({
                        Branch: topSellingItemsReportCollectionView.items[i].Branch,
                        Item: topSellingItemsReportCollectionView.items[i].Item,
                        Price: topSellingItemsReportCollectionView.items[i].Price,
                        Unit: topSellingItemsReportCollectionView.items[i].Unit,
                        Quantity: topSellingItemsReportCollectionView.items[i].Quantity,
                        Amount: topSellingItemsReportCollectionView.items[i].Amount
                    });
                }

                topSellingItemsReportCollectionView.moveToNextPage();
                if (p == topSellingItemsReportCollectionView.pageCount) {
                    topSellingItemsReportCollectionView.moveToPage(currentPageIndex - 1);
                }
            }

            var Headers = [];
            Headers.push({
                Branch: "Branch",
                Item: "Item",
                Price: "Price",
                Unit: "Unit",
                Quantity: "Quantity",
                Amount: "Amount"
            });

            var header = '';
            for (var b in Headers[0]) {
                header += '"' + Headers[0][b] + '",';
            }

            CSV = header + '\r\n';
            for (var i = 0; i < topSellingItemsReport.length; i++) {
                var row = '';
                for (var a in topSellingItemsReport[i]) {
                    row += '"' + topSellingItemsReport[i][a] + '",';
                }

                row.slice(0, row.length - 1);
                CSV += row + '\r\n';
            }

            if (CSV == '') {
                toastr.error("No data.");
            } else {
                var link = document.createElement("a");
                if (link.download !== undefined) {
                    var blob = new Blob([CSV], { type: 'text/csv;charset=utf-8;' });
                    var url = URL.createObjectURL(blob);
                    link.setAttribute("href", url);
                    link.setAttribute("download", fileName);
                    link.style = "visibility:hidden";
                }

                if (navigator.msSaveBlob) {
                    link.addEventListener("click", function (event) {
                        var blob = new Blob([CSV], {
                            "type": "text/csv;charset=utf-8;"
                        });
                        navigator.msSaveBlob(blob, fileName);
                    }, false);
                }

                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);

                toastr.success("Dowload successful.");
            }
        }

        // =========
        // Print PDF
        // =========
        function btnTopSellingItemsReportPDFOnclick() {
            if (cboTopSellingItemsReportCompanyFilter.selectedValue != null) {
                if (cboTopSellingItemsReportBranchFilter.selectedValue != null) {
                    var startDate = cboTopSellingItemsReportStartDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var endDate = cboTopSellingItemsReportEndDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var companyId = cboTopSellingItemsReportCompanyFilter.selectedValue;
                    var branchId = cboTopSellingItemsReportBranchFilter.selectedValue;

                    window.location.href = '/RepTopSellingItemsReport/TopSellingItemsReport?StartDate=' + startDate + '&EndDate=' + endDate + '&CompanyId=' + companyId + '&BranchId=' + branchId;
                } else {
                    toastr.error("Lacking Information");
                }
            } else {
                toastr.error("Lacking Information");
            }
        }

        // ============
        // On Load Page
        // ============
        $(document).ready(function () {
            createCboDateRangeFilters();
        });
    </script>
    <script type="text/javascript">
        // =================
        // User Branch Combo
        // =================
        var cboUserBranches = new wijmo.input.ComboBox('#cboUserBranches');

        // =========================
        // Get Current User Branches
        // =========================
        function getUserBranches() {
            $("#loadingUserBranches").show();
            document.getElementById("loadingUserBranches").innerHTML = '<i class="fa fa-spinner fa-spin"></i>';
            $("#userBranchContent").hide();

            $('#userBranchesModal').modal({ show: true, backdrop: 'static' });

            document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
            $("#updateUserBranch").prop("disabled", false);
            $("#closeUserBranch").prop("disabled", false);

            var userBranchesObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/userBranch/list',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (userBranchesResults) {
                    if (userBranchesResults.length > 0) {
                        for (i = 0; i < userBranchesResults.length; i++) {
                            userBranchesObservableArray.push({
                                BranchId: userBranchesResults[i]["BranchId"],
                                Branch: userBranchesResults[i]["Branch"]
                            });
                        }
                    }

                    var currentBranch = "@ViewData["BranchId"]";
                    cboUserBranches.dispose();
                    cboUserBranches = new wijmo.input.ComboBox('#cboUserBranches', {
                        isEditable: false,
                        placeholder: "Select Branches",
                        itemsSource: userBranchesObservableArray,
                        displayMemberPath: "Branch",
                        selectedValuePath: "BranchId",
                        selectedValue: currentBranch
                    });

                    setTimeout(function () {
                        $("#loadingUserBranches").hide();
                        $("#userBranchContent").show();
                    }, 500)
                }
            });
        }

        // ==========================
        // Update Current User Branch
        // ==========================
        function updateUserBranchOnclick() {
            document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-spinner fa-spin'></i> Updating";
            $("#updateUserBranch").prop("disabled", true);
            $("#closeUserBranch").prop("disabled", true);

            var userBranchObject = new Object();
            userBranchObject.BranchId = cboUserBranches.selectedValue;
            var userBranchData = JSON.stringify(userBranchObject);

            $.ajax({
                type: "PUT",
                url: '/api/userBranch/update',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: userBranchData,
                statusCode: {
                    200: function () {
                        toastr.success("Update Successful");
                        window.setTimeout(function () {
                            location.reload()
                        }, 1000);
                    },
                    404: function () {
                        toastr.error("Not Found");
                        document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
                        $("#updateUserBranch").prop("disabled", false);
                        $("#closeUserBranch").prop("disabled", false);
                    },
                    500: function () {
                        toastr.error("Bad Request");
                        document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
                        $("#updateUserBranch").prop("disabled", false);
                        $("#closeUserBranch").prop("disabled", false);
                    }
                }
            });
        }
    </script>
