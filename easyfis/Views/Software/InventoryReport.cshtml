    <script type="text/javascript">
        // ================
        // Global Variables
        // ================
        var companies;
        var cboCompany;
        var cboStockCardCompany;
        var accounts;
        var cboAccounts;
        var cboStockCardAccounts;
        var suppliers;
        var suppliersStockCard;
        var cboSuppliers;
        var cboStockCardSuppliers;
        var cboItems;
        var inventoryStartDate;
        var inventoryEndDate;
        var stockCardStartDate;
        var stockCardEndDate;

        // ===========
        // Get company
        // ===========
        function getCompanies() {
            companies = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/listCompany',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            companies.push({
                                Id: results[i]["Id"],
                                Company: results[i]["Company"],
                                Address: results[i]["Address"],
                                ContactNumber: results[i]["ContactNumber"],
                                TaxNumber: results[i]["TaxNumber"],
                            });
                        }

                        createCboComapny(companies);
                    }
                }
            }).fail(function (xhr, textStatus, err) {
                alert(err);
            });
            return companies;
        }

        // ===================
        // Company - Combo Box
        // ===================
        function createCboComapny(companies) {
            var companyCollection = new wijmo.collections.CollectionView(companies);
            var companyList = new Array();
            for (var i = 0; i < companyCollection.items.length; i++) {
                companyList.push(companyCollection.items[i].Company);
            }

            cboCompany.dispose();
            cboCompany = new wijmo.input.ComboBox('#Inventory_Company', {
                placeholder: "Select Company",
                itemsSource: companyList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('Inventory_Company_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#Inventory_Company_data").val(companyCollection.items[this.selectedIndex].Id);
                }
            });

            cboStockCardCompany.dispose();
            cboStockCardCompany = new wijmo.input.ComboBox('#StockCard_Company', {
                placeholder: "Select Company",
                itemsSource: companyList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('StockCard_Company_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#StockCard_Company_data").val(companyCollection.items[this.selectedIndex].Id);
                }
            });
        }

        // ============
        // Get Accounts
        // ============
        function getAccounts() {
            accounts = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/listAccount',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            accounts.push({
                                Id: results[i]["Id"],
                                AccountCode: results[i]["AccountCode"],
                                Account: results[i]["Account"],
                                AccountTypeId: results[i]["AccountTypeId"],
                                AccountType: results[i]["AccountType"],
                                AccountCashFlowId: results[i]["AccountCashFlowId"],
                                AccountCashFlow: results[i]["AccountCashFlow"]
                            });
                        }

                        createCboAccount(accounts);
                    }
                }
            }).fail(function (xhr, textStatus, err) {
                alert(err);
            });
            return accounts;
        }

        // ===================
        // Company - Combo Box
        // ===================
        function createCboAccount(accounts) {
            var accountCollection = new wijmo.collections.CollectionView(accounts);
            var accountList = new Array();
            for (var i = 0; i < accountCollection.items.length; i++) {
                accountList.push(accountCollection.items[i].Account);
            }

            cboAccounts.dispose();
            cboAccounts = new wijmo.input.ComboBox('#Inventory_Accounts', {
                placeholder: "Select Account",
                itemsSource: accountList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('Inventory_Accounts_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#Inventory_Accounts_data").val(accountCollection.items[this.selectedIndex].Id);

                    getSuppliers();
                }
            });

            cboStockCardAccounts.dispose();
            cboStockCardAccounts = new wijmo.input.ComboBox('#StockCard_Accounts', {
                placeholder: "Select Account",
                itemsSource: accountList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('StockCard_Accounts_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#StockCard_Accounts_data").val(accountCollection.items[this.selectedIndex].Id);

                    getSuppliersStockCard();
                }
            });
        }


        // =========================
        // Get supplier of Inventory
        // =========================
        function getSuppliers() {
