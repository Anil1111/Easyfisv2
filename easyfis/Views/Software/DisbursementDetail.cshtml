                    });
                } else {
                    $.ajax({
                        type: "PUT",
                        url: '/api/updateDisbursementLine/' + disbursementLineId,
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        data: disbursementLineData,
