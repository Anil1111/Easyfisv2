        // ===================================================
        // Create Combo Accounts Payable Voucher RR No. Filter
        // ===================================================
        function createCboAccountsPayableVoucherRRNoFilter() {
            var RRNosObservableArray = new wijmo.collections.ObservableArray();

            $.ajax({
                url: '/api/listReceivingReceipt',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {

                            var RRDateResult = new Date(results[i]["RRDate"])
                            var RRDate = ('0' + (RRDateResult.getMonth() + 1)).slice(-2) + '-' + ('0' + RRDateResult.getDate()).slice(-2) + '-' + RRDateResult.getFullYear();

                            RRNosObservableArray.push({
                                Id: results[i]["Id"],
                                RRNumber: results[i]["RRNumber"] + " (" + RRDate + ")",
                                RRDate: results[i]["RRDate"],
                            });
                        }
                    }

                    var disabled = true;
                    if (results.length > 0) {
                        disabled = false;
                    } else {
                        $("#btnAccountsPayableVoucherPrint").prop("disabled", true);
                    }

                    cboAccountsPayableVoucherRRNo.dispose();
                    cboAccountsPayableVoucherRRNo = new wijmo.input.ComboBox('#cboAccountsPayableVoucherRRNo', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: RRNosObservableArray,
                        displayMemberPath: "RRNumber",
                        selectedValuePath: "Id"
                    });

                    createCboAccountsPayableReportAcocuntFilter();
                }
            });
        }

