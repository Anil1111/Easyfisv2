        // ================
        // AP RR No. Filter
        // ================
        function createCboAPRRNoFilter() {
            var RRNosObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/listReceivingReceipt',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (RRNoFilterResults) {
                    if (RRNoFilterResults.length > 0) {
                        for (i = 0; i < RRNoFilterResults.length; i++) {

                            var RRDateResult = new Date(RRNoFilterResults[i]["RRDate"])
                            var RRDate = ('0' + (RRDateResult.getMonth() + 1)).slice(-2) + '-' + ('0' + RRDateResult.getDate()).slice(-2) + '-' + RRDateResult.getFullYear();

                            RRNosObservableArray.push({
                                Id: RRNoFilterResults[i]["Id"],
                                RRNumber: RRNoFilterResults[i]["RRNumber"] + " (" + RRDate + ")",
                                RRDate: RRNoFilterResults[i]["RRDate"],
                            });
                        }
                    }

                    var disabled = true;
                    if (RRNoFilterResults.length > 0) {
                        disabled = false;
                    } else {
                        $("#btnAccountsPayableVoucherPrint").prop("disabled", true);
                    }

                    cboAccountsPayableVoucherRRNo.dispose();
                    cboAccountsPayableVoucherRRNo = new wijmo.input.ComboBox('#cboAccountsPayableVoucherRRNo', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: RRNosObservableArray,
                        displayMemberPath: "RRNumber",
                        selectedValuePath: "Id"
                    });

                    createCboAPAccountFilter();
                }
            });
        }

