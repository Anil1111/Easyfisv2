            } else {
                companyId = document.getElementById('accountsPaybleReport_Comapny_data').value;
                document.getElementById('accountsPaybleReport_Comapny').style.border = " ";
                window.location.href = '/Software/AccountsPayablePDF?DateAsOf=' + accountsPaybleReportDateAsOf + '&CompanyId=' + companyId;
            }
        }

