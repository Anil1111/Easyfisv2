    <script type="text/javascript">
        // ================
        // Global Variables
        // ================
        var accountsPayableReportDateAsOf;
        var companies;
        var cboCompany;
        var accounts;
        var cboAccounts;
        var suppliers;
        var cboSuppliers;
        var RRNos;
        var cboRRNo;
        var InputVATSummaryReportStartDate;
        var InputVATSummaryReportEndDate;
        var withHoldingTAXDetailReportStartDate;
        var withHoldingTAXDetailReportEndDate;
        var withHoldingTAXSummarryReportStartDate;
        var withHoldingTAXSummarryReportEndDate;
        var certificateOfCreditableTaxWithheldStartDate;
        var certificateOfCreditableTaxWithheldEndDate;

        // ===========
        // Get company
        // ===========
        function getCompanies() {
            companies = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/listCompany',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            companies.push({
                                Id: results[i]["Id"],
                                Company: results[i]["Company"],
                                Address: results[i]["Address"],
                                ContactNumber: results[i]["ContactNumber"],
                                TaxNumber: results[i]["TaxNumber"],
                            });
                        }

                        createCboComapny(companies);
                    }
                }
            });
            return companies;
        }

        // ===================
        // Company - Combo Box
        // ===================
        function createCboComapny(companies) {
            var companyCollection = new wijmo.collections.CollectionView(companies);
            var companyList = new Array();
            for (var i = 0; i < companyCollection.items.length; i++) {
                companyList.push(companyCollection.items[i].Company);
            }

            cboCompany.dispose();
            cboCompany = new wijmo.input.ComboBox('#accountsPaybleReport_Comapny', {
                placeholder: "Select Company",
                itemsSource: companyList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('accountsPaybleReport_Comapny_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#accountsPaybleReport_Comapny_data").val(companyCollection.items[this.selectedIndex].Id);
                }
            });
        }

        // ============
        // Get Accounts
        // ============
        function getAccounts() {
            accounts = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/listAccount',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            accounts.push({
                                Id: results[i]["Id"],
                                AccountCode: results[i]["AccountCode"],
                                Account: results[i]["Account"],
                                AccountTypeId: results[i]["AccountTypeId"],
                                AccountType: results[i]["AccountType"],
                                AccountCashFlowId: results[i]["AccountCashFlowId"],
                                AccountCashFlow: results[i]["AccountCashFlow"]
                            });
                        }

                        createCboAccount(accounts);
                    }
                }
            });
            return accounts;
        }

        // ===================
        // Company - Combo Box
        // ===================
        function createCboAccount(accounts) {
            var accountCollection = new wijmo.collections.CollectionView(accounts);
            var accountList = new Array();
            for (var i = 0; i < accountCollection.items.length; i++) {
                accountList.push(accountCollection.items[i].Account);
            }

            cboAccounts.dispose();
            cboAccounts = new wijmo.input.ComboBox('#accountsPaybleReport_Accounts', {
                placeholder: "Select Account",
                itemsSource: accountList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('accountsPaybleReport_Accounts_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#accountsPaybleReport_Accounts_data").val(accountCollection.items[this.selectedIndex].Id);

                    getSuppliers();
                }
            });
        }


        // ============
        // Get supplier
        // ============
        function getSuppliers() {
            suppliers = new wijmo.collections.ObservableArray();
            var accountId = document.getElementById('accountsPaybleReport_Accounts_data').value;

            var disabled;
            if (document.getElementById('accountsPaybleReport_Accounts_data').value == "") {
                var accountId = 0;
            }
            else {
                var accountId = document.getElementById('accountsPaybleReport_Accounts_data').value;
            }

            $.ajax({
                url: '/api/articleByAccountTypeIdAndArticleTypeId/' + accountId + "/" + '3',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (Results) {
                    if (Results.length > 0) {
                        for (i = 0; i < Results.length; i++) {
                            suppliers.push({
                                Id: Results[i]["Id"],
                                ArticleCode: Results[i]["ArticleCode"],
                                ManualArticleCode: Results[i]["ManualArticleCode"],
                                Article: Results[i]["Article"],
                                Category: Results[i]["Category"],
                                ArticleTypeId: Results[i]["ArticleTypeId"],
                                ArticleType: Results[i]["ArticleType"],
                                ArticleGroupId: Results[i]["ArticleGroupId"],
                                ArticleGroup: Results[i]["ArticleGroup"],
                                AccountId: Results[i]["AccountId"],
                                Account: Results[i]["Account"],
                                SalesAccountId: Results[i]["SalesAccountId"],
                                CostAccountId: Results[i]["CostAccountId"],
                                AssetAccountId: Results[i]["AssetAccountId"],
                                ExpenseAccountId: Results[i]["ExpenseAccountId"],
                                UnitId: Results[i]["UnitId"],
                                OutputTaxId: Results[i]["OutputTaxId"],
                                InputTaxId: Results[i]["InputTaxId"],
                                WTaxTypeId: Results[i]["WTaxTypeId"],
                                Price: Results[i]["Price"],
                                Cost: Results[i]["Cost"],
                                IsInventory: Results[i]["IsInventory"],
                                Particulars: Results[i]["Particulars"],
                                Address: Results[i]["Address"],
                                TermId: Results[i]["TermId"],
                                ContactNumber: Results[i]["ContactNumber"],
                                ContactPerson: Results[i]["ContactPerson"],
                                TaxNumber: Results[i]["TaxNumber"],
                                CreditLimit: Results[i]["CreditLimit"],
                                DateAcquired: Results[i]["DateAcquired"],
                                UsefulLife: Results[i]["UsefulLife"],
                                SalvageValue: Results[i]["SalvageValue"],
                                ManualArticleOldCode: Results[i]["ManualArticleOldCode"],
                                IsLocked: Results[i]["IsLocked"],
                                CreatedById: Results[i]["CreatedById"],
                                CreatedBy: Results[i]["CreatedBy"],
                                CreatedDateTime: Results[i]["CreatedDateTime"],
                                UpdatedById: Results[i]["UpdatedById"],
                                UpdatedBy: Results[i]["UpdatedBy"],
                                UpdatedDateTime: Results[i]["UpdatedDateTime"]
                            });
                        }
                        createCboSupplier(suppliers);
                    } else {
                        createCboSupplier(suppliers);
                    }
                }
            });
            return suppliers;
        }

        // ====================
        // Supplier - Combo Box
        // ====================
        function createCboSupplier(suppliers) {
            var supplierCollection = new wijmo.collections.CollectionView(suppliers);
            var supplierList = new Array();
            for (var i = 0; i < supplierCollection.items.length; i++) {
                supplierList.push(supplierCollection.items[i].Article);
            }

            var disabled;
            if (suppliers == 0) {
                disabled = true;
            } else {
                disabled = false;
            }

            document.getElementById('accountsPaybleReport_Supplier_data').value = "";

            cboSuppliers.dispose();
            cboSuppliers = new wijmo.input.ComboBox('#accountsPaybleReport_Supplier', {
                disabled: disabled,
                placeholder: "Select Supplier",
                itemsSource: supplierList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('accountsPaybleReport_Supplier_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#accountsPaybleReport_Supplier_data").val(supplierCollection.items[this.selectedIndex].Id);
                }
            });
        }

        // ======
        // Get RR
        // ======
        function getRRs() {
            RRNos = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/listReceivingReceipt',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (Results) {
                    if (Results.length > 0) {
                        for (i = 0; i < Results.length; i++) {
                            RRNos.push({
                                Id: Results[i]["Id"],
                                BranchId: Results[i]["BranchId"],
                                Branch: Results[i]["Branch"],
                                RRNumber: Results[i]["RRNumber"],
                                RRDate: Results[i]["RRDate"],
                                SupplierId: Results[i]["SupplierId"],
                                Supplier: Results[i]["Supplier"],
                                TermId: Results[i]["TermId"],
                                Term: Results[i]["Term"],
                                DocumentReference: Results[i]["DocumentReference"],
                                ManualRRNumber: Results[i]["ManualRRNumber"],
                                Remarks: Results[i]["Remarks"],
                                Amount: Results[i]["Amount"],
                                WTaxAmount: Results[i]["WTaxAmount"],
                                PaidAmount: Results[i]["PaidAmount"],
                                AdjustmentAmount: Results[i]["AdjustmentAmount"],
                                BalanceAmount: Results[i]["BalanceAmount"],
                                ReceivedById: Results[i]["ReceivedById"],
                                ReceivedBy: Results[i]["ReceivedBy"],
                                PreparedById: Results[i]["PreparedById"],
                                PreparedBy: Results[i]["PreparedBy"],
                                CheckedById: Results[i]["CheckedById"],
                                CheckedBy: Results[i]["CheckedBy"],
                                ApprovedById: Results[i]["ApprovedById"],
                                ApprovedBy: Results[i]["ApprovedBy"],
                                IsLocked: Results[i]["IsLocked"],
                                CreatedById: Results[i]["CreatedById"],
                                CreatedBy: Results[i]["CreatedBy"],
                                CreatedDateTime: Results[i]["CreatedDateTime"],
                                UpdatedById: Results[i]["UpdatedById"],
                                UpdatedBy: Results[i]["UpdatedBy"],
                                UpdatedDateTime: Results[i]["UpdatedDateTime"]
                            });
                        }
                        createCboRRNo(RRNos);
                    }
                }
            });
            return RRNos;
        }

        // =================
        // RR No - Combo Box
        // =================
        function createCboRRNo(RRNos) {
            var rrNoCollection = new wijmo.collections.CollectionView(RRNos);
            var rrNoList = new Array();
            for (var i = 0; i < rrNoCollection.items.length; i++) {
                rrNoList.push(rrNoCollection.items[i].RRNumber);
            }

            cboRRNo.dispose();
            cboRRNo = new wijmo.input.ComboBox('#accountsPayableVoucher_RRNo', {
                placeholder: "Select RR No",
                itemsSource: rrNoList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('accountsPayableVoucher_RRNo_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#accountsPayableVoucher_RRNo_data").val(rrNoCollection.items[this.selectedIndex].Id);
                }
            });
        }

        // get Input VAT Summary Report Start Date
        function getInputVATSummaryReportStartDate() {
            // Input Vat Summary Report Start Date
            InputVATSummaryReportStartDate.dispose();
            InputVATSummaryReportStartDate = new wijmo.input.InputDate('#InputVATSummaryReport_StartDate', {
                format: 'MM/dd/yyyy',
                value: new Date(document.getElementById('InputVATSummaryReport_StartDate_data').value),
                mask: '99/99/9999',
                max: new Date(document.getElementById('InputVATSummaryReport_EndDate_data').value),
                onValueChanged: function () {
                    document.getElementById('InputVATSummaryReport_StartDate_data').value = this.value.toLocaleDateString();
                    setTimeout(function () {
                        getInputVATSummaryReportEndDate();
                    }, 50);
                }
            });
