<!DOCTYPE html>
<html>
<head>
    <!-- Meta -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <meta name="description" content="Some slide and push menu demos using CSS3 transitions.">

    <!-- Title of the Page -->
    <title>Software Dashboard</title>

    <!-- CSS StyleSheets-->
    @Styles.Render("~/Font-Awesome/css")
    @Styles.Render("~/Content/Software-css")
</head>
<body>
    <div id="wrapper" class="wrapper">
        <!-- Header -->
        <header>
            <nav class="navbar navbar-inverse navbar-fixed-top">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a href="#" class="navbar-brand"> ERP System</a>
                    </div>
                    <div class="collapse navbar-collapse" id="myNavbar">
                        <ul class="nav navbar-nav navbar-right">
                            <li><a href="#">Home</a></li>
                            <li><a href="#">About</a></li>
                            <li><a href="#">Contact</a></li>
                            <li><a href="#">Login</a></li>
                            <li><a href="#"><i class="fa fa-gear"></i></a></li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>

        <!-- Section -->
        <section>
            <br><br><br>
            <div class="container">
                <button id="c-button--push-left" class="btn btn-default"><i class="fa fa-bars"></i> Software Dashboard Menu</button><br><br>
                <div class="jumbotron">
                    <div class="row">
                        <!-- First Vertical Row -->
                        <div class="col-lg-3 col-md-6 col-sm-12 col-xs-12">
                            <a class="btn-masterlist btn-dashboard button--masterlist" href="#">Supplier</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Purchase Order</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Receiving Receipt</a>
                            <a class="btn-reports btn-dashboard button--reports" href="#">Accounts Payable</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Disbursement</a>
                            <a class="btn-masterlist btn-dashboard  button--masterlist" href="#">Bank</a>
                        </div>
                        <!-- Second Vertical Row -->
                        <div class="col-lg-3 col-md-6 col-sm-12 col-xs-12">
                            <a class="btn-masterlist btn-dashboard button--masterlist" href="#">Customer</a>
                            <a class="btn-submenu btn-sale button--submenu" href="#">Sales</a>
                            <a class="btn-reports btn-dashboard button--reports" href="#">Accounts Receivable</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Collection</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Bank Reconciliation</a>
                        </div>
                        <!-- Third Vertical Row -->
                        <div class="col-lg-3 col-md-6 col-sm-12 col-xs-12">
                            <a class="btn-masterlist btn-dashboard button--masterlist" href="#">Items</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Stock In</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Stock Out</a>
                            <a class="btn-reports btn-dashboard button--reports" href="#">Inventory Report</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Stock Transfer</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">System Tables</a>
                        </div>
                        <!-- Fourt Vertical Table-->
                        <div class="col-lg-3 col-md-6 col-sm-12 col-xs-12">
                            <a class="btn-masterlist btn-dashboard button--masterlist" href="#">Chart of Accounts</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Journal Voucher</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Comapany</a>
                            <a class="btn-reports btn-dashboard button--reports" href="#">Financial Statements</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Users</a>
                            <a class="btn-submenu btn-dashboard button--submenu" href="#">Settings</a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>

    <!-- SideBar Menu -->
