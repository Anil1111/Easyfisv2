<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="shortcut icon" href="~/Images/icon/easyfislogomini.ico">
    <title>Receiving Receipt Detail Report</title>

    @Styles.Render("~/Content/Software-css")
</head>
<body>
    <div id="o-wrapper" class="o-wrapper">
        <main class="o-content">
            @Html.Partial("_SoftwareHeader")

            <section class="content-header">
                <div class="container">
                    <h3>
                        Receiving Receipt Detail Report
                    </h3>
                    <p>
                        <i class="fa fa-building-o fa-fw"></i> &nbsp; @ViewData["Company"] <br />
                        <i class="fa fa-home fa-fw"></i> &nbsp; <a href="#" onclick="getUserBranches()" style="color: white;">@ViewData["Branch"]</a>
                    </p>
                </div>
            </section>
            <section class="container">
                <div class="panel panel-default">

                    <!--
                        ===============
                        Search / Filter
                        ===============
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 49.5%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                        <input type="text" class="form-control" id="inpReceivingReceiptDetailReportFilter" placeholder="Search...">
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><b><i class="fa fa-table"></i></b></span>
                                        <div class="comboBox-wide" id="cboReceivingReceiptDetailReportShowNumberOfRows"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24.5%" align="right">
                                    <button onclick="btnReceivingReceiptDetailReportDownloadCSVOnclick()" id="btnReceivingReceiptDetailReportDownloadCSV" class="btn btn-success"><i class="fa fa-file-excel-o" aria-hidden="true"></i> CSV</button>
                                    <button onclick="btnReceivingReceiptDetailReportPDFOnclick()" id="btnReceivingReceiptDetailReportPDF" class="btn btn-primary"><i class="fa fa-print"></i> Print</button>
                                    <a href="/Software/AccountsPayable" class="btn btn-danger"><i class="fa fa-close"></i> Close</a>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        ======================================
                        Date Range, Company and Branch Filters
                        ======================================
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Start Date</b></span>
                                        <div class="comboBox-wide" id="cboReceivingReceiptDetailReportStartDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%" align="right">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>End Date</b></span>
                                        <div class="comboBox-wide" id="cboReceivingReceiptDetailReportEndDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Company</b></span>
                                        <div class="comboBox-wide" id="cboReceivingReceiptDetailReportCompanyFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Branch</b></span>
                                        <div class="comboBox-wide" id="cboReceivingReceiptDetailReportBranchFilter"></div>
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        =========
                        Flex Grid
                        =========
                    -->
                    <div class="panel-body">
                        <div id="receivingReceiptDetailReportFlexGrid" style="font-size: 11.5px;"></div>
                    </div>

                    <!--
                        ==================
                        Navigation Buttons
                        ==================
                    -->
                    <div class="panel-footer">
                        <div class="row">
                            <div class="btn-group col-md-7" id="receivingReceiptDetailReportNavigationPageGrid">
                                <button type="button" class="btn btn-default border-custom" id="btnReceivingReceiptDetailReportMoveToFirstPageGrid">
                                    <span class="glyphicon glyphicon-fast-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnReceivingReceiptDetailReportMoveToPreviousPageGrid">
                                    <span class="glyphicon glyphicon-step-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" disabled style="width: 100px" id="btnReceivingReceiptDetailReportCurrentPageGrid"></button>
                                <button type="button" class="btn btn-default border-custom" id="btnReceivingReceiptDetailReportMoveToNextPageGrid">
                                    <span class="glyphicon glyphicon-step-forward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnReceivingReceiptDetailReportMoveToLastPageGrid">
                                    <span class="glyphicon glyphicon-fast-forward"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <!--
                    ======
                    Totals
                    ======
                -->
                <div class="panel panel-default">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-6"></div>
                            <div class="col-md-6">
                                <dl class="dl-horizontal dl-custom">
                                    <dt>
                                        <b>Total Amount</b>
                                    </dt>
                                    <dd>
                                        <input type="text" class="form-control numberField" id="inpTotalAmount" placeholder="0.00" disabled />
                                    </dd>
                                    <dt>
                                        <b>Total VAT</b>
                                    </dt>
                                    <dd>
                                        <input type="text" class="form-control numberField" id="inpTotalVATAmount" placeholder="0.00" disabled />
                                    </dd>
                                    <dt>
                                        <b>Total WTAX</b>
                                    </dt>
                                    <dd>
                                        <input type="text" class="form-control numberField" id="inpTotalWTAXAmount" placeholder="0.00" disabled />
                                    </dd>
                                </dl>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            @Html.Partial("_SoftwareFooter")
        </main>
    </div>
    <div id="c-mask" class="c-mask"></div>

    @Scripts.Render("~/Scripts/Software-js")
    <script type="text/javascript">
        // ============
        // Date Filters
        // ============
        var cboReceivingReceiptDetailReportStartDateFilter = new wijmo.input.InputDate('#cboReceivingReceiptDetailReportStartDateFilter');
        var cboReceivingReceiptDetailReportEndDateFilter = new wijmo.input.InputDate('#cboReceivingReceiptDetailReportEndDateFilter');
        var isReceivingReceiptDetailReportStartDateSelected = false;
        var isReceivingReceiptDetailReportEndDateSelected = false;

        // =========================
        // Company and Branch Filter
        // =========================
        var cboReceivingReceiptDetailReportCompanyFilter = new wijmo.input.ComboBox('#cboReceivingReceiptDetailReportCompanyFilter');
        var cboReceivingReceiptDetailReportBranchFilter = new wijmo.input.ComboBox('#cboReceivingReceiptDetailReportBranchFilter');
        var isCboReceivingReceiptDetailReportCompanySelected = false;

        // =============================
        // Collection View and Flex Grid
        // =============================
        var receivingReceiptDetailReportCollectionView;
        var receivingReceiptDetailReportFlexGrid;
        var cboReceivingReceiptDetailReportShowNumberOfRows = new wijmo.input.ComboBox('#cboReceivingReceiptDetailReportShowNumberOfRows');

        // ==================
        // Navigation Buttons
        // ==================
        var btnReceivingReceiptDetailReportMoveToFirstPageGrid = document.getElementById('btnReceivingReceiptDetailReportMoveToFirstPageGrid');
        var btnReceivingReceiptDetailReportMoveToPreviousPageGrid = document.getElementById('btnReceivingReceiptDetailReportMoveToPreviousPageGrid');
        var btnReceivingReceiptDetailReportMoveToNextPageGrid = document.getElementById('btnReceivingReceiptDetailReportMoveToNextPageGrid');
        var btnReceivingReceiptDetailReportMoveToLastPageGrid = document.getElementById('btnReceivingReceiptDetailReportMoveToLastPageGrid');
        var btnReceivingReceiptDetailReportCurrentPageGrid = document.getElementById('btnReceivingReceiptDetailReportCurrentPageGrid');
        var currentPageIndex = 0;

        // =======================
        // Get URL Parameter Value
        // =======================
        function getURLParameterValue(name) {
            name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");

            var regexS = "[\\?&]" + name + "=([^&#]*)";
            var regex = new RegExp(regexS);

            var results = regex.exec(window.location.href);
            if (results == null) {
                return "";
            } else {
                return results[1];
            }
        }

        // ================================
        // Create Combo Date Ranged Filters
        // ================================
        function createCboDateRangeFilters() {
            NProgress.start();

            cboReceivingReceiptDetailReportStartDateFilter.dispose();
            cboReceivingReceiptDetailReportStartDateFilter = new wijmo.input.InputDate('#cboReceivingReceiptDetailReportStartDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameterValue("StartDate")),
                onValueChanged: function () {
                    if (!isReceivingReceiptDetailReportStartDateSelected) {
                        NProgress.start();
                        refreshReceivingReceiptDetailReportFlexGrid();
                    } else {
                        isReceivingReceiptDetailReportStartDateSelected = false;
                    }
                }
            });

            cboReceivingReceiptDetailReportEndDateFilter.dispose();
            cboReceivingReceiptDetailReportEndDateFilter = new wijmo.input.InputDate('#cboReceivingReceiptDetailReportEndDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameterValue("EndDate")),
                onValueChanged: function () {
                    if (!isReceivingReceiptDetailReportEndDateSelected) {
                        NProgress.start();
                        refreshReceivingReceiptDetailReportFlexGrid();
                    } else {
                        isReceivingReceiptDetailReportEndDateSelected = false;
                    }
                }
            });

            createCboCompanyFilter();
        }

        // ===========================
        // Create Combo Company Filter
        // ===========================
        function createCboCompanyFilter() {
            var companyObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/receivingReceiptDetailReport/dropdown/list/company',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            companyObservableArray.push({
                                Id: results[i]["Id"],
                                Company: results[i]["Company"]
                            });
                        }
                    }

                    var disabled = true;
                    if (results.length > 0) {
                        disabled = false;
                    } else {
                        $("#btnReceivingReceiptDetailReportDownloadCSV").prop("disabled", true);
                        $("#btnReceivingReceiptDetailReportPDF").prop("disabled", true);
                    }

                    cboReceivingReceiptDetailReportCompanyFilter.dispose();
                    cboReceivingReceiptDetailReportCompanyFilter = new wijmo.input.ComboBox('#cboReceivingReceiptDetailReportCompanyFilter', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: companyObservableArray,
                        displayMemberPath: "Company",
                        selectedValuePath: "Id",
                        selectedValue: getURLParameterValue("CompanyId"),
                        onSelectedIndexChanged: function () {
                            NProgress.start();
                            $("#btnReceivingReceiptDetailReportDownloadCSV").prop("disabled", false);
                            $("#btnReceivingReceiptDetailReportPDF").prop("disabled", false);
                            isCboReceivingReceiptDetailReportCompanySelected = true;
                            createCboBranchFilter();
                        }
                    });

                    createCboBranchFilter();
                }
            });
        }

        // ==========================
        // Create Combo Branch Filter
        // ==========================
        function createCboBranchFilter() {
            var branchObservableArray = new wijmo.collections.ObservableArray();
            if (cboReceivingReceiptDetailReportCompanyFilter.selectedValue != null) {
                $.ajax({
                    url: '/api/receivingReceiptDetailReport/dropdown/list/branch/' + cboReceivingReceiptDetailReportCompanyFilter.selectedValue,
                    cache: false,
                    type: 'GET',
                    contentType: 'application/json; charset=utf-8',
                    success: function (branchResults) {
                        if (branchResults.length > 0) {
                            for (i = 0; i < branchResults.length; i++) {
                                branchObservableArray.push({
                                    Id: branchResults[i]["Id"],
                                    Branch: branchResults[i]["Branch"]
                                });
                            }
                        }

                        var disabled = true;
                        if (branchResults.length > 0) {
                            disabled = false;
                        } else {
                            $("#btnReceivingReceiptDetailReportDownloadCSV").prop("disabled", true);
                            $("#btnReceivingReceiptDetailReportPDF").prop("disabled", true);
                        }

                        cboReceivingReceiptDetailReportBranchFilter.dispose();
                        cboReceivingReceiptDetailReportBranchFilter = new wijmo.input.ComboBox('#cboReceivingReceiptDetailReportBranchFilter', {
                            disabled: disabled,
                            isEditable: false,
                            itemsSource: branchObservableArray,
                            displayMemberPath: "Branch",
                            selectedValuePath: "Id",
                            selectedValue: getURLParameterValue("BranchId"),
                            onSelectedIndexChanged: function () {
                                NProgress.start();
                                refreshReceivingReceiptDetailReportFlexGrid();
                            }
                        });

                        createCboShowRows();
                    }
                });
            }
        }

        // ===================================
        // Create Combo Display Number of Rows
        // ===================================
        function createCboShowRows() {
            var rowsObservableArray = new wijmo.collections.ObservableArray();

            for (var i = 0; i <= 4; i++) {
                var rows = 0;
                var rowsString = "";

                if (i == 0) {
                    rows = 15;
                    rowsString = "Show 15 Rows";
                } else if (i == 1) {
                    rows = 50;
                    rowsString = "Show 50 Rows";
                } else if (i == 2) {
                    rows = 100;
                    rowsString = "Show 100 Rows";
                } else if (i == 3) {
                    rows = 150;
                    rowsString = "Show 150 Rows";
                } else {
                    rows = 200;
                    rowsString = "Show 200 Rows";
                }

                rowsObservableArray.push({
                    rowNumber: rows,
                    rowString: rowsString
                });
            }

            cboReceivingReceiptDetailReportShowNumberOfRows.dispose();
            cboReceivingReceiptDetailReportShowNumberOfRows = new wijmo.input.ComboBox('#cboReceivingReceiptDetailReportShowNumberOfRows', {
                isEditable: false,
                itemsSource: rowsObservableArray,
                displayMemberPath: "rowString",
                selectedValuePath: "rowNumber",
                selectedValue: 50,
                onSelectedIndexChanged: function () {
                    NProgress.start();

                    setTimeout(function () {
                        var numberOfRows = cboReceivingReceiptDetailReportShowNumberOfRows.selectedValue;
                        receivingReceiptDetailReportCollectionView.pageSize = parseInt(numberOfRows);
                        receivingReceiptDetailReportCollectionView.refresh();
                        receivingReceiptDetailReportFlexGrid.refresh();

                        NProgress.done();
                    }, 300);
                }
            });

            if (!isCboReceivingReceiptDetailReportCompanySelected) {
                createReceivingReceiptDetailReportFlexGrid();
            } else {
                refreshReceivingReceiptDetailReportFlexGrid();
            }
        }

        // =================================
        // Get Receiving Receipt Detail Data
        // =================================
        function getReceivingReceiptDetailReportData() {
            var receivingReceiptDetailReports = new wijmo.collections.ObservableArray;
            if (cboReceivingReceiptDetailReportCompanyFilter.selectedValue != null) {
                if (cboReceivingReceiptDetailReportBranchFilter.selectedValue != null) {

                    var startDate = cboReceivingReceiptDetailReportStartDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var endDate = cboReceivingReceiptDetailReportEndDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var companyId = cboReceivingReceiptDetailReportCompanyFilter.selectedValue;
                    var branchId = cboReceivingReceiptDetailReportBranchFilter.selectedValue;

                    $.ajax({
                        url: '/api/receivingReceiptDetailReport/list/' + startDate + '/' + endDate + '/' + + companyId + '/' + branchId,
                        cache: false,
                        type: 'GET',
                        contentType: 'application/json; charset=utf-8',
                        success: function (results) {
                            var inpTotalAmount = 0, totalVAT = 0, totalWTAX = 0;
                            if (results.length > 0) {
                                for (i = 0; i < results.length; i++) {
                                    var docLink = "/Software/ReceivingReceiptDetail?id=" + results[i]["RRId"];

                                    var RRDateResult = new Date(results[i]["RRDate"])
                                    var RRDate = ('0' + (RRDateResult.getMonth() + 1)).slice(-2) + '-' + ('0' + RRDateResult.getDate()).slice(-2) + '-' + RRDateResult.getFullYear();

                                    receivingReceiptDetailReports.push({
                                        BtnDetail: "<a href='" + docLink + "' class='btn btn-primary btn-xs btn-block' style='font-size: 9px;'><i class='fa fa-edit'></i> Detail",
                                        RRId: results[i]["RRId"],
                                        Branch: results[i]["Branch"],
                                        RRNumber: "RR-" + results[i]["RRNumber"],
                                        RRDate: RRDate,
                                        Supplier: results[i]["Supplier"],
                                        Remarks: results[i]["Remarks"],
                                        PONumber: "PO-" + results[i]["PONumber"],
                                        Quantity: results[i]["Quantity"],
                                        QuantityDisplay: parseFloat(results[i]["Quantity"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        ItemCode: results[i]["ItemCode"],
                                        ItemManualArticleOldCode: results[i]["ItemManualArticleOldCode"],
                                        ItemDescription: results[i]["ItemDescription"],
                                        Unit: results[i]["Unit"],
                                        Cost: results[i]["Cost"],
                                        CostDisplay: parseFloat(results[i]["Cost"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        Amount: results[i]["Amount"],
                                        AmountDisplay: parseFloat(results[i]["Amount"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        VAT: results[i]["VAT"],
                                        VATDisplay: parseFloat(results[i]["VAT"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        WTAX: results[i]["WTAX"],
                                        WTAXDisplay: parseFloat(results[i]["WTAX"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                    });

                                    inpTotalAmount += results[i]["Amount"];
                                    totalVAT += results[i]["VAT"];
                                    totalWTAX += results[i]["WTAX"];
                                }
                            }

                            document.getElementById("inpTotalAmount").value = parseFloat(inpTotalAmount).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                            document.getElementById("inpTotalVATAmount").value = parseFloat(totalVAT).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                            document.getElementById("inpTotalWTAXAmount").value = parseFloat(totalWTAX).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                            NProgress.done();
                        }
                    });
                } else {
                    document.getElementById("inpTotalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalVATAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    document.getElementById("inpTotalWTAXAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    NProgress.done();
                }
            } else {
                document.getElementById("inpTotalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                document.getElementById("inpTotalVATAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                document.getElementById("inpTotalWTAXAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                NProgress.done();
            }

            return receivingReceiptDetailReports;
        }

        // ================================================
        // Create Receiving Receipt Detail Report Flex Grid
        // ================================================
        function createReceivingReceiptDetailReportFlexGrid() {
            receivingReceiptDetailReportCollectionView = new wijmo.collections.CollectionView(getReceivingReceiptDetailReportData());
            receivingReceiptDetailReportCollectionView.canFilter = true;
            receivingReceiptDetailReportCollectionView.pageSize = cboReceivingReceiptDetailReportShowNumberOfRows.selectedValue;

            var filterText = '';
            $('#inpReceivingReceiptDetailReportFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                receivingReceiptDetailReportCollectionView.refresh();
            });

            receivingReceiptDetailReportCollectionView.filter = function (item) {
                return !filterText || (item.RRNumber.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Supplier.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.PONumber.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemManualArticleOldCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemDescription.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Unit.toLowerCase().indexOf(filterText) > -1);
            }

            receivingReceiptDetailReportCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateReceivingReceiptDetailReportNavigationButtons();
            });

            receivingReceiptDetailReportFlexGrid = new wijmo.grid.FlexGrid('#receivingReceiptDetailReportFlexGrid');
            receivingReceiptDetailReportFlexGrid.initialize({
                columns: [
                    {
                        "header": "Detail",
                        "binding": "BtnDetail",
                        "width": 55,
                        "isContentHtml": true
                    },
                    {
                        "header": "RR No.",
                        "binding": "RRNumber",
                        "isContentHtml": true,
                        "allowSorting": true,
                        "width": "1.4*"
                    },
                    {
                        "header": "RR Date",
                        "binding": "RRDate",
                        "allowSorting": true,
                        "width": "1*"
                    },
                    {
                        "header": "Supplier",
                        "binding": "Supplier",
                        "allowSorting": true,
                        "width": "1.5*"
                    },
                    {
                        "header": "Remarks",
                        "binding": "Remarks",
                        "allowSorting": true,
                        "width": "1.2*"
                    },
                    {
                        "header": "PO No.",
                        "binding": "PONumber",
                        "allowSorting": true,
                        "width": "1.4*"
                    },
                    {
                        "header": "Quantity",
                        "binding": "QuantityDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1*"
                    },
                    {
                        "header": "Barcode",
                        "binding": "ItemCode",
                        "allowSorting": true,
                        "width": "1*"
                    },
                    {
                        "header": "SKU Code",
                        "binding": "ItemManualArticleOldCode",
                        "allowSorting": true,
                        "width": "1*"
                    },
                    {
                        "header": "Description",
                        "binding": "ItemDescription",
                        "allowSorting": true,
                        "width": "1.5*"
                    },
                    {
                        "header": "Unit",
                        "binding": "Unit",
                        "allowSorting": true,
                        "width": "0.7*"
                    },
                    {
                        "header": "Cost",
                        "binding": "CostDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1*"
                    },
                    {
                        "header": "Amount",
                        "binding": "AmountDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1*"
                    },
                    {
                        "header": "VAT",
                        "binding": "VATDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1*"
                    },
                    {
                        "header": "WTax",
                        "binding": "WTAXDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1*"
                    }
                ],

                autoGenerateColumns: false,
                itemsSource: receivingReceiptDetailReportCollectionView,
                isReadOnly: true,
                autoSizeMode: wijmo.grid.AutoSizeMode.Both,
                allowDragging: wijmo.grid.AllowDragging.None,
                selectionMode: wijmo.grid.SelectionMode.Row
            });

            receivingReceiptDetailReportFlexGrid.trackChanges = true;

            updateReceivingReceiptDetailReportNavigationButtons();

            btnReceivingReceiptDetailReportMoveToFirstPageGrid.addEventListener('click', function () {
                receivingReceiptDetailReportCollectionView.moveToFirstPage();
                currentPageIndex = receivingReceiptDetailReportCollectionView.pageIndex + 1;
                updateReceivingReceiptDetailReportNavigationButtons();
            });

            btnReceivingReceiptDetailReportMoveToPreviousPageGrid.addEventListener('click', function () {
                receivingReceiptDetailReportCollectionView.moveToPreviousPage();
                currentPageIndex = receivingReceiptDetailReportCollectionView.pageIndex + 1;
                updateReceivingReceiptDetailReportNavigationButtons();
            });

            btnReceivingReceiptDetailReportMoveToNextPageGrid.addEventListener('click', function () {
                receivingReceiptDetailReportCollectionView.moveToNextPage();
                currentPageIndex = receivingReceiptDetailReportCollectionView.pageIndex + 1;
                updateReceivingReceiptDetailReportNavigationButtons();
            });

            btnReceivingReceiptDetailReportMoveToLastPageGrid.addEventListener('click', function () {
                receivingReceiptDetailReportCollectionView.moveToLastPage();
                currentPageIndex = receivingReceiptDetailReportCollectionView.pageIndex + 1;
                updateReceivingReceiptDetailReportNavigationButtons();
            });
        }

        // =========================================================
        // Update Receiving Receipt Detail Report Navigation Buttons
        // =========================================================
        function updateReceivingReceiptDetailReportNavigationButtons() {
            if (receivingReceiptDetailReportCollectionView.pageSize <= 0) {
                document.getElementById('receivingReceiptDetailReportNavigationPageGrid').style.display = 'none';
                return;
            }
            document.getElementById('receivingReceiptDetailReportNavigationPageGrid').style.display = 'block';
            if (receivingReceiptDetailReportCollectionView.pageIndex === 0) {
                btnReceivingReceiptDetailReportMoveToFirstPageGrid.setAttribute('disabled', 'disabled');
                btnReceivingReceiptDetailReportMoveToPreviousPageGrid.setAttribute('disabled', 'disabled');
                btnReceivingReceiptDetailReportMoveToNextPageGrid.removeAttribute('disabled');
                btnReceivingReceiptDetailReportMoveToLastPageGrid.removeAttribute('disabled');
            } else if (receivingReceiptDetailReportCollectionView.pageIndex === (receivingReceiptDetailReportCollectionView.pageCount - 1)) {
                btnReceivingReceiptDetailReportMoveToFirstPageGrid.removeAttribute('disabled');
                btnReceivingReceiptDetailReportMoveToPreviousPageGrid.removeAttribute('disabled');
                btnReceivingReceiptDetailReportMoveToLastPageGrid.setAttribute('disabled', 'disabled');
                btnReceivingReceiptDetailReportMoveToNextPageGrid.setAttribute('disabled', 'disabled');
            } else {
                btnReceivingReceiptDetailReportMoveToFirstPageGrid.removeAttribute('disabled');
                btnReceivingReceiptDetailReportMoveToPreviousPageGrid.removeAttribute('disabled');
                btnReceivingReceiptDetailReportMoveToNextPageGrid.removeAttribute('disabled');
                btnReceivingReceiptDetailReportMoveToLastPageGrid.removeAttribute('disabled');
            }
            btnReceivingReceiptDetailReportCurrentPageGrid.innerHTML = (receivingReceiptDetailReportCollectionView.pageIndex + 1) + ' / ' + receivingReceiptDetailReportCollectionView.pageCount;
        }

        // =================
        // Refresh Flex Grid
        // =================
        function refreshReceivingReceiptDetailReportFlexGrid() {
            receivingReceiptDetailReportCollectionView = new wijmo.collections.CollectionView(getReceivingReceiptDetailReportData());
            receivingReceiptDetailReportCollectionView.canFilter = true;
            receivingReceiptDetailReportCollectionView.pageSize = cboReceivingReceiptDetailReportShowNumberOfRows.selectedValue;

            var filterText = '';
            $('#inpReceivingReceiptDetailReportFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                receivingReceiptDetailReportCollectionView.refresh();
            });

            receivingReceiptDetailReportCollectionView.filter = function (item) {
                return !filterText || (item.RRNumber.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Supplier.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.PONumber.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemManualArticleOldCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemDescription.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Unit.toLowerCase().indexOf(filterText) > -1);
            }

            receivingReceiptDetailReportCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateReceivingReceiptDetailReportNavigationButtons();
            });

            receivingReceiptDetailReportFlexGrid.itemsSource = receivingReceiptDetailReportCollectionView;
            receivingReceiptDetailReportFlexGrid.trackChanges = true;
        }

        // ============
        // Download CSV
        // ============
        function btnReceivingReceiptDetailReportDownloadCSVOnclick() {
            var CSV = '';
            var receivingReceiptDetailReport = [];
            var fileName = 'ReceivingReceiptDetailReport.CSV';

            receivingReceiptDetailReportCollectionView.moveToFirstPage();

            for (var p = 1; p <= receivingReceiptDetailReportCollectionView.pageCount; p++) {
                for (i = 0; i < receivingReceiptDetailReportCollectionView.items.length; i++) {
                    receivingReceiptDetailReport.push({
                        Branch: receivingReceiptDetailReportCollectionView.items[i].Branch,
                        RRNumber: receivingReceiptDetailReportCollectionView.items[i].RRNumber,
                        RRDate: receivingReceiptDetailReportCollectionView.items[i].RRDate,
                        Supplier: receivingReceiptDetailReportCollectionView.items[i].Supplier,
                        Remarks: receivingReceiptDetailReportCollectionView.items[i].Remarks,
                        PONumber: receivingReceiptDetailReportCollectionView.items[i].PONumber,
                        Quantity: receivingReceiptDetailReportCollectionView.items[i].Quantity,
                        Barcode: receivingReceiptDetailReportCollectionView.items[i].ItemCode,
                        SKUCode: receivingReceiptDetailReportCollectionView.items[i].ItemManualArticleOldCode,
                        ItemDescription: receivingReceiptDetailReportCollectionView.items[i].ItemDescription,
                        Unit: receivingReceiptDetailReportCollectionView.items[i].Unit,
                        Cost: receivingReceiptDetailReportCollectionView.items[i].Cost,
                        Amount: receivingReceiptDetailReportCollectionView.items[i].Amount,
                        VAT: receivingReceiptDetailReportCollectionView.items[i].VAT,
                        WTAX: receivingReceiptDetailReportCollectionView.items[i].WTAX,
                    });
                }

                receivingReceiptDetailReportCollectionView.moveToNextPage();
                if (p == receivingReceiptDetailReportCollectionView.pageCount) {
                    receivingReceiptDetailReportCollectionView.moveToPage(currentPageIndex - 1);
                }
            }

            var Headers = [];
            Headers.push({
                Branch: "Branch",
                RRNumber: "RR Number",
                RRDate: "RR Date",
                Supplier: "Supplier",
                Remarks: "Remarks",
                PONumber: "PO Number",
                Quantity: "Quantity",
                Barcode: "Barcode",
                SKUCode: "SKU Code",
                ItemDescription: "Description",
                Unit: "Unit",
                Cost: "Cost",
                Amount: "Amount",
                VAT: "VAT",
                WTAX: "WTAX",
            });

            var header = '';
            for (var b in Headers[0]) {
                header += '"' + Headers[0][b] + '",';
            }

            CSV = header + '\r\n';
            for (var i = 0; i < receivingReceiptDetailReport.length; i++) {
                var row = '';
                for (var a in receivingReceiptDetailReport[i]) {
                    row += '"' + receivingReceiptDetailReport[i][a] + '",';
                }

                row.slice(0, row.length - 1);
                CSV += row + '\r\n';
            }

            if (CSV == '') {
                toastr.error("No data.");
            } else {
                var link = document.createElement("a");
                if (link.download !== undefined) {
                    var blob = new Blob([CSV], { type: 'text/csv;charset=utf-8;' });
                    var url = URL.createObjectURL(blob);
                    link.setAttribute("href", url);
                    link.setAttribute("download", fileName);
                    link.style = "visibility:hidden";
                }

                if (navigator.msSaveBlob) {
                    link.addEventListener("click", function (event) {
                        var blob = new Blob([CSV], {
                            "type": "text/csv;charset=utf-8;"
                        });
                        navigator.msSaveBlob(blob, fileName);
                    }, false);
                }

                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);

                toastr.success("Dowload successful.");
            }
        }

        // =========
        // Print PDF
