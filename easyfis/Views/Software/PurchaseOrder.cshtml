<!DOCTYPE html>
<html>
<head>
    <!-- Meta -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">

    <!-- Title of the Page -->
    <link rel="shortcut icon" href="~/Images/icon/easyfislogomini.ico">
    <title>Purchase Order</title>

    <!-- CSS StyleSheets-->
    @Styles.Render("~/Content/Software-css")
</head>
<body>
    <div id="o-wrapper" class="o-wrapper">
        <main class="o-content">
            @Html.Partial("_SoftwareHeader")
            <section class="content-header">
                <div class="container">
                    <h3>
                        Purchase Order
                    </h3>
                    <p>
                        <i class="fa fa-building-o fa-fw"></i> &nbsp; @ViewData["Company"] <br />
                        <i class="fa fa-home fa-fw"></i> &nbsp; <a href="#" onclick="getUserBranches()" style="color: white;">@ViewData["Branch"]</a>
                    </p>
                </div>
            </section>
            <section class="container">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 59%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                        <input type="text" class="form-control " id="InputFilter" placeholder="Search">
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 40%" align="right">
                                    <button class="btn btn-primary " id="btnAddPurchaseOrder" onclick="btnAddPurchaseOrder_OnClick()"><i class="fa fa-plus"></i> Add</button>
                                    <button class="btn btn-danger " id="btnAddClose" onclick="btnClosePurchaseOrder_OnClick()"><i class=" fa fa-times"></i> Close</button>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 29%">
                                    <div class="input-group ">
                                        <span class="input-group-addon">Start Date</span>
                                        <div id="cboStartDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 29%">
                                    <div class="input-group ">
                                        <span class="input-group-addon">End Date</span>
                                        <div id="cboEndDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 41%"></td>
                            </tr>
                        </table>
                    </div>
                    <div class="panel-body">
                        <div id="POGrid" class="grid" align="left"></div>
                    </div>
                    <div class="panel-footer">
                        <div class="row">
                            <div class="btn-group col-md-7" id="naviagtionPageGrid">
                                <button type="button" class="btn btn-default border-custom" id="btnMoveToFirstPageGrid">
                                    <span class="glyphicon glyphicon-fast-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnMoveToPreviousPageGrid">
                                    <span class="glyphicon glyphicon-step-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" disabled style="width: 100px" id="btnCurrentPageGrid"></button>
                                <button type="button" class="btn btn-default border-custom" id="btnMoveToNextPageGrid">
                                    <span class="glyphicon glyphicon-step-forward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnMoveToLastPageGrid">
                                    <span class="glyphicon glyphicon-fast-forward"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            @Html.Partial("_SoftwareFooter")
        </main>
    </div>
    <div id="c-mask" class="c-mask"></div>

    <!-- Purchase Order Delete Modal -->
    <div class="modal fade" id="deletePOConfirm" role="dialog">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Delete</h4>
                </div>
                <div class="modal-body">
                    Are you sure you want to delete this PO?
                </div>
                <div class="modal-footer">
                    <button id="cmdConfirmDeletePO" class="btn btn-danger" onclick="cmdPOConfirmDeleteButton_OnClick()"><i class="fa fa-trash"></i> Delete</button>
                    <button id="cmdConfirmDeleteCloseButton" class="btn btn-primary " data-dismiss="modal"><i class="fa fa-close"></i> Cancel</button>
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/Scripts/Software-js")
    <script type="text/javascript">
        // ================
        // Global Variables
        // ================
        var purchaseOrderCollectionView;
        var purchaseOrderFlexGrid;
        var btnFirstPageGrid;
        var btnPreviousPageGrid;
        var btnNextPageGrid;
        var btnLastPageGrid;
        var btnCurrentPageGrid;

        var cboStartDateFilter = new wijmo.input.InputDate('#cboStartDateFilter');
        var cboEndDateFilter = new wijmo.input.InputDate('#cboEndDateFilter');
        var isStartDateFirstSelect = true;
        var isEndDateFirstSelect = true;

        function cboDateFilters() {
            cboStartDateFilter.dispose();
            cboStartDateFilter = new wijmo.input.InputDate('#cboStartDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(),
                onValueChanged: function () {
                    if (!isStartDateFirstSelect) {
                        refreshPurchaseOrderFlexGrid();
                    } else {
                        isStartDateFirstSelect = false;
                    }
                }
            });

            cboEndDateFilter.dispose();
            cboEndDateFilter = new wijmo.input.InputDate('#cboEndDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(),
                onValueChanged: function () {
                    if (!isEndDateFirstSelect) {
                        refreshPurchaseOrderFlexGrid();
                    } else {
                        isEndDateFirstSelect = false;
                    }
                }
            });
        }

        // ==================
        // GET Purchase Order
        // ==================
        function getPurchaseOrderFilterByDates() {
            NProgress.start();
            var startDate = cboStartDateFilter.value.toLocaleDateString().replace(/\//g, '-');
            var endDate = cboEndDateFilter.value.toLocaleDateString().replace(/\//g, '-');

            var purchaseOrderObservableArray = new wijmo.collections.ObservableArray;
            $.ajax({
                url: '/api/listPurchaseOrderFilterByPODate/' + startDate + '/' + endDate,
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (purchaseOrderListResults) {
                    if (purchaseOrderListResults.length > 0) {
                        for (i = 0; i < purchaseOrderListResults.length; i++) {

                            var canDelete = "@ViewData["CanDelete"]";
                            var deleteDisabled = "";
                            if (canDelete == "False") {
                                deleteDisabled = "disabled";
                            }

                            var canEdit = "@ViewData["CanEdit"]";
                            var editDisabled = "";
                            if (canEdit == "False") {
                                editDisabled = "disabled";
                            }

                            var PODateResult = new Date(purchaseOrderListResults[i]["PODate"])
                            var PODate = ('0' + (PODateResult.getMonth() + 1)).slice(-2) + '-' + ('0' + PODateResult.getDate()).slice(-2) + '-' + PODateResult.getFullYear();

                            purchaseOrderObservableArray.push({
                                EditId: "<button class='btn btn-primary btn-xs btn-block' id='POEditButton' onclick='POEditButton_OnClick()' " + editDisabled + "><i class='fa fa-edit'></i> Edit</button>",
                                DeleteId: "<button class='btn btn-danger btn-xs btn-block' onclick='PODeleteButton_OnClick()' " + deleteDisabled + "><i class='fa fa-trash'></i> Delete</button>",
                                Id: purchaseOrderListResults[i]["Id"],
                                BranchId: purchaseOrderListResults[i]["BranchId"],
                                Branch: purchaseOrderListResults[i]["Branch"],
                                PONumber: purchaseOrderListResults[i]["PONumber"],
                                PODate: PODate,
                                SupplierId: purchaseOrderListResults[i]["SupplierId"],
                                Supplier: purchaseOrderListResults[i]["Supplier"],
                                TermId: purchaseOrderListResults[i]["TermId"],
                                Term: purchaseOrderListResults[i]["Term"],
                                ManualRequestNumber: purchaseOrderListResults[i]["ManualRequestNumber"],
                                ManualPONumber: purchaseOrderListResults[i]["ManualPONumber"],
                                DateNeeded: purchaseOrderListResults[i]["DateNeeded"],
                                Remarks: purchaseOrderListResults[i]["Remarks"],
                                IsClose: purchaseOrderListResults[i]["IsClose"],
                                Amount: purchaseOrderListResults[i]["Amount"],
                                RequestedById: purchaseOrderListResults[i]["RequestedById"],
                                RequestedBy: purchaseOrderListResults[i]["RequestedBy"],
                                PreparedById: purchaseOrderListResults[i]["PreparedById"],
                                PreparedBy: purchaseOrderListResults[i]["PreparedBy"],
                                CheckedById: purchaseOrderListResults[i]["CheckedById"],
                                CheckedBy: purchaseOrderListResults[i]["CheckedBy"],
                                ApprovedById: purchaseOrderListResults[i]["ApprovedById"],
                                ApprovedBy: purchaseOrderListResults[i]["ApprovedBy"],
                                IsLocked: purchaseOrderListResults[i]["IsLocked"],
                                CreatedById: purchaseOrderListResults[i]["CreatedById"],
                                CreatedBy: purchaseOrderListResults[i]["CreatedBy"],
                                CreatedDateTime: purchaseOrderListResults[i]["CreatedDateTime"],
                                UpdatedById: purchaseOrderListResults[i]["UpdatedById"],
                                UpdatedBy: purchaseOrderListResults[i]["UpdatedBy"],
                                UpdatedDateTime: purchaseOrderListResults[i]["UpdatedDateTime"]
                            });
                        }
                    }

                    NProgress.done();
                }
            });

            return purchaseOrderObservableArray;
        }

        // =========================
        // Purchase Order Add Button
        // =========================
        function btnAddPurchaseOrder_OnClick() {
            document.getElementById('btnAddPurchaseOrder').innerHTML = "<i class='fa fa-spinner fa-spin'></i> Adding";
            $('#btnAddPurchaseOrder').prop('disabled', true);
            $('#btnAddClose').prop('disabled', true);

            var canAccessToDetailPage = "@ViewData["CanAccessToDetailPage"]";
            if (canAccessToDetailPage == "True") {
                $.ajax({
                    type: "POST",
                    url: '/api/addPurchaseOrder',
                    contentType: "application/json; charset=utf-8",
                    success: function (id) {
                        if (id > 0) {
                            window.location = '/Software/PurchaseOrderDetail?PurchaseOrderId=' + id;
                        } else {
                            toastr.error("Internal Server Error");
                            document.getElementById('btnAddPurchaseOrder').innerHTML = "<i class='fa fa-plus'></i> Add";
                            $('#btnAddPurchaseOrder').prop('disabled', false);
                            $('#btnAddClose').prop('disabled', false);
                        }
                    }
                });
            } else {
                if (canAccessToDetailPage == "False") {
                    window.location = '/Software/Forbidden';
                }
            }
        }

        // ==============
        // PO Edit Button
        // ==============
        function POEditButton_OnClick() {
            purchaseOrderCollectionView.editItem(purchaseOrderCollectionView.currentItem);
            var PO = purchaseOrderCollectionView.currentEditItem;
            var PurchaseOrderId = PO.Id;
            window.location = '/Software/PurchaseOrderDetail?PurchaseOrderId=' + PurchaseOrderId;
        }

        // ===========================
        // Purchase Order Close Button
        // ===========================
        function btnClosePurchaseOrder_OnClick() {
            window.location = '/Software';
        }

        // ================
        // PO Delete Button
        // ================
        function PODeleteButton_OnClick() {
            purchaseOrderCollectionView.editItem(purchaseOrderCollectionView.currentItem);
            var PO = purchaseOrderCollectionView.currentEditItem;
            if (PO.IsLocked) {
                toastr.error("Delete not allowed when the selected record is locked!");
            } else {
                $('#deletePOConfirm').modal({
                    show: true,
                    backdrop: 'static'
                });

                document.getElementById('cmdConfirmDeletePO').innerHTML = "<i class='fa fa-trash'></i> Delete";
                $("#cmdConfirmDeletePO").prop("disabled", false);
                $("#cmdConfirmDeleteCloseButton").prop("disabled", false);
            }
        }

        // PO Confirm Delete Button
        function cmdPOConfirmDeleteButton_OnClick() {
            document.getElementById('cmdConfirmDeletePO').innerHTML = "<i class='fa fa-spinner fa-spin'></i> Deleting";
            $("#cmdConfirmDeletePO").prop("disabled", true);
            $("#cmdConfirmDeleteCloseButton").prop("disabled", true);

            purchaseOrderCollectionView.editItem(purchaseOrderCollectionView.currentItem);
            var id = purchaseOrderCollectionView.currentEditItem.Id;
            $.ajax({
                url: '/api/deletePO/' + id,
                type: "DELETE",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                statusCode: {
                    200: function () {
                        toastr.success("Delete Successful");
                        $('#deletePOConfirm').modal('hide');
                        refreshPurchaseOrderFlexGrid();
                    },
                    404: function () {
                        toastr.error("Not Found");
                        $('#deletePOConfirm').modal('hide');
                        document.getElementById('cmdConfirmDeletePO').innerHTML = "<i class='fa fa-trash'></i> Delete";
                        $("#cmdConfirmDeletePO").prop("disabled", false);
                        $("#cmdConfirmDeleteCloseButton").prop("disabled", false);
                    },
                    400: function () {
                        toastr.error("Bad Request");
                        $('#deletePOConfirm').modal('hide');
                        document.getElementById('cmdConfirmDeletePO').innerHTML = "<i class='fa fa-trash'></i> Delete";
                        $("#cmdConfirmDeletePO").prop("disabled", false);
                        $("#cmdConfirmDeleteCloseButton").prop("disabled", false);
                    }
                }
            });
        }

        // ===================
        // FlexGrid Selection
        // ===================
        function updateDetails() {
            var item = purchaseOrderCollectionView.currentItem;
            document.getElementById('createdBy').innerHTML = item.CreatedBy;
            document.getElementById('createdDate').innerHTML = item.CreatedDateTime;
            document.getElementById('updatedBy').innerHTML = item.UpdatedBy;
            document.getElementById('updatedDate').innerHTML = item.UpdatedDateTime;
        }

        // ===============================
        // Navigation Buttons for Comapany
        // ===============================
        function updateNavigateButtonsPO() {
            if (purchaseOrderCollectionView.pageSize <= 0) {
                document.getElementById('naviagtionPageGrid').style.display = 'none';
                return;
            }
            document.getElementById('naviagtionPageGrid').style.display = 'block';
            if (purchaseOrderCollectionView.pageIndex === 0) {
                btnFirstPageGrid.setAttribute('disabled', 'disabled');
                btnPreviousPageGrid.setAttribute('disabled', 'disabled');
                btnNextPageGrid.removeAttribute('disabled');
                btnLastPageGrid.removeAttribute('disabled');
            } else if (purchaseOrderCollectionView.pageIndex === (purchaseOrderCollectionView.pageCount - 1)) {
                btnFirstPageGrid.removeAttribute('disabled');
                btnPreviousPageGrid.removeAttribute('disabled');
                btnLastPageGrid.setAttribute('disabled', 'disabled');
                btnNextPageGrid.setAttribute('disabled', 'disabled');
            } else {
                btnFirstPageGrid.removeAttribute('disabled');
                btnPreviousPageGrid.removeAttribute('disabled');
                btnNextPageGrid.removeAttribute('disabled');
                btnLastPageGrid.removeAttribute('disabled');
            }
            btnCurrentPageGrid.innerHTML = (purchaseOrderCollectionView.pageIndex + 1) + ' / ' + purchaseOrderCollectionView.pageCount;
        }

        // ===============================
        // Refresh Purchase Order FlexGrid
        // ===============================
        function refreshPurchaseOrderFlexGrid() {
            purchaseOrderCollectionView = new wijmo.collections.CollectionView(getPurchaseOrderFilterByDates());
            purchaseOrderCollectionView.canFilter = true;
            purchaseOrderCollectionView.pageSize = 15;

            var filterText = '';
            $('#InputFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                purchaseOrderCollectionView.refresh();
            });

            purchaseOrderCollectionView.filter = function (item) {
                return !filterText || (item.PONumber.toLowerCase().indexOf(filterText) > -1)
                    || (item.Supplier.toLowerCase().indexOf(filterText) > -1);
            }

            purchaseOrderCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateNavigateButtonsPO();
            });

            purchaseOrderCollectionView.currentChanged.addHandler(function (sender, args) {
                updateDetails();
            });

            purchaseOrderFlexGrid.itemsSource = purchaseOrderCollectionView;
            purchaseOrderFlexGrid.trackChanges = true;
        }

        // =======================
        // Purchase Order FlexGrid
        // =======================
        function createPurchaseOrderFlexGrid() {
            cboDateFilters();
            purchaseOrderCollectionView = new wijmo.collections.CollectionView(getPurchaseOrderFilterByDates());
            purchaseOrderCollectionView.canFilter = true;
            purchaseOrderCollectionView.pageSize = 15;

            var filterText = '';
            $('#InputFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                purchaseOrderCollectionView.refresh();
            });

            purchaseOrderCollectionView.filter = function (item) {
                return !filterText || (item.PONumber.toLowerCase().indexOf(filterText) > -1)
                                   || (item.Supplier.toLowerCase().indexOf(filterText) > -1);
            }

            purchaseOrderCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateNavigateButtonsPO();
            });

            purchaseOrderCollectionView.currentChanged.addHandler(function (sender, args) {
                updateDetails();
            });

            purchaseOrderFlexGrid = new wijmo.grid.FlexGrid('#POGrid');
            purchaseOrderFlexGrid.initialize({
                columns: [
                            {
                                "header": "Edit",
                                "binding": "EditId",
                                "width": 70,
                                "align": "center",
                                "allowResizing": false,
                                "allowSorting": false,
                                "isContentHtml": true
                            },
                            {
                                "header": "Delete",
                                "binding": "DeleteId",
                                "width": 70,
                                "align": "center",
                                "allowResizing": false,
                                "allowSorting": false,
                                "isContentHtml": true
                            },
                            {
                                "header": "PO Number",
                                "binding": "PONumber",
                                "allowSorting": true,
                                "width": "1.5*"
                            },
                            {
                                "header": "PO Date",
                                "binding": "PODate",
                                "allowSorting": true,
                                "width": "1.5*"
                            },
                            {
                                "header": "Supplier",
                                "binding": "Supplier",
                                "allowSorting": true,
                                "width": "4*"
                            },
                            {
                                "header": "Remarks",
                                "binding": "Remarks",
                                "allowSorting": true,
                                "width": "2.5*"
                            },
                            {
                                "header": "Amount",
                                "binding": "Amount",
                                "allowSorting": true,
                                "width": "2*"
                            },
                            {
                                "header": "C",
                                "binding": "IsClose",
                                "allowSorting": true,
                                "width": "0.5*"
                            },
                            {
                                "header": "L",
                                "binding": "IsLocked",
                                "allowSorting": true,
                                "width": "0.5*"
                            }
                ],
                autoGenerateColumns: false,
                itemsSource: purchaseOrderCollectionView,
                isReadOnly: true,
                autoSizeMode: wijmo.grid.AutoSizeMode.Both,
                allowDragging: wijmo.grid.AllowDragging.None,
                selectionMode: wijmo.grid.SelectionMode.Row
            });

            purchaseOrderFlexGrid.trackChanges = true;

            btnFirstPageGrid = document.getElementById('btnMoveToFirstPageGrid');
            btnPreviousPageGrid = document.getElementById('btnMoveToPreviousPageGrid');
            btnNextPageGrid = document.getElementById('btnMoveToNextPageGrid');
            btnLastPageGrid = document.getElementById('btnMoveToLastPageGrid');
            btnCurrentPageGrid = document.getElementById('btnCurrentPageGrid');

            updateNavigateButtonsPO();

            btnFirstPageGrid.addEventListener('click', function () {
                purchaseOrderCollectionView.moveToFirstPage();
                updateNavigateButtonsPO();
            });

            btnPreviousPageGrid.addEventListener('click', function () {
                purchaseOrderCollectionView.moveToPreviousPage();
                updateNavigateButtonsPO();
            });

            btnNextPageGrid.addEventListener('click', function () {
                purchaseOrderCollectionView.moveToNextPage();
                updateNavigateButtonsPO();
            });

            btnLastPageGrid.addEventListener('click', function () {
                purchaseOrderCollectionView.moveToLastPage();
                updateNavigateButtonsPO();
            });

            var canAdd = "@ViewData["CanAdd"]";
            if (canAdd == "False") {
                $("#btnAddPurchaseOrder").prop("disabled", true)
            }
        }

        // ============
        // On Load Page
        // ============
        $(document).ready(function () {
            createPurchaseOrderFlexGrid();
        });
    </script>
    <script type="text/javascript">
        var cboUserBranches = new wijmo.input.ComboBox('#cboUserBranches');
        function getUserBranches() {
            $("#loadingUserBranches").show();
            document.getElementById("loadingUserBranches").innerHTML = '<i class="fa fa-spinner fa-spin"></i>';
            $("#userBranchContent").hide();

            $('#userBranchesModal').modal({ show: true, backdrop: 'static' });

            document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
            $("#updateUserBranch").prop("disabled", false);
            $("#closeUserBranch").prop("disabled", false);

            var userBranchesObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/userBranch/list',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (userBranchesResults) {
                    if (userBranchesResults.length > 0) {
                        for (i = 0; i < userBranchesResults.length; i++) {
                            userBranchesObservableArray.push({
                                BranchId: userBranchesResults[i]["BranchId"],
                                Branch: userBranchesResults[i]["Branch"]
                            });
                        }
                    }

                    var currentBranch = "@ViewData["BranchId"]";
                    cboUserBranches.dispose();
                    cboUserBranches = new wijmo.input.ComboBox('#cboUserBranches', {
                        isEditable: false,
                        placeholder: "Select Branches",
                        itemsSource: userBranchesObservableArray,
                        displayMemberPath: "Branch",
                        selectedValuePath: "BranchId",
                        selectedValue: currentBranch
                    });

                    setTimeout(function () {
                        $("#loadingUserBranches").hide();
                        $("#userBranchContent").show();
                    }, 500)
                }
            });
        }

        function updateUserBranchOnclick() {
            document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-spinner fa-spin'></i> Updating";
            $("#updateUserBranch").prop("disabled", true);
            $("#closeUserBranch").prop("disabled", true);

            var userBranchObject = new Object();
            userBranchObject.BranchId = cboUserBranches.selectedValue;
            var userBranchData = JSON.stringify(userBranchObject);

            $.ajax({
                type: "PUT",
                url: '/api/userBranch/update',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: userBranchData,
                statusCode: {
                    200: function () {
                        toastr.success("Update Successful");
                        window.setTimeout(function () {
                            location.reload()
                        }, 1000);
                    },
                    404: function () {
                        toastr.error("Not Found");
                        document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
                        $("#updateUserBranch").prop("disabled", false);
                        $("#closeUserBranch").prop("disabled", false);
                    },
                    500: function () {
                        toastr.error("Bad Request");
                        document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
                        $("#updateUserBranch").prop("disabled", false);
                        $("#closeUserBranch").prop("disabled", false);
                    }
                }
            });
        }
    </script>
    <script type="text/javascript">
        var slideLeft = new Menu({
            wrapper: '#o-wrapper',
            type: 'slide-left',
            menuOpenerClass: '.c-button',
            maskId: '#c-mask'
        });

        var slideLeftBtn = document.querySelector('#c-button--slide-left');

        slideLeftBtn.addEventListener('click', function (e) {
            e.preventDefault;
            slideLeft.open();
        });
    </script>
</body>
</html>