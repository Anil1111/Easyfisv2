<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="shortcut icon" href="~/Images/icon/easyfislogomini.ico">
    <title>Stock Out Detail Report</title>

    @Styles.Render("~/Content/Software-css")
</head>
<body>
    <div id="o-wrapper" class="o-wrapper">
        <main class="o-content">
            @Html.Partial("_SoftwareHeader")

            <section class="content-header">
                <div class="container">
                    <h3>
                        Stock Out Detail Report
                    </h3>
                    <p>
                        <i class="fa fa-building-o fa-fw"></i> &nbsp; @ViewData["Company"] <br />
                        <i class="fa fa-home fa-fw"></i> &nbsp; <a href="#" onclick="getUserBranches()" style="color: white;">@ViewData["Branch"]</a>
                    </p>
                </div>
            </section>
            <section class="container">
                <div class="panel panel-default">

                    <!--
                        ===============
                        Search / Filter
                        ===============
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 49.5%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                        <input type="text" class="form-control" id="inpStockOutDetailReportFilter" placeholder="Search...">
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group">
                                        <span class="input-group-addon"><b><i class="fa fa-table"></i></b></span>
                                        <div class="comboBox-wide" id="cboStockOutDetailReportShowNumberOfRows"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24.5%" align="right">
                                    <button onclick="btnStockOutDetailReportDownloadCSVOnclick()" id="btnStockOutDetailReportDownloadCSV" class="btn btn-success"><i class="fa fa-file-excel-o" aria-hidden="true"></i> CSV</button>
                                    <button onclick="btnStockOutDetailReportPDFOnclick()" id="btnStockOutDetailReportPDF" class="btn btn-primary"><i class="fa fa-print"></i> Print</button>
                                    <a href="/Software/AccountsReceivable" class="btn btn-danger"><i class="fa fa-close"></i> Close</a>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        ======================================
                        Date Range, Company and Branch Filters
                        ======================================
                    -->
                    <div class="panel-heading">
                        <table style="width: 100%" border="0">
                            <tr>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Start Date</b></span>
                                        <div class="comboBox-wide" id="cboStockOutDetailReportStartDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%" align="right">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>End Date</b></span>
                                        <div class="comboBox-wide" id="cboStockOutDetailReportEndDateFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Company</b></span>
                                        <div class="comboBox-wide" id="cboStockOutDetailReportCompanyFilter"></div>
                                    </div>
                                </td>
                                <td style="width: 1%"></td>
                                <td style="width: 24%">
                                    <div class="input-group ">
                                        <span class="input-group-addon"><b>Branch</b></span>
                                        <div class="comboBox-wide" id="cboStockOutDetailReportBranchFilter"></div>
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--
                        =========
                        Flex Grid
                        =========
                    -->
                    <div class="panel-body">
                        <div id="stockOutDetailReportFlexGrid"></div>
                    </div>

                    <!--
                        ==================
                        Navigation Buttons
                        ==================
                    -->
                    <div class="panel-footer">
                        <div class="row">
                            <div class="btn-group col-md-7" id="stockOutDetailReportNavigationPageGrid">
                                <button type="button" class="btn btn-default border-custom" id="btnStockOutDetailReportMoveToFirstPageGrid">
                                    <span class="glyphicon glyphicon-fast-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnStockOutDetailReportMoveToPreviousPageGrid">
                                    <span class="glyphicon glyphicon-step-backward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" disabled style="width: 100px" id="btnStockOutDetailReportCurrentPageGrid"></button>
                                <button type="button" class="btn btn-default border-custom" id="btnStockOutDetailReportMoveToNextPageGrid">
                                    <span class="glyphicon glyphicon-step-forward"></span>
                                </button>
                                <button type="button" class="btn btn-default border-custom" id="btnStockOutDetailReportMoveToLastPageGrid">
                                    <span class="glyphicon glyphicon-fast-forward"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <!--
                    ======
                    Totals
                    ======
                -->
                <div class="panel panel-default">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-6"></div>
                            <div class="col-md-6">
                                <dl class="dl-horizontal dl-custom">
                                    <dt>
                                        <b>Total Amount</b>
                                    </dt>
                                    <dd>
                                        <input type="text" class="form-control numberField" id="totalAmount" placeholder="0.00" disabled />
                                    </dd>
                                </dl>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            @Html.Partial("_SoftwareFooter")
        </main>
    </div>
    <div id="c-mask" class="c-mask"></div>

    @Scripts.Render("~/Scripts/Software-js")
    <script type="text/javascript">
        // ============
        // Date Filters
        // ============
        var cboStockOutDetailReportStartDateFilter = new wijmo.input.InputDate('#cboStockOutDetailReportStartDateFilter');
        var cboStockOutDetailReportEndDateFilter = new wijmo.input.InputDate('#cboStockOutDetailReportEndDateFilter');
        var isStockOutDetailReportStartDateSelected = false;
        var isStockOutDetailReportEndDateSelected = false;

        // =========================
        // Company and Branch Filter
        // =========================
        var cboStockOutDetailReportCompanyFilter = new wijmo.input.ComboBox('#cboStockOutDetailReportCompanyFilter');
        var cboStockOutDetailReportBranchFilter = new wijmo.input.ComboBox('#cboStockOutDetailReportBranchFilter');
        var isCboStockOutDetailReportCompanySelected = false;

        // =============================
        // Collection View and Flex Grid
        // =============================
        var stockOutDetailReportCollectionView;
        var stockOutDetailReportFlexGrid;
        var cboStockOutDetailReportShowNumberOfRows = new wijmo.input.ComboBox('#cboStockOutDetailReportShowNumberOfRows');

        // ==================
        // Navigation Buttons
        // ==================
        var btnStockOutDetailReportMoveToFirstPageGrid = document.getElementById('btnStockOutDetailReportMoveToFirstPageGrid');
        var btnStockOutDetailReportMoveToPreviousPageGrid = document.getElementById('btnStockOutDetailReportMoveToPreviousPageGrid');
        var btnStockOutDetailReportMoveToNextPageGrid = document.getElementById('btnStockOutDetailReportMoveToNextPageGrid');
        var btnStockOutDetailReportMoveToLastPageGrid = document.getElementById('btnStockOutDetailReportMoveToLastPageGrid');
        var btnStockOutDetailReportCurrentPageGrid = document.getElementById('btnStockOutDetailReportCurrentPageGrid');
        var currentPageIndex = 0;

        // =======================
        // Get URL Parameter Value
        // =======================
        function getURLParameterValue(name) {
            name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");

            var regexS = "[\\?&]" + name + "=([^&#]*)";
            var regex = new RegExp(regexS);

            var results = regex.exec(window.location.href);
            if (results == null) {
                return "";
            } else {
                return results[1];
            }
        }

        // ================================
        // Create Combo Date Ranged Filters
        // ================================
        function createCboDateRangeFilters() {
            NProgress.start();

            cboStockOutDetailReportStartDateFilter.dispose();
            cboStockOutDetailReportStartDateFilter = new wijmo.input.InputDate('#cboStockOutDetailReportStartDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameterValue("StartDate")),
                onValueChanged: function () {
                    if (!isStockOutDetailReportStartDateSelected) {
                        NProgress.start();
                        refreshStockOutDetailReportFlexGrid();
                    } else {
                        isStockOutDetailReportStartDateSelected = false;
                    }
                }
            });

            cboStockOutDetailReportEndDateFilter.dispose();
            cboStockOutDetailReportEndDateFilter = new wijmo.input.InputDate('#cboStockOutDetailReportEndDateFilter', {
                format: 'MM-dd-yyyy',
                value: new Date(getURLParameterValue("EndDate")),
                onValueChanged: function () {
                    if (!isStockOutDetailReportEndDateSelected) {
                        NProgress.start();
                        refreshStockOutDetailReportFlexGrid();
                    } else {
                        isStockOutDetailReportEndDateSelected = false;
                    }
                }
            });

            createCboCompanyFilter();
        }

        // ===========================
        // Create Combo Company Filter
        // ===========================
        function createCboCompanyFilter() {
            var companyObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/stockOutDetailReport/dropdown/list/company',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            companyObservableArray.push({
                                Id: results[i]["Id"],
                                Company: results[i]["Company"]
                            });
                        }
                    }

                    var disabled = true;
                    if (results.length > 0) {
                        disabled = false;
                    } else {
                        $("#btnStockOutDetailReportDownloadCSV").prop("disabled", true);
                        $("#btnStockOutDetailReportPDF").prop("disabled", true);
                    }

                    cboStockOutDetailReportCompanyFilter.dispose();
                    cboStockOutDetailReportCompanyFilter = new wijmo.input.ComboBox('#cboStockOutDetailReportCompanyFilter', {
                        disabled: disabled,
                        isEditable: false,
                        itemsSource: companyObservableArray,
                        displayMemberPath: "Company",
                        selectedValuePath: "Id",
                        selectedValue: getURLParameterValue("CompanyId"),
                        onSelectedIndexChanged: function () {
                            NProgress.start();
                            $("#btnStockOutDetailReportDownloadCSV").prop("disabled", false);
                            $("#btnStockOutDetailReportPDF").prop("disabled", false);
                            isCboStockOutDetailReportCompanySelected = true;
                            createCboBranchFilter();
                        }
                    });

                    createCboBranchFilter();
                }
            });
        }

        // ==========================
        // Create Combo Branch Filter
        // ==========================
        function createCboBranchFilter() {
            var branchObservableArray = new wijmo.collections.ObservableArray();
            if (cboStockOutDetailReportCompanyFilter.selectedValue != null) {
                $.ajax({
                    url: '/api/stockOutDetailReport/dropdown/list/branch/' + cboStockOutDetailReportCompanyFilter.selectedValue,
                    cache: false,
                    type: 'GET',
                    contentType: 'application/json; charset=utf-8',
                    success: function (branchResults) {
                        if (branchResults.length > 0) {
                            for (i = 0; i < branchResults.length; i++) {
                                branchObservableArray.push({
                                    Id: branchResults[i]["Id"],
                                    Branch: branchResults[i]["Branch"]
                                });
                            }
                        }

                        var disabled = true;
                        if (branchResults.length > 0) {
                            disabled = false;
                        } else {
                            $("#btnStockOutDetailReportDownloadCSV").prop("disabled", true);
                            $("#btnStockOutDetailReportPDF").prop("disabled", true);
                        }

                        cboStockOutDetailReportBranchFilter.dispose();
                        cboStockOutDetailReportBranchFilter = new wijmo.input.ComboBox('#cboStockOutDetailReportBranchFilter', {
                            disabled: disabled,
                            isEditable: false,
                            itemsSource: branchObservableArray,
                            displayMemberPath: "Branch",
                            selectedValuePath: "Id",
                            selectedValue: getURLParameterValue("BranchId"),
                            onSelectedIndexChanged: function () {
                                NProgress.start();
                                refreshStockOutDetailReportFlexGrid();
                            }
                        });

                        createCboShowRows();
                    }
                });
            }
        }

        // ===================================
        // Create Combo Display Number of Rows
        // ===================================
        function createCboShowRows() {
            var rowsObservableArray = new wijmo.collections.ObservableArray();

            for (var i = 0; i <= 4; i++) {
                var rows = 0;
                var rowsString = "";

                if (i == 0) {
                    rows = 15;
                    rowsString = "Show 15 Rows";
                } else if (i == 1) {
                    rows = 50;
                    rowsString = "Show 50 Rows";
                } else if (i == 2) {
                    rows = 100;
                    rowsString = "Show 100 Rows";
                } else if (i == 3) {
                    rows = 150;
                    rowsString = "Show 150 Rows";
                } else {
                    rows = 200;
                    rowsString = "Show 200 Rows";
                }

                rowsObservableArray.push({
                    rowNumber: rows,
                    rowString: rowsString
                });
            }

            cboStockOutDetailReportShowNumberOfRows.dispose();
            cboStockOutDetailReportShowNumberOfRows = new wijmo.input.ComboBox('#cboStockOutDetailReportShowNumberOfRows', {
                isEditable: false,
                itemsSource: rowsObservableArray,
                displayMemberPath: "rowString",
                selectedValuePath: "rowNumber",
                selectedValue: 50,
                onSelectedIndexChanged: function () {
                    NProgress.start();

                    setTimeout(function () {
                        var numberOfRows = cboStockOutDetailReportShowNumberOfRows.selectedValue;
                        stockOutDetailReportCollectionView.pageSize = parseInt(numberOfRows);
                        stockOutDetailReportCollectionView.refresh();
                        stockOutDetailReportFlexGrid.refresh();

                        NProgress.done();
                    }, 300);
                }
            });

            if (!isCboStockOutDetailReportCompanySelected) {
                createStockOutDetailReportFlexGrid();
            } else {
                refreshStockOutDetailReportFlexGrid();
            }
        }

        // ================================
        // Get Stock Out Detail Report Data
        // ================================
        function getStockOutDetailReportData() {
            var stockOutDetailReports = new wijmo.collections.ObservableArray;
            if (cboStockOutDetailReportCompanyFilter.selectedValue != null) {
                if (cboStockOutDetailReportBranchFilter.selectedValue != null) {

                    var startDate = cboStockOutDetailReportStartDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var endDate = cboStockOutDetailReportEndDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var companyId = cboStockOutDetailReportCompanyFilter.selectedValue;
                    var branchId = cboStockOutDetailReportBranchFilter.selectedValue;

                    $.ajax({
                        url: '/api/stockOutDetailReport/list/' + startDate + '/' + endDate + '/' + +companyId + '/' + branchId,
                        cache: false,
                        type: 'GET',
                        contentType: 'application/json; charset=utf-8',
                        success: function (stockOutDetailReportResults) {
                            var totalAmount = 0;
                            if (stockOutDetailReportResults.length > 0) {
                                for (i = 0; i < stockOutDetailReportResults.length; i++) {
                                    var docLink = "/Software/StockOutDetail?id=" + stockOutDetailReportResults[i]["OTId"];

                                    var OTDateResult = new Date(stockOutDetailReportResults[i]["OTDate"])
                                    var OTDate = ('0' + (OTDateResult.getMonth() + 1)).slice(-2) + '-' + ('0' + OTDateResult.getDate()).slice(-2) + '-' + OTDateResult.getFullYear();

                                    stockOutDetailReports.push({
                                        OTId: stockOutDetailReportResults[i]["OTId"],
                                        Id: stockOutDetailReportResults[i]["Id"],
                                        OT: stockOutDetailReportResults[i]["OT"],
                                        OTForCSV: "OT-" + stockOutDetailReportResults[i]["OT"],
                                        OTButton: "<a href='" + docLink + "' class='btn btn-primary btn-xs btn-block'>" + stockOutDetailReportResults[i]["OT"],
                                        OTDate: OTDate,
                                        ItemId: stockOutDetailReportResults[i]["ItemId"],
                                        ItemCode: stockOutDetailReportResults[i]["ItemCode"],
                                        ItemManualArticleOldCode: stockOutDetailReportResults[i]["ItemManualArticleOldCode"],
                                        Item: stockOutDetailReportResults[i]["Item"],
                                        Particulars: stockOutDetailReportResults[i]["Particulars"],
                                        UnitId: stockOutDetailReportResults[i]["UnitId"],
                                        Unit: stockOutDetailReportResults[i]["Unit"],
                                        Quantity: stockOutDetailReportResults[i]["Quantity"],
                                        QuantityDisplay: parseFloat(stockOutDetailReportResults[i]["Quantity"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        Cost: stockOutDetailReportResults[i]["Cost"],
                                        CostDisplay: parseFloat(stockOutDetailReportResults[i]["Cost"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        Amount: stockOutDetailReportResults[i]["Amount"],
                                        AmountDisplay: parseFloat(stockOutDetailReportResults[i]["Amount"]).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ","),
                                        BaseUnitId: stockOutDetailReportResults[i]["BaseUnitId"],
                                        BaseQuantity: stockOutDetailReportResults[i]["BaseQuantity"],
                                        BaseCost: stockOutDetailReportResults[i]["BaseCost"],
                                    });

                                    totalAmount += stockOutDetailReportResults[i]["Amount"];
                                }
                            }

                            document.getElementById("totalAmount").value = parseFloat(totalAmount).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                            NProgress.done();
                        }
                    });
                } else {
                    document.getElementById("totalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    NProgress.done();
                }
            } else {
                document.getElementById("totalAmount").value = parseFloat(0).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                NProgress.done();
            }

            return stockOutDetailReports;
        }

        // ========================================
        // Create Stock Out Detail Report Flex Grid
        // ========================================
        function createStockOutDetailReportFlexGrid() {
            stockOutDetailReportCollectionView = new wijmo.collections.CollectionView(getStockOutDetailReportData());
            stockOutDetailReportCollectionView.canFilter = true;
            stockOutDetailReportCollectionView.pageSize = cboStockOutDetailReportShowNumberOfRows.selectedValue;

            var filterText = '';
            $('#inpStockOutDetailReportFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                stockOutDetailReportCollectionView.refresh();
            });

            stockOutDetailReportCollectionView.filter = function (item) {
                return !filterText || (item.OT.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemManualArticleOldCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Item.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Unit.toLowerCase().indexOf(filterText) > -1);
            }

            stockOutDetailReportCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateStockOutDetailReportNavigationButtons();
            });

            stockOutDetailReportFlexGrid = new wijmo.grid.FlexGrid('#stockOutDetailReportFlexGrid');
            stockOutDetailReportFlexGrid.initialize({
                columns: [
                    {
                        "header": "OT Number  ",
                        "binding": "OTButton",
                        "isContentHtml": true,
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "OT Date",
                        "binding": "OTDate",
                        "allowSorting": true,
                        "width": "1.5*"
                    },
                    {
                        "header": "Barcode",
                        "binding": "ItemCode",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "SKU Code",
                        "binding": "ItemManualArticleOldCode",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Item",
                        "binding": "Item",
                        "allowSorting": true,
                        "width": "4*"
                    },
                    {
                        "header": "Quantity",
                        "binding": "QuantityDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1.5*"
                    },
                    {
                        "header": "Unit",
                        "binding": "Unit",
                        "allowSorting": true,
                        "width": "2*"
                    },
                    {
                        "header": "Cost",
                        "binding": "CostDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1.5*"
                    },
                    {
                        "header": "Amount",
                        "binding": "AmountDisplay",
                        "allowSorting": true,
                        "align": "right",
                        "width": "1.5*"
                    }
                ],
                autoGenerateColumns: false,
                itemsSource: stockOutDetailReportCollectionView,
                isReadOnly: true,
                autoSizeMode: wijmo.grid.AutoSizeMode.Both,
                allowDragging: wijmo.grid.AllowDragging.None,
                selectionMode: wijmo.grid.SelectionMode.Row
            });

            stockOutDetailReportFlexGrid.trackChanges = true;

            updateStockOutDetailReportNavigationButtons();

            btnStockOutDetailReportMoveToFirstPageGrid.addEventListener('click', function () {
                stockOutDetailReportCollectionView.moveToFirstPage();
                currentPageIndex = stockOutDetailReportCollectionView.pageIndex + 1;
                updateStockOutDetailReportNavigationButtons();
            });

            btnStockOutDetailReportMoveToPreviousPageGrid.addEventListener('click', function () {
                stockOutDetailReportCollectionView.moveToPreviousPage();
                currentPageIndex = stockOutDetailReportCollectionView.pageIndex + 1;
                updateStockOutDetailReportNavigationButtons();
            });

            btnStockOutDetailReportMoveToNextPageGrid.addEventListener('click', function () {
                stockOutDetailReportCollectionView.moveToNextPage();
                currentPageIndex = stockOutDetailReportCollectionView.pageIndex + 1;
                updateStockOutDetailReportNavigationButtons();
            });

            btnStockOutDetailReportMoveToLastPageGrid.addEventListener('click', function () {
                stockOutDetailReportCollectionView.moveToLastPage();
                currentPageIndex = stockOutDetailReportCollectionView.pageIndex + 1;
                updateStockOutDetailReportNavigationButtons();
            });
        }

        // =================================================
        // Update Stock Out Detail Report Navigation Buttons
        // =================================================
        function updateStockOutDetailReportNavigationButtons() {
            if (stockOutDetailReportCollectionView.pageSize <= 0) {
                document.getElementById('stockOutDetailReportNavigationPageGrid').style.display = 'none';
                return;
            }
            document.getElementById('stockOutDetailReportNavigationPageGrid').style.display = 'block';
            if (stockOutDetailReportCollectionView.pageIndex === 0) {
                btnStockOutDetailReportMoveToFirstPageGrid.setAttribute('disabled', 'disabled');
                btnStockOutDetailReportMoveToPreviousPageGrid.setAttribute('disabled', 'disabled');
                btnStockOutDetailReportMoveToNextPageGrid.removeAttribute('disabled');
                btnStockOutDetailReportMoveToLastPageGrid.removeAttribute('disabled');
            } else if (stockOutDetailReportCollectionView.pageIndex === (stockOutDetailReportCollectionView.pageCount - 1)) {
                btnStockOutDetailReportMoveToFirstPageGrid.removeAttribute('disabled');
                btnStockOutDetailReportMoveToPreviousPageGrid.removeAttribute('disabled');
                btnStockOutDetailReportMoveToLastPageGrid.setAttribute('disabled', 'disabled');
                btnStockOutDetailReportMoveToNextPageGrid.setAttribute('disabled', 'disabled');
            } else {
                btnStockOutDetailReportMoveToFirstPageGrid.removeAttribute('disabled');
                btnStockOutDetailReportMoveToPreviousPageGrid.removeAttribute('disabled');
                btnStockOutDetailReportMoveToNextPageGrid.removeAttribute('disabled');
                btnStockOutDetailReportMoveToLastPageGrid.removeAttribute('disabled');
            }
            btnStockOutDetailReportCurrentPageGrid.innerHTML = (stockOutDetailReportCollectionView.pageIndex + 1) + ' / ' + stockOutDetailReportCollectionView.pageCount;
        }

        // =================
        // Refresh Flex Grid
        // =================
        function refreshStockOutDetailReportFlexGrid() {
            stockOutDetailReportCollectionView = new wijmo.collections.CollectionView(getStockOutDetailReportData());
            stockOutDetailReportCollectionView.canFilter = true;
            stockOutDetailReportCollectionView.pageSize = cboStockOutDetailReportShowNumberOfRows.selectedValue;

            var filterText = '';
            $('#inpStockOutDetailReportFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                stockOutDetailReportCollectionView.refresh();
            });

            stockOutDetailReportCollectionView.filter = function (item) {
                return !filterText || (item.OT.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.ItemManualArticleOldCode.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Item.toLowerCase().indexOf(filterText) > -1)
                    || !filterText || (item.Unit.toLowerCase().indexOf(filterText) > -1);
            }

            stockOutDetailReportCollectionView.collectionChanged.addHandler(function (sender, args) {
                updateStockOutDetailReportNavigationButtons();
            });

            stockOutDetailReportFlexGrid.itemsSource = stockOutDetailReportCollectionView;
            stockOutDetailReportFlexGrid.trackChanges = true;
        }

        // ============
        // Download CSV
        // ============
        function btnStockOutDetailReportDownloadCSVOnclick() {
            var CSV = '';
            var stockOutDetailReport = [];
            var fileName = 'StockOutDetailReport.CSV';

            stockOutDetailReportCollectionView.moveToFirstPage();

            for (var p = 1; p <= stockOutDetailReportCollectionView.pageCount; p++) {
                for (i = 0; i < stockOutDetailReportCollectionView.items.length; i++) {
                    var IsClose = stockOutDetailReportCollectionView.items[i].IsClose;
                    var isPOClose = "Not Closed";
                    if (IsClose) {
                        isPOClose = "Closed"
                    }

                    stockOutDetailReport.push({
                        OTNumber: stockOutDetailReportCollectionView.items[i].OTForCSV,
                        OTDate: stockOutDetailReportCollectionView.items[i].OTDate,
                        Item: stockOutDetailReportCollectionView.items[i].Item,
                        Quantity: stockOutDetailReportCollectionView.items[i].Quantity,
                        Barcode: stockOutDetailReportCollectionView.items[i].ItemCode,
                        SKUCode: stockOutDetailReportCollectionView.items[i].ItemManualArticleOldCode,
                        Unit: stockOutDetailReportCollectionView.items[i].Unit,
                        Cost: stockOutDetailReportCollectionView.items[i].Cost,
                        Amount: stockOutDetailReportCollectionView.items[i].Amount,
                    });
                }

                stockOutDetailReportCollectionView.moveToNextPage();
                if (p == stockOutDetailReportCollectionView.pageCount) {
                    stockOutDetailReportCollectionView.moveToPage(currentPageIndex - 1);
                }
            }

            var Headers = [];
            Headers.push({
                OTNumber: "OT Number",
                OTDate: "OT Date",
                Item: "Item",
                Quantity: "Quantity",
                Barcode: "Barcode",
                SKUCode: "SKU Code",
                Unit: "Unit",
                Cost: "Cost",
                Amount: "Amount"
            });

            var header = '';
            for (var b in Headers[0]) {
                header += '"' + Headers[0][b] + '",';
            }

            CSV = header + '\r\n';
            for (var i = 0; i < stockOutDetailReport.length; i++) {
                var row = '';
                for (var a in stockOutDetailReport[i]) {
                    row += '"' + stockOutDetailReport[i][a] + '",';
                }

                row.slice(0, row.length - 1);
                CSV += row + '\r\n';
            }

            if (CSV == '') {
                toastr.error("No data.");
            } else {
                var link = document.createElement("a");
                if (link.download !== undefined) {
                    var blob = new Blob([CSV], { type: 'text/csv;charset=utf-8;' });
                    var url = URL.createObjectURL(blob);
                    link.setAttribute("href", url);
                    link.setAttribute("download", fileName);
                    link.style = "visibility:hidden";
                }

                if (navigator.msSaveBlob) {
                    link.addEventListener("click", function (event) {
                        var blob = new Blob([CSV], {
                            "type": "text/csv;charset=utf-8;"
                        });
                        navigator.msSaveBlob(blob, fileName);
                    }, false);
                }

                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);

                toastr.success("Dowload successful.");
            }
        }

        // =========
        // Print PDF
        // =========
        function btnStockOutDetailReportPDFOnclick() {
            if (cboStockOutDetailReportCompanyFilter.selectedValue != null) {
                if (cboStockOutDetailReportBranchFilter.selectedValue != null) {
                    var startDate = cboStockOutDetailReportStartDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var endDate = cboStockOutDetailReportEndDateFilter.value.toLocaleDateString().replace(/\//g, '-');
                    var companyId = cboStockOutDetailReportCompanyFilter.selectedValue;
                    var branchId = cboStockOutDetailReportBranchFilter.selectedValue;

                    window.location.href = '/RepStockOutDetailReport/StockOutDetailReport?StartDate=' + startDate + '&EndDate=' + endDate + '&CompanyId=' + companyId + '&BranchId=' + branchId;
                } else {
                    toastr.error("Lacking Information");
                }
            } else {
                toastr.error("Lacking Information");
            }
        }

        // ============
        // On Load Page
        // ============
        $(document).ready(function () {
            createCboDateRangeFilters();
        });
    </script>
    <script type="text/javascript">
        // =================
        // User Branch Combo
        // =================
        var cboUserBranches = new wijmo.input.ComboBox('#cboUserBranches');

        // =========================
        // Get Current User Branches
        // =========================
        function getUserBranches() {
            $("#loadingUserBranches").show();
            document.getElementById("loadingUserBranches").innerHTML = '<i class="fa fa-spinner fa-spin"></i>';
            $("#userBranchContent").hide();

            $('#userBranchesModal').modal({ show: true, backdrop: 'static' });

            document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
            $("#updateUserBranch").prop("disabled", false);
            $("#closeUserBranch").prop("disabled", false);

            var userBranchesObservableArray = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/userBranch/list',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (userBranchesResults) {
                    if (userBranchesResults.length > 0) {
                        for (i = 0; i < userBranchesResults.length; i++) {
                            userBranchesObservableArray.push({
                                BranchId: userBranchesResults[i]["BranchId"],
                                Branch: userBranchesResults[i]["Branch"]
                            });
                        }
                    }

                    var currentBranch = "@ViewData["BranchId"]";
                    cboUserBranches.dispose();
                    cboUserBranches = new wijmo.input.ComboBox('#cboUserBranches', {
                        isEditable: false,
                        placeholder: "Select Branches",
                        itemsSource: userBranchesObservableArray,
                        displayMemberPath: "Branch",
                        selectedValuePath: "BranchId",
                        selectedValue: currentBranch
                    });

                    setTimeout(function () {
                        $("#loadingUserBranches").hide();
                        $("#userBranchContent").show();
                    }, 500)
                }
            });
        }

        // ==========================
        // Update Current User Branch
        // ==========================
        function updateUserBranchOnclick() {
            document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-spinner fa-spin'></i> Updating";
            $("#updateUserBranch").prop("disabled", true);
            $("#closeUserBranch").prop("disabled", true);

            var userBranchObject = new Object();
            userBranchObject.BranchId = cboUserBranches.selectedValue;
            var userBranchData = JSON.stringify(userBranchObject);

            $.ajax({
                type: "PUT",
                url: '/api/userBranch/update',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: userBranchData,
                statusCode: {
                    200: function () {
                        toastr.success("Update Successful");
                        window.setTimeout(function () {
                            location.reload()
                        }, 1000);
                    },
                    404: function () {
                        toastr.error("Not Found");
                        document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
                        $("#updateUserBranch").prop("disabled", false);
                        $("#closeUserBranch").prop("disabled", false);
                    },
                    500: function () {
                        toastr.error("Bad Request");
                        document.getElementById('updateUserBranch').innerHTML = "<i class='fa fa-save fa-fw'></i> Update";
                        $("#updateUserBranch").prop("disabled", false);
                        $("#closeUserBranch").prop("disabled", false);
                    }
                }
            });
        }
    </script>
