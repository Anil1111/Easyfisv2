    <script type="text/javascript">
        // ================
        // Global Variables
        // ================
        var accountsReceivableDateAsOf;
        var accountsReceivableSummaryDateAsOf;
        var StatementOfTheAccountDateAsOf;
        var companies;
        var accountReceivableCboCompany;
        var accountReceivableSummaryCboCompany;
        var StatementOfTheAccountCboCompany;
        var customers;
        var cboCustomer;
        var saleSummaryReportStartDate;
        var saleSummaryReportEndDate;
        var saleDetailReportStartDate;
        var saleDetailReportEndDate;
        var collectionSummaryReportStartDate;
        var collectionSummaryReportEndDate;
        var collectionDetailReportStartDate;
        var collectionDetailReportEndDate;
        var salesDetailReportVatSalesStartDate;
        var salesDetailReportVatSalesEndDate;
        var cancelledSalesSummaryReportStartDate;
        var cancelledSalesSummaryReportEndDate;
        var seniorCitizenSalesSummaryReportStartDate;
        var seniorCitizenSalesSummaryReportEndDate;
        var topSellingItemsReportStartDate;
        var topSellingItemsReportEndDate;
        var hourlyTopSellingItemsReportStartDate;
        var hourlyTopSellingItemsReportEndDate;
        var chartMonthlySalesTrendStartDate;
        var chartMonthlySalesTrendEndDate;
        var chartMonthlySalesComparisonStartDate;
        var chartMonthlySalesComparisonEndDate;
        var chartYearlySalesTrendComparisonStartDate;
        var chartYearlySalesTrendComparisonEndDate;
        var chartHourlySalesComparisonStartDate;
        var chartHourlySalesComparisonEndDate;
        var chartHourlySalesComparisonAmountStartDate;
        var chartHourlySalesComparisonAmountEndDate;
        var chartCategorySoldComparisonStartDate;
        var chartCategorySoldComparisonEndDate;
        var salesRewardSummaryReportStartDate;
        var salesRewardSummaryReportEndDate;
