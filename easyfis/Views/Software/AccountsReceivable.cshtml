        function getBranch() {
            branch = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/branch/list',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            branch.push({
                                Id: results[i]["Id"],
                                Branch: results[i]["Branch"],
                                Address: results[i]["Address"],
                                ContactNumber: results[i]["ContactNumber"],
                                TaxNumber: results[i]["TaxNumber"],
                                CompanyId: results[i]["CompanyId"]
                            });
                        }
                        createBranch(branch)
                    }
                }
            });
        }

        function salesSummaryReportBranchListFunction() {
            branch = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/branch/list',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            branch.push({
                                Id: results[i]["Id"],
                                Branch: results[i]["Branch"],
                                Address: results[i]["Address"],
                                ContactNumber: results[i]["ContactNumber"],
                                TaxNumber: results[i]["TaxNumber"],
                                CompanyId: results[i]["CompanyId"]
                            });
                        }

                        var branchCollection = new wijmo.collections.CollectionView(branch);
                        var branchList = new Array();

                        if (salesSummaryReportCboCompany.selectedValue == null) {
                            for (var i = 0; i < branchCollection.items.length; i++) {
                                branchList.push({
                                    Id: branchCollection.items[i].Id,
                                    Branch: branchCollection.items[i].Branch,
                                    Company: branchCollection.items[i].Company
                                });
                            }
                        } else {
                            for (var i = 0; i < branchCollection.items.length; i++) {
                                if (branchCollection.items[i].CompanyId == salesSummaryReportCboCompany.selectedItem["Id"]) {
                                    branchList.push({
                                        Id: branchCollection.items[i].Id,
                                        Branch: branchCollection.items[i].Branch,
                                        Company: branchCollection.items[i].Company
                                    });
                                }
                            }
                        }

                        salesSummaryReportCboBranch.dispose();
                        salesSummaryReportCboBranch = new wijmo.input.ComboBox('#salesSummaryReport_Branch', {
                            placeholder: "Select Branch",
                            itemsSource: branchList,
                            isEditable: false,
                            required: false,
                            displayMemberPath: "Branch",
                            selectedValue: document.getElementById('salesSummaryReport_Branch_selectedValue').value.toString(),
                            onSelectedIndexChanged: function () {
                                if (branchCollection.items[this.selectedIndex] != null) {
                                    $("#salesSummaryReport_Branch_data").val(branchCollection.items[this.selectedIndex].Id);
                                } else {
                                    $("#salesSummaryReport_Branch_data").val("");
                                }
                            }
                        });

                    }
                }
            });
        }

