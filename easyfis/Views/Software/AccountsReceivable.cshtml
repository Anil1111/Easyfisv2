        // get Sale Detail Report End Date
        function getSaleDetailReportEndDate() {
            saleDetailReportEndDate.dispose();
            saleDetailReportEndDate = new wijmo.input.InputDate('#SaleDetailReport_EndDate', {
                format: 'MM-dd-yyyy',
                value: new Date(document.getElementById('SaleDetailReport_EndDate_data').value),
                mask: '99-99-9999',
