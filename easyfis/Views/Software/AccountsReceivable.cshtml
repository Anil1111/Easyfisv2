    <script type="text/javascript">
        // ================
        // Global Variables
        // ================
        var accountsReceivableDateAsOf;
        var accountsReceivableSummaryDateAsOf;
        var StatementOfTheAccountDateAsOf;
        var companies;
        var accountReceivableCboCompany;
        var accountReceivableSummaryCboCompany;
        var StatementOfTheAccountCboCompany;
        var customers;
        var cboCustomer;
        var saleSummaryReportStartDate;
        var saleSummaryReportEndDate;
        var saleDetailReportStartDate;
        var saleDetailReportEndDate;
        var collectionSummaryReportStartDate;
        var collectionSummaryReportEndDate;
        var collectionDetailReportStartDate;
        var collectionDetailReportEndDate;

        // ===========
        // Get company
        // ===========
        function getCompanies() {
            companies = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/listCompany',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            companies.push({
                                Id: results[i]["Id"],
                                Company: results[i]["Company"],
                                Address: results[i]["Address"],
                                ContactNumber: results[i]["ContactNumber"],
                                TaxNumber: results[i]["TaxNumber"],
                            });
                        }

                        createCboComapny(companies);
                    }
                }
            });
            return companies;
        }

        // ===================
        // Company - Combo Box
        // ===================
        function createCboComapny(companies) {
            var companyCollection = new wijmo.collections.CollectionView(companies);
            var companyList = new Array();
            for (var i = 0; i < companyCollection.items.length; i++) {
                companyList.push(companyCollection.items[i].Company);
            }

            accountReceivableCboCompany.dispose();
            accountReceivableCboCompany = new wijmo.input.ComboBox('#accountReceivableReport_Company', {
                placeholder: "Select Company",
                itemsSource: companyList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('accountReceivableReport_Company_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#accountReceivableReport_Company_data").val(companyCollection.items[this.selectedIndex].Id);
                }
            });

            accountReceivableSummaryCboCompany.dispose();
            accountReceivableSummaryCboCompany = new wijmo.input.ComboBox('#accountReceivableSummaryReport_Company', {
                placeholder: "Select Company",
                itemsSource: companyList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('accountReceivableSummaryReport_Company_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#accountReceivableSummaryReport_Company_data").val(companyCollection.items[this.selectedIndex].Id);
                }
            });

            StatementOfTheAccountCboCompany.dispose();
            StatementOfTheAccountCboCompany = new wijmo.input.ComboBox('#StatementOfTheAccount_Company', {
                placeholder: "Select Company",
                itemsSource: companyList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('StatementOfTheAccount_Company_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#StatementOfTheAccount_Company_data").val(companyCollection.items[this.selectedIndex].Id);
                }
            });
        }

        // ============
        // GET Customer
        // ============
        function getCustomers() {
            customers = new wijmo.collections.ObservableArray();
            $.ajax({
                url: '/api/listArticleByArticleTypeId/2',
                cache: false,
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                data: {},
                success: function (results) {
                    if (results.length > 0) {
                        for (i = 0; i < results.length; i++) {
                            customers.push({
                                CustomerId: results[i]["Id"],
                                Customer: results[i]["Article"]
                            });
                        }
                        createCboCustomer(customers);
                    }
                }
            });
            return customers;
        }

        // ======================
        // Combo Box for Customer
        // ======================
        function createCboCustomer(customers) {
            var customerCollection = new wijmo.collections.CollectionView(customers);
            var customerList = new Array();
            for (var i = 0; i < customerCollection.items.length; i++) {
                customerList.push(customerCollection.items[i].Customer);
            }

            cboCustomer.dispose();
            cboCustomer = new wijmo.input.ComboBox('#StatementOfTheAccount_Customer', {
                placeholder: "Select Customer",
                itemsSource: customerList,
                isEditable: false,
                required: false,
                selectedValue: document.getElementById('StatementOfTheAccount_Customer_selectedValue').value.toString(),
                onSelectedIndexChanged: function () {
                    $("#StatementOfTheAccount_Customer_data").val(customerCollection.items[this.selectedIndex].CustomerId);
                }
            });
        }

        function AccountsReceivablePDF_OnClick() {
            var accountReceivableReportDateAsOf = document.getElementById('accountReceivableReport_DateAsOf_data').value;

            var companyId;
            if (document.getElementById('accountReceivableReport_Company_data').value == "") {
                companyId = 0;
                document.getElementById('accountReceivableReport_Company').style.border = "1px solid red";
                toastr.error("Lacking Information");
            } else {
                companyId = document.getElementById('accountReceivableReport_Company_data').value;
                document.getElementById('accountReceivableReport_Company').style.border = " ";
                window.location.href = '/RepAccountsReceivable/AccountsReceivable?DateAsOf=' + accountReceivableReportDateAsOf + '&CompanyId=' + companyId;
            }
        }

        function AccountsReceivableView_OnClick() {
            var accountReceivableReportDateAsOf = document.getElementById('accountReceivableReport_DateAsOf_data').value;

            var companyId;
            if (document.getElementById('accountReceivableReport_Company_data').value == "") {
                companyId = 0;
                document.getElementById('accountReceivableReport_Company').style.border = "1px solid red";
                toastr.error("Lacking Information");
            } else {
                companyId = document.getElementById('accountReceivableReport_Company_data').value;
                document.getElementById('accountReceivableReport_Company').style.border = " ";
                window.location.href = '/Software/AccountsReceivableReport?DateAsOf=' + accountReceivableReportDateAsOf + '&CompanyId=' + companyId;
            }
        }

        function AccountsReceivableSummaryPDF_OnClick() {
            var accountReceivableSummaryReportDateAsOf = document.getElementById('accountReceivableSummaryReport_DateAsOf_data').value;

            var companyId;
            if (document.getElementById('accountReceivableSummaryReport_Company_data').value == "") {
                companyId = 0;
                document.getElementById('accountReceivableSummaryReport_Company').style.border = "1px solid red";
                toastr.error("Lacking Information");
            } else {
                companyId = document.getElementById('accountReceivableSummaryReport_Company_data').value;
                document.getElementById('accountReceivableSummaryReport_Company').style.border = " ";
                window.location.href = '/RepAccountsReceivableSummary/AccountsReceivableSummary?DateAsOf=' + accountReceivableSummaryReportDateAsOf + '&CompanyId=' + companyId;
            }
        }


        function AccountsReceivableStatementOfTheAccountPDF_OnClick() {
            var accountReceivableStatementOfTheAccountReportDateAsOf = document.getElementById('StatementOfTheAccount_DateAsOf_data').value;
            var accountReceivableStatementOfTheAccountReportCustomerId;

            var companyId;
            if (document.getElementById('StatementOfTheAccount_Company_data').value == "") {
                companyId = 0;
                document.getElementById('StatementOfTheAccount_Company').style.border = "1px solid red";
                toastr.error("Lacking Information");
            } else if (document.getElementById('StatementOfTheAccount_Customer_data').value == "") {
                accountReceivableStatementOfTheAccountReportCustomerId = 0;
                document.getElementById('StatementOfTheAccount_Customer').style.border = "1px solid red";
                toastr.error("Lacking Information");
            } else {
                companyId = document.getElementById('StatementOfTheAccount_Company_data').value;
                accountReceivableStatementOfTheAccountReportCustomerId = document.getElementById('StatementOfTheAccount_Customer_data').value;
                document.getElementById('StatementOfTheAccount_Company').style.border = " ";
                document.getElementById('StatementOfTheAccount_Customer').style.border = " ";
                window.location.href = '/RepStatementOfAccount/StatementOfAccount?DateAsOf=' + accountReceivableStatementOfTheAccountReportDateAsOf + '&CompanyId=' + companyId + '&CustomerId=' + accountReceivableStatementOfTheAccountReportCustomerId;
            }
        }


        // get Sale Summary Report Start Date
        function getSaleSummaryReportStartDate() {
            saleSummaryReportStartDate.dispose();
            saleSummaryReportStartDate = new wijmo.input.InputDate('#SaleSummaryReport_StartDate', {
                format: 'MM-dd-yyyy',
                value: new Date(document.getElementById('SaleSummaryReport_StartDate_data').value),
                mask: '99-99-9999',
                max: new Date(document.getElementById('SaleSummaryReport_EndDate_data').value),
                onValueChanged: function () {
                    var thisDateValue = [this.value.getMonth() + 1, this.value.getDate(), this.value.getFullYear()].join('-');
                    document.getElementById('SaleSummaryReport_StartDate_data').value = thisDateValue;
                    setTimeout(function () {
                        getSaleSummaryReportEndDate();
                    }, 50);
                }
            });
        }

        // get Sale Summary Report End Date
        function getSaleSummaryReportEndDate() {
            saleSummaryReportEndDate.dispose();
            saleSummaryReportEndDate = new wijmo.input.InputDate('#SaleSummaryReport_EndDate', {
                format: 'MM-dd-yyyy',
                value: new Date(document.getElementById('SaleSummaryReport_EndDate_data').value),
                mask: '99-99-9999',
                min: new Date(document.getElementById('SaleSummaryReport_StartDate_data').value),
                onValueChanged: function () {
                    var thisDateValue = [this.value.getMonth() + 1, this.value.getDate(), this.value.getFullYear()].join('-');
                    document.getElementById('SaleSummaryReport_EndDate_data').value = thisDateValue;
                    setTimeout(function () {
                        getSaleSummaryReportStartDate();
                    }, 50);
                }
            });
        }

