                            }
                        }

                        document.getElementById('inpTotalStockOutAmount').value = parseFloat(totalAmount).toFixed(2).toLocaleString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                        NProgress.done();
                    }
                });
            }

            return itemsObservableArray;
        }

