        // =========================================
        // Update Stock Out Items Navigation Buttons
        // =========================================
        function updateStockOutItemsNavigationButtons() {
            if (stockOutItemsCollectionView.pageSize <= 0) {
                document.getElementById('stockOutItemsNavigationPageGridItem').style.display = 'none';
                return;
            }
            document.getElementById('stockOutItemsNavigationPageGridItem').style.display = 'block';
            if (stockOutItemsCollectionView.pageIndex === 0) {
                btnStockOutItemsMoveToFirstPageGrid.setAttribute('disabled', 'disabled');
                btnStockOutItemsMoveToPreviousPageGrid.setAttribute('disabled', 'disabled');
                btnStockOutItemsMoveToNextPageGrid.removeAttribute('disabled');
                btnStockOutItemsMoveToLastPageGrid.removeAttribute('disabled');
            } else if (stockOutItemsCollectionView.pageIndex === (stockOutItemsCollectionView.pageCount - 1)) {
                btnStockOutItemsMoveToFirstPageGrid.removeAttribute('disabled');
                btnStockOutItemsMoveToPreviousPageGrid.removeAttribute('disabled');
                btnStockOutItemsMoveToNextPageGrid.setAttribute('disabled', 'disabled');
                btnStockOutItemsMoveToLastPageGrid.setAttribute('disabled', 'disabled');
            } else {
                btnStockOutItemsMoveToFirstPageGrid.removeAttribute('disabled');
                btnStockOutItemsMoveToPreviousPageGrid.removeAttribute('disabled');
                btnStockOutItemsMoveToNextPageGrid.removeAttribute('disabled');
                btnStockOutItemsMoveToLastPageGrid.removeAttribute('disabled');
            }
            btnStockOutItemsCurrentPageGrid.innerHTML = (stockOutItemsCollectionView.pageIndex + 1) + ' / ' + stockOutItemsCollectionView.pageCount;
        }

