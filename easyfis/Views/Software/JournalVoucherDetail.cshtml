        // ==================================================================
        // Function with leading zeroes to display the journal voucher number
        // ==================================================================
        function zerofill(number, length) {
            var result = number.toString();
            var pad = length - result.length;
            while (pad > 0) {
                result = '0' + result;
                pad--;
            }
            return result;
        }

