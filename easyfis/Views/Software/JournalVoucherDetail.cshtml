                                            <table>
                                                <tr>
                                                    <td><b>Total Debit: </b></td>
                                                    <td style="padding-left: 15px;"><span id="totalDebit">0</span></td>
                                                </tr>
                                                <tr>
                                                    <td><b>Total Credit: </b></td>
                                                    <td style="padding-left: 15px;"><span id="totalCredit">0</span></td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>

        @Html.Partial("_SoftwareFooter")
    </div>


