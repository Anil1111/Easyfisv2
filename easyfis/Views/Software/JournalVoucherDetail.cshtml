                                            <div class="col-sm-8">
                                                <div id="preparedByUserId" class="comboBox-wide"></div>
                                                <input type="hidden" id="preparedByUserId_Data" class="requiredJournalVoucher" required />
                                                <input type="hidden" id="preparedByUserId_selectedValue" />
                                                <input type="hidden" id="userId_data" value="@userId" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-sm-4">Checked by</label>
                                            <div class="col-sm-8">
                                                <div id="checkedByUserId" class="comboBox-wide"></div>
                                                <input type="hidden" id="checkedByUserId_Data" class="requiredJournalVoucher" required />
                                                <input type="hidden" id="checkedByUserId_selectedValue" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-sm-4">Approved by</label>
                                            <div class="col-sm-8">
                                                <div id="approvedByUserId" class="comboBox-wide"></div>
                                                <input type="hidden" id="approvedByUserId_Data" class="requiredJournalVoucher" required />
                                                <input type="hidden" id="approvedByUserId_selectedValue" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-sm-4">Lock</label>
                                            <div class="col-sm-8">
                                                <input type="checkbox" class="checkbox_custom" id="isLocked_Status" />
                                            </div>
                                        </div>
                                        <br />
                                        @*<div class="form-group">
                                                <div class="col-sm-4"></div>
                                                <div class="col-sm-8">
                                                    <button type="button" class="btn btn-primary btn-sm">Depreciation</button>
                                                </div>
                                            </div>*@
                                        <div class="form-group">
                                            <div class="col-sm-8">
                                                <input type="hidden" id="JV_IsLocked">
                                                <input type="hidden" id="JV_IsLocked_defaultTrue">
                                                <input type="hidden" id="JV_IsLocked_defaultFalse">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <br />
                        <ul class="nav nav-tabs" role="tablist" id="myTab">
                            <li role="presentation" class="active"><a data-toggle="tab" href="#JournalVoucherLineList" id="branchList">Journal Voucher Line</a></li>
                        </ul>
                        <br />
                        <div class="tab-content">
                            <div id="JournalVoucherLineList" class="tab-pane fade in active">
                                <div class="row">
                                    <div class="col-lg-12" align="right">
                                        <button class="btn btn-primary btn-sm pull-right" id="cmdJournalVoucherLineAddJournalVoucherLineButton" onclick="cmdJournalVoucherLineAddJournalVoucherLineButton_OnClick()"><i class="fa fa-plus"></i> Add</button>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div id="JournalVoucherLineGrid" class="grid" align="left"></div>
                                        <br />
                                        <br />
