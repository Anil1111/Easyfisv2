                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="control-label col-sm-4">Branch</label>
                                            <div class="col-sm-8">
                                                <div id="branch_Id" class="comboBox-wide"></div>
                                                <input type="hidden" id="branch_Id_data" class="requiredJournalVoucher" />
                                                <input type="hidden" id="branch_selectedValue" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-sm-4">JV Number</label>
