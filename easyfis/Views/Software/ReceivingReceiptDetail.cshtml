                                    EditId: btnEditId,
                                    DeleteId: btnDeleteId,
                                    Id: Results[i]["Id"],
                                    RRId: Results[i]["RRId"],
                                    RR: Results[i]["RR"],
                                    POId: Results[i]["POId"],
                                    PO: Results[i]["PO"],
                                    ItemId: Results[i]["ItemId"],
                                    Item: Results[i]["Item"],
