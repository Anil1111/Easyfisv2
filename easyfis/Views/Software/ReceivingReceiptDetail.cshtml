                            disabled: disabled,
                            isEditable: false,
                            itemsSource: purchaseOrderItemsObservableArray,
                            displayMemberPath: "ItemCode",
                            selectedValuePath: "ItemId",
                            selectedValue: ItemIdSelectedValue,
