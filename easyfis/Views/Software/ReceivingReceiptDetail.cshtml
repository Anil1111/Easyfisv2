        function getPurchaseOrderItemForPOSTatus() {
            var purchaseOrderStatusObservableArray = new wijmo.collections.ObservableArray;
            if (cboPONumberPOStatus.selectedValue != null) {
                var POId = cboPONumberPOStatus.selectedValue;
                $.ajax({
