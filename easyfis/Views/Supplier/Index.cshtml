        // ===========================
        // Supplier Delete Lock Button
        // ===========================
        function cmdSupplierDeleteLockButton_OnClick() {
            toastr.error("Record Locked")
        }

        // =========================
        // Refresh Supplier Flexgrid
        // =========================
        function refreshSupplierFlexgrid() {
            suppliers = new wijmo.collections.CollectionView(getArtcileForSuppliers());
            suppliers.canFilter = true;
            suppliers.pageSize = 15;

            var filterText = '';
            $('#InputFilter').keyup(function () {
                filterText = this.value.toLowerCase();
                suppliers.refresh();
            });
            suppliers.filter = function (item) {
                return !filterText || (item.Article.toLowerCase().indexOf(filterText) > -1)
                                   || (item.ContactNumber.toLowerCase().indexOf(filterText) > -1)
                                   || (item.ArticleCode.toLowerCase().indexOf(filterText) > -1)
                                   || (item.ArticleGroup.toLowerCase().indexOf(filterText) > -1);
            }
            suppliers.collectionChanged.addHandler(function (sender, args) {
                updateNavigateButtonsSupplier();
            });
            suppliers.currentChanged.addHandler(function (sender, args) {
                updateDetails();
            });

            // Flex Grid
            supplierGrid.itemsSource = suppliers;
            supplierGrid.trackChanges = true;
        }

        // ===================
        // Supplier Add Button
        // ===================
        function btnAddSupplier_OnClick() {
            //window.location = '/Software/SupplierDetail';
            document.getElementById('btnAddSupplier').innerHTML = "<i class='fa fa-spinner fa-spin'></i> Adding";
            $('#btnAddSupplier').prop('disabled', true);
            $('#btnCloseSupplier').prop('disabled', true);

            var canAccessToDetailPage = "@ViewData["CanAccessToDetailPage"]";
            if (canAccessToDetailPage == "True") {
                $.ajax({
                    type: "POST",
                    url: '/api/addArticle/3',
                    contentType: "application/json; charset=utf-8",
                    success: function (id) {
                        if (id > 0) {
                            window.location = '/Software/SupplierDetail?SupplierId=' + id;
                        } else {
                            toastr.error("Internal Server Error");
                        }
                    }
                });
            } else if (canAccessToDetailPage == "False") {
                window.location = '/Software/Forbidden';
            } else {
                document.getElementById('btnAddSupplier').innerHTML = "<i class='fa fa-plus'></i> Add";
                $('#btnAddSupplier').prop('disabled', false);
                $('#btnCloseSupplier').prop('disabled', false);
            }
        }

        // =====================
        // Supplier Close Button
        // =====================
        function btnCloseSupplier_OnClick() {
            window.location = '/Software';
        }

        // ====================
        // Supplier Edit Button
        // ====================
        function cmdSupplierEditButton_OnClick() {
            suppliers.editItem(suppliers.currentItem);
            var supplier = suppliers.currentEditItem;
            var supplierId = supplier.Id;
            window.location = '/Software/SupplierDetail?SupplierId=' + supplierId;
        }

        // =====================
        // Supplier delete Button
        // =====================
        function cmdSupplierDeleteButton_OnClick() {
            $('#deleteSupplierConfirm').modal({
                show: true,
                backdrop: 'static'
            });

            document.getElementById('cmdConfirmDeleteSupplier').innerHTML = "<i class='fa fa-trash'></i> Delete";
            $('#cmdConfirmDeleteSupplier').prop('disabled', false);
            $('#cmdConfirmDeleteCloseButton').prop('disabled', false);
        }
        // Supplier Confirm Delete Button
        function cmdSupplierConfirmDeleteButton_OnClick() {
            document.getElementById('cmdConfirmDeleteSupplier').innerHTML = "<i class='fa fa-spinner fa-spin'></i> Deleting";
            $('#cmdConfirmDeleteSupplier').prop('disabled', true);
            $('#cmdConfirmDeleteCloseButton').prop('disabled', true);

            suppliers.editItem(suppliers.currentItem);
            var supplierId = suppliers.currentEditItem.Id;
            $.ajax({
                url: '/api/deleteArticle/' + supplierId,
                type: "DELETE",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                statusCode: {
                    200: function () {
                        document.getElementById('cmdConfirmDeleteSupplier').innerHTML = "<i class='fa fa-spinner fa-spin'></i> Deleting";
                        toastr.success("Delete Successful");
                        $('#deleteSupplierConfirm').modal('hide');
                        refreshSupplierFlexgrid();
                    },
                    404: function () {
                        toastr.error("Not Found");
                        $('#deleteSupplierConfirm').modal('hide');
                    },
                    400: function () {
                        toastr.error("Bad Request");
                        $('#deleteSupplierConfirm').modal('hide');
                    }
                }
            });
        }

        // ===================
        // FlexGrid Selection
        // ===================
        function updateDetails() {
            var supplier = suppliers.currentItem;
            document.getElementById('createdBy').innerHTML = supplier.CreatedBy;
            document.getElementById('createdDate').innerHTML = supplier.CreatedDateTime;
            document.getElementById('updatedBy').innerHTML = supplier.UpdatedBy;
            document.getElementById('updatedDate').innerHTML = supplier.UpdatedDateTime;
        }

        // ===============================
        // Navigation Buttons for Supplier
        // ===============================
        function updateNavigateButtonsSupplier() {
            if (suppliers.pageSize <= 0) {
                document.getElementById('naviagtionPageGrid').style.display = 'none';
                return;
            }
            document.getElementById('naviagtionPageGrid').style.display = 'block';
            if (suppliers.pageIndex === 0) {
                btnFirstPageGrid.setAttribute('disabled', 'disabled');
                btnPreviousPageGrid.setAttribute('disabled', 'disabled');
                btnNextPageGrid.removeAttribute('disabled');
                btnLastPageGrid.removeAttribute('disabled');
            } else if (suppliers.pageIndex === (suppliers.pageCount - 1)) {
                btnFirstPageGrid.removeAttribute('disabled');
                btnPreviousPageGrid.removeAttribute('disabled');
                btnLastPageGrid.setAttribute('disabled', 'disabled');
                btnNextPageGrid.setAttribute('disabled', 'disabled');
            } else {
                btnFirstPageGrid.removeAttribute('disabled');
                btnPreviousPageGrid.removeAttribute('disabled');
                btnNextPageGrid.removeAttribute('disabled');
                btnLastPageGrid.removeAttribute('disabled');
            }
            btnCurrentPageGrid.innerHTML = (suppliers.pageIndex + 1) + ' / ' + suppliers.pageCount;
        }

        function refreshGrids() {
            setTimeout(function () {
                suppliers.refresh();
            }, 500)
        }

