@using Microsoft.AspNet.Identity
@using Microsoft.AspNet.Identity.EntityFramework

@if (Request.IsAuthenticated)
{
    <!-- Section -->
    <section class="secStyle">
        <br /><br /><br />
        <div class="container">
            <div class="box box-default">
                <div class="row">
                    <div class="col-md-3">
                        <ul class="nav nav-pills nav-stacked pill-custom" role="tablist" id="myTab">
                            <li class="active"><a data-toggle="pill" href="#UI"><i class="fa fa-user"></i> User Information</a></li>
                            <li class="manageDivider"></li>
                            <li><a data-toggle="pill" href="#ST"><i class="fa fa-gears"></i> Settings</a></li>
                            <li><a data-toggle="pill" href="#UT"><i class="fa fa-wrench"></i> Utilities</a></li>
                        </ul>
                        <br />
                        <button class="btn btn-success btn-block btn-xl launchSoftware" id="LaunchSoftware" onclick="cmdLaunchSoftware_OnClick()"><i class=" fa fa-rocket"></i> Launch Software</button>
                        <br /><br />
                    </div>
                    <div class="col-md-9">
                        <div class="tab-content">
                            <div id="UI" class="tab-pane fade in active">
                                <div class="well">
                                    <h3><b>@ViewData["FullName"]</b></h3>
                                    <br />
                                    <button class="btn btn-primary btn-sm" onclick="cmdEditProfileDetails_OnClickButton()"> Edit</button>
                                </div>
                            </div>
